  <rdf:Description rdf:about="http://dati.gov.it/onto/covapit#hasContactPoint">
    <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
    <rdfs:comment xml:lang="en">This property links the organization with one contact point. The contact point may consist of different elements (one or more email addresses, one or more social netwrk accounts one or more telephone numbers, ecc.),</rdfs:comment>
    <rdfs:domain rdf:resource="http://dati.gov.it/onto/covapit#Organization"/>
