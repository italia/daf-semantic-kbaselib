  <rdf:Description rdf:about="http://dati.gov.it/onto/covapit#REANumber">
    <rdfs:label xml:lang="en">REA number</rdfs:label>
    <rdfs:comment xml:lang="en">This property represents a specific code named REA number associated with the registration of a private organization in the related Italian national registry.</rdfs:comment>
