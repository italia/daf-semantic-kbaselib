    <rdfs:subClassOf rdf:resource="http://dati.gov.it/onto/covapit#ChangeEvent"/>
    <rdfs:comment xml:lang="en">This class represents the merger event for an organization.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/covapit#resultedFrom">
    <rdfs:comment xml:lang="en">This property is the inverse of resulting organization and links an organization to the change event through which the organization has been created.</rdfs:comment>
