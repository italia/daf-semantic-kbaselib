###  http://dati.gov.it/onto/covapit#actsOnBehalfOf

:actsOnBehalfOf rdf:type owl:ObjectProperty ;

                rdfs:label "acts on behalf of"@en ,
                           "agisce per conto di"@it ;

                rdfs:comment "This property links an agent to another agent and it is used to represent  general reporting or accountability relationships"@en ;

                owl:versionInfo "stable"@en ;

                rdfs:comment "Questa proprietà lega un agent ad un altro agente ed è utilizzata per modellare relazioni generali di responsabilità e di resoconto."@it ;

                owl:versionInfo "stabile"@it ;

                rdfs:isDefinedBy <http://dati.gov.it/onto/covapit/> ;

                rdfs:domain l0:Agent ;

                rdfs:range l0:Agent .



###  http://dati.gov.it/onto/covapit#changedBy

:changedBy rdf:type owl:ObjectProperty ;

           rdfs:label "changed by"@en ,
                      "cambiata da"@it ;

           rdfs:comment "This property is the inverse of orginal organization and it is used to link the organization with the change event."@en ;

           owl:versionInfo "stable"@en ;

           rdfs:comment "Questa proprietà è l'inversa di organizzazione d'origine e collega un'organizzazione a un evento di cambiamento."@it ;

           owl:versionInfo "stabile"@it ;

           rdfs:range :ChangeEvent ;

           rdfs:domain :Organization ;

           rdfs:isDefinedBy <http://dati.gov.it/onto/covapit/> .



###  http://dati.gov.it/onto/covapit#employmentFor

:employmentFor rdf:type owl:ObjectProperty ;

               rdfs:label "membership for"@en ,
                          "appartenenza per"@it ;

               rdfs:comment "This property links the situation Membership to the organization."@en ;

               owl:versionInfo "stable"@en ;

               rdfs:comment "Questa proprietà lega la situazione appartenenza all'organizzazione."@it ;

               owl:versionInfo "stabile"@it ;

               rdfs:domain :Employment ;

               rdfs:range :Organization ;

               rdfs:isDefinedBy <http://dati.gov.it/onto/covapit/> .



###  http://dati.gov.it/onto/covapit#follows

:follows rdf:type owl:ObjectProperty ,
                  owl:TransitiveProperty ;

         rdfs:label "follows"@en ,
                    "segue"@it ;

         rdfs:comment "This property models an ordered sequence of organizations that precede and succeed each other.. In particular this property represents the organization that follows another."@en ;

         owl:versionInfo "stable"@en ;

         rdfs:comment "Questa proprietà modella una sequenza ordinata di organizzazioni that precedono o si susseguono le un con le altre.  Nello specifico questa proprietà rappresenta un'organizzazione che segue un'altra."@it ;

         owl:versionInfo "stabile"@it ;

         rdfs:range :Organization ;

         rdfs:domain :Organization ;

         rdfs:isDefinedBy <http://dati.gov.it/onto/covapit/> .



###  http://dati.gov.it/onto/covapit#hasActivityType

:hasActivityType rdf:type owl:ObjectProperty ;

                 rdfs:label "has activity type"@en ,
                            "ha tipo di attività"@it ;

                 rdfs:comment "This property links an organization with the type of function/activities it carries out."@en ;

                 owl:versionInfo "unstable"@en ;

                 rdfs:comment "Questa proprietà lega un'organizzazione con il tipo di funzioni/attività svolte."@it ;

                 owl:versionInfo "provvisoria"@it ;

                 rdfs:range :ActivityType ;

                 rdfs:domain :Organization ;

                 rdfs:isDefinedBy <http://dati.gov.it/onto/covapit/> .



###  http://dati.gov.it/onto/covapit#hasBalanceSheet

:hasBalanceSheet rdf:type owl:ObjectProperty ;

                 rdfs:label "has balance sheet"@en ,
                            "ha bilancio"@it ;

                 rdfs:comment "This property links the Organization with one or more balance sheets."@en ;

                 owl:versionInfo "stable"@en ;

                 rdfs:comment "Questa proprietà lega un'organizzazione a uno o più bilanci."@it ;

                 owl:versionInfo "stabile"@it ;

                 rdfs:range :BalanceSheet ;

                 rdfs:domain :Organization ;

                 rdfs:isDefinedBy <http://dati.gov.it/onto/covapit/> .



###  http://dati.gov.it/onto/covapit#hasCategory

:hasCategory rdf:type owl:ObjectProperty ;

             rdfs:label "has category"@en ,
                        "ha categoria"@it ;

             rdfs:comment "This property links the public organization to its category."@en ;

             owl:versionInfo "stable"@en ;

             rdfs:comment "Questa proprietà lega l'organizzazione pubblica alla propria categoria."@it ;

             owl:versionInfo "stabile"@it ;

             rdfs:domain :PublicOrganization ;

             rdfs:range :PublicOrganizationCategory ;

             rdfs:isDefinedBy <http://dati.gov.it/onto/covapit/> .



###  http://dati.gov.it/onto/covapit#hasContactPoint

:hasContactPoint rdf:type owl:ObjectProperty ;

                 rdfs:label "has contact point"@en ,
                            "ha punto di contatto"@it ;

                 rdfs:comment "This property links the organization with one contact point. The contact point may consist of different elements (one or more email addresses, one or more social netwrk accounts one or more telephone numbers, ecc.),"@en ;

                 owl:versionInfo "stable"@en ;

                 rdfs:comment "Questa proprietà lega un'organizzazione a un punto di contatto. Il punto di contatto può essere formato da diversi elementi (uno o più social network account, uno o più indirizzi email, uno o più numeri di telefono)."@it ;

                 owl:versionInfo "stabile"@it ;

                 rdfs:range :ContactPoint ;

                 rdfs:domain :Organization ;

                 rdfs:isDefinedBy <http://dati.gov.it/onto/covapit/> .



###  http://dati.gov.it/onto/covapit#hasEInvoiceService

:hasEInvoiceService rdf:type owl:ObjectProperty ;

                    rdfs:label "has eInvoice service"@en ,
                               "ha servizio di fatturazione elettronica"@it ;

                    rdfs:comment "This property links an office (or organizational unit) to a eInvoice Service. This property is materialized (it is mandatory) in the case of public organization."@en ;

                    owl:versionInfo "stable"@en ;

                    rdfs:comment "Questa proprietà lega l'ufficio o l'unità organizzativa al servizio di fatturazione elettronica. Questa proprietà è materializzata nello specifico per le organizzazioni pubbliche"@it ;

                    owl:versionInfo "stabile"@it ;

                    rdfs:domain :Office ;

                    rdfs:range :eInvoiceService ;

                    rdfs:isDefinedBy <http://dati.gov.it/onto/covapit/> .



###  http://dati.gov.it/onto/covapit#hasLegalStatus

:hasLegalStatus rdf:type owl:ObjectProperty ;

                rdfs:label "has legal status"@en ,
                           "ha forma giuridica"@it ;

                rdfs:comment "This property links the private organization to its legal status"@en ;

                owl:versionInfo "stable"@en ;

                rdfs:comment "Questa proprietà lega l'organizzazione privata alla sua forma giuridica."@it ;

                owl:versionInfo "stabile"@it ;

                rdfs:range :LegalStatus ;

                rdfs:domain :PrivateOrganization ;

                rdfs:isDefinedBy <http://dati.gov.it/onto/covapit/> .



###  http://dati.gov.it/onto/covapit#hasLogo

:hasLogo rdf:type owl:ObjectProperty ;

         rdfs:label "has logo"@en ,
                    "ha logo"@it ;

         rdfs:comment "This property links the organization to the Image object representing the logo of the organization."@en ;

         owl:versionInfo "stable"@en ;

         rdfs:comment "Questa proprietà collega un'organizzazione a un oggetto immagine e rappresenta il logo dell'organizzazione."@it ;

         owl:versionInfo "stabile"@it ;

         rdfs:domain :Organization ;

         rdfs:isDefinedBy <http://dati.gov.it/onto/covapit/> ;

         rdfs:range smapit:Image ;

         rdfs:subPropertyOf smapit:hasImage .



###  http://dati.gov.it/onto/covapit#hasParticipationType

:hasParticipationType rdf:type owl:ObjectProperty ;

                      rdfs:label "has participation type"@en ,
                                 "ha tipo di partecipazione"@it ;

                      rdfs:comment "This property links the Participation to its type (i.e., direct or indirect)."@en ;

                      owl:versionInfo "stable"@en ;

                      rdfs:comment "Questa proprietà collega la partecipazione al suo tipo (diretta o indiretta)."@it ;

                      owl:versionInfo "stabile"@it ;

                      rdfs:domain :Participation ;

                      rdfs:range :ParticipationType ;

                      rdfs:isDefinedBy <http://dati.gov.it/onto/covapit/> .



###  http://dati.gov.it/onto/covapit#hasSubOrganization

:hasSubOrganization rdf:type owl:ObjectProperty ;

                    rdfs:label "has sub-organization"@en ,
                               "ha sotto-organizzazione"@it ;

                    rdfs:comment "Organizations are often large and complex and may be a collection of smaller organizations, each of which has a specific identity that may be legally defined.  This property, together with its inverse subOrganizationOf allows one to model a hierarchical structure between organizations."@en ;

                    owl:versionInfo "stable"@en ;

                    rdfs:comment "Organizzazioni sono spesso grandi e complesse e possono essere viste come una collezione di organizzazioni più piccole, ognuna delle quali con una specifica identità legamente definita. Questa proprietà, insieme alla sua inversa sotto-organizzazione di, consente di modellare una struttura gerarchica tra organizzazioni. Si pensi per esempio ai concetti di holding per organizazioni private."@it ;

                    owl:versionInfo "stabile"@it ;

                    rdfs:domain :Organization ;

                    rdfs:range :Organization ;

                    rdfs:isDefinedBy <http://dati.gov.it/onto/covapit/> .



###  http://dati.gov.it/onto/covapit#hasSupportUnit

:hasSupportUnit rdf:type owl:ObjectProperty ;

                rdfs:label "has support unit"@en ,
                           "ha unità di supperto (dipartimento)"@it ;

                rdfs:comment "This property links an organization to its support units (or dividision or departments) that can be homogeneous organizational areas or offices."@en ;

                owl:versionInfo "stable"@en ;

                rdfs:comment "Questa proprietà lega un'organizzazione alle sue unità di supporto (o divisioni o dipartimenti) che possono essere areee organizzative omogenee o uffici"@it ;

                owl:versionInfo "stabile"@it ;

                rdfs:domain :Organization ;

                rdfs:range :SupportUnit ;

                rdfs:isDefinedBy <http://dati.gov.it/onto/covapit/> .



###  http://dati.gov.it/onto/covapit#hasTypoloy

:hasTypoloy rdf:type owl:ObjectProperty ;

            rdfs:label "has typology"@en ,
                       "ha tipologia"@it ;

            rdfs:comment "This property links the organization to its typology"@en ;

            owl:versionInfo "stable"@en ;

            rdfs:comment "Questa proprietà lega l'organizzazione alla sua tipologia."@it ;

            owl:versionInfo "stabile"@it ;

            rdfs:domain :Organization ;

            rdfs:range :OrganizationTypology ;

            rdfs:isDefinedBy <http://dati.gov.it/onto/covapit/> .



###  http://dati.gov.it/onto/covapit#holdEmployment

:holdEmployment rdf:type owl:ObjectProperty ;

                rdfs:label "hold membership"@en ,
                           "detiene una relazione di appartenenza"@it ;

                rdfs:comment "This property links an Agent to the situation Membership in an organization."@en ;

                owl:versionInfo "stable"@en ;

                rdfs:comment "Questa proprietà lega un agente alla sitauzione di appartenenza a un'organizzazione."@it ;

                owl:versionInfo "stabile"@it ;

                rdfs:range :Employment ;

                rdfs:isDefinedBy <http://dati.gov.it/onto/covapit/> ;

                rdfs:domain l0:Agent .



###  http://dati.gov.it/onto/covapit#isPartOf

:isPartOf rdf:type owl:ObjectProperty ;

          rdfs:label "is part of"@en ,
                     "è parte di"@it ;

          rdfs:comment "This property links an office (or organizational unit) to its homogeneous organizational area of reference. This may be applied in the specific case of public organizations."@en ;

          owl:versionInfo "unstable"@en ;

          rdfs:comment "Questa proprietà lega un ufficio (o unità organizzativa) alla sua AOO (area organizzativa omogenea) di riferimento. Questo caso potrebbe presentarsi nello specifico per le organizzazioni pubbliche."@it ;

          owl:versionInfo "provvisoria"@it ;

          rdfs:range :HomogeneousOrganizationalArea ;

          rdfs:domain :Office ;

          rdfs:isDefinedBy <http://dati.gov.it/onto/covapit/> .



###  http://dati.gov.it/onto/covapit#isSupportUnitOf

:isSupportUnitOf rdf:type owl:ObjectProperty ;

                 rdfs:label "is support unit of"@en ,
                            "è unità di supporto di"@it ;

                 rdfs:comment "This property is the inverse of has support unit and links a support unit with its organization."@en ;

                 owl:versionInfo "stable"@en ;

                 rdfs:comment "Questa proprietà è l'inversa di ha unità di supporto e lega l'unità di supporto con la relativa organizzazione."@it ;

                 owl:versionInfo "stabile"@it ;

                 rdfs:range :Organization ;

                 rdfs:domain :SupportUnit ;

                 owl:inverseOf :hasSupportUnit ;

                 rdfs:isDefinedBy <http://dati.gov.it/onto/covapit/> .



###  http://dati.gov.it/onto/covapit#orginalOrganization

:orginalOrganization rdf:type owl:ObjectProperty ;

                     rdfs:label "original organization"@en ,
                                "organizzazione di origine"@it ;

                     rdfs:comment "The property links a Change Event to the organization that existed before the change. Although the Foundation Event class is defined as a sub class of Change Event, it is inappropriate to use this property with the Foundation Event class."@en ;

                     owl:versionInfo "stable"@en ;

                     rdfs:comment "Questa proprietà collega un evento del cambiamento all'organizzazione che esisteva prima del cambiamento. Sebbene la classe Costituzione sia sotto classe di Evento del Cambiamento, è inappropriato utilizzare tale classe con questa proprietà."@it ;

                     owl:versionInfo "stabile"@it ;

                     rdfs:domain :ChangeEvent ;

                     rdfs:range :Organization ;

                     owl:inverseOf :changedBy ;

                     rdfs:isDefinedBy <http://dati.gov.it/onto/covapit/> .



###  http://dati.gov.it/onto/covapit#participated

:participated rdf:type owl:ObjectProperty ;

              rdfs:label "participated"@en ,
                         "partecipata"@it ;

              rdfs:comment "This property links the situation of participation of an organization to an organization. Specifically this property models the fact that an organization is participated by another one."@en ;

              owl:versionInfo "unstable"@en ;

              rdfs:comment "Questa proprietà collega la situazione partecipazione di un'organizzazione all0organizzazione. In particolare, questa proprietà modella il fatto che un'organizzazione è partecipata dall'altra."@it ;

              owl:versionInfo "provvisoria"@it ;

              rdfs:range :Organization ;

              rdfs:domain :Participation ;

              rdfs:isDefinedBy <http://dati.gov.it/onto/covapit/> .



###  http://dati.gov.it/onto/covapit#participating

:participating rdf:type owl:ObjectProperty ;

               rdfs:label "participating"@en ,
                          "partecipante"@it ;

               rdfs:comment "This property links the organization to the situation partecipation. The property models the fact that an organization participates in another one."@en ;

               owl:versionInfo "unstable"@en ;

               rdfs:comment "Questa proprietà collega un'organizzazione alla situazione partecipazione. In particolare, la proprietà modella il fatto che un'organizzazione partecipa in un'altra."@it ;

               owl:versionInfo "provvisoria"@it ;

               rdfs:domain :Organization ;

               rdfs:range :Participation ;

               rdfs:isDefinedBy <http://dati.gov.it/onto/covapit/> .



###  http://dati.gov.it/onto/covapit#precedes

:precedes rdf:type owl:ObjectProperty ,
                   owl:TransitiveProperty ;

          rdfs:label "precedes"@en ,
                     "precede"@it ;

          rdfs:comment "This property models an ordered sequence of organizations that precede and succeed each other.. In particular this property represents the organization that precedes another."@en ;

          owl:versionInfo "stable"@en ;

          rdfs:comment "Questa proprietà modella una sequenza ordinata di organizzazioni that precedono o si susseguono le un con le altre.  Nello specifico questa proprietà rappresenta un'organizzazione che precede un'altra."@it ;

          owl:versionInfo "stabile"@it ;

          rdfs:domain :Organization ;

          rdfs:range :Organization ;

          rdfs:isDefinedBy <http://dati.gov.it/onto/covapit/> .



###  http://dati.gov.it/onto/covapit#resultedFrom

:resultedFrom rdf:type owl:ObjectProperty ;

              rdfs:label "resulted from"@en ,
                         "risultante da"@it ;

              rdfs:comment "This property is the inverse of resulting organization and links an organization to the change event through which the organization has been created."@en ;

              owl:versionInfo "stable"@en ;

              rdfs:comment "Questa proprietà è l'inversa di organizzazione risultante e lega un'organizzazione all'evento di cambiameno attraverso cui l'organizzazione si è originata."@it ;

              owl:versionInfo "stabile"@it ;

              rdfs:range :ChangeEvent ;

              rdfs:domain :Organization ;

              owl:inverseOf :resultingOrganization ;

              rdfs:isDefinedBy <http://dati.gov.it/onto/covapit/> .



###  http://dati.gov.it/onto/covapit#resultingOrganization

:resultingOrganization rdf:type owl:ObjectProperty ;

                       rdfs:label "resulting organization"@en ,
                                  "organizzazione risultante"@it ;

                       rdfs:comment "This property links a Change Event  to the organization that resulted from it."@en ;

                       owl:versionInfo "stable"@en ;

                       rdfs:comment "Questa proprietà lega un evento di cambiamento alla organizzazione che risultante al termine del cambiamento."@it ;

                       owl:versionInfo "stabile"@it ;

                       rdfs:domain :ChangeEvent ;

                       rdfs:range :Organization ;

                       rdfs:isDefinedBy <http://dati.gov.it/onto/covapit/> .



###  http://dati.gov.it/onto/covapit#subOrganizationOf

:subOrganizationOf rdf:type owl:ObjectProperty ;

                   rdfs:label "sub-organization of"@en ,
                              "sotto-organizzazione di"@it ;

                   rdfs:comment "Organizations are often large and complex and may be a collection of smaller organizations, each of which has a specific identity that may be legally defined.  This property, together with its inverse hasSubOrganization allows one to model a hierarchical structure between organizations."@en ;

                   owl:versionInfo "stable"@en ;

                   rdfs:comment "Organizzazioni sono spesso grandi e complesse e possono essere viste come una collezione di organizzazioni più piccole, ognuna delle quali con una specifica identità legamente definita. Questa proprietà, insieme alla sua inversa ha sotto-organizzazione, consente di modellare una struttura gerarchica tra organizzazioni. Si pensi per esempio ai concetti di holding per organizazioni private."@it ;

                   owl:versionInfo "stabile"@it ;

                   rdfs:domain :Organization ;

                   rdfs:range :Organization ;

                   owl:inverseOf :hasSubOrganization ;

                   rdfs:isDefinedBy <http://dati.gov.it/onto/covapit/> .



###  http://dati.gov.it/onto/covapit#withRole

:withRole rdf:type owl:FunctionalProperty ,
                   owl:ObjectProperty ;

          rdfs:label "with role"@en ,
                     "con ruolo"@it ;

          rdfs:comment "This property links the situation membership with the related Role."@en ;

          owl:versionInfo "stable"@en ;

          rdfs:comment "Questa proprietà lega la situazione Appartenenza al relativo Ruolo. E' utilizzata per specificare il ruolo di un membro in una situazione di appartenenza dello stesso a un'organizzazione."@it ;

          owl:versionInfo "stabile"@it ;

          rdfs:domain :Employment ;

          rdfs:range :Role ;

          rdfs:isDefinedBy <http://dati.gov.it/onto/covapit/> .





