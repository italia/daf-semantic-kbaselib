###  http://dati.gov.it/onto/covapit#hasLogo
:hasLogo rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf smapit:hasImage ;
         rdfs:domain :Organization ;
         rdfs:range smapit:Image ;
         rdfs:comment "Questa proprietà collega un'organizzazione a un oggetto immagine e rappresenta il logo dell'organizzazione."@it ,
                      "This property links the organization to the Image object representing the logo of the organization."@en ;
         rdfs:isDefinedBy <http://dati.gov.it/onto/covapit/> ;
         rdfs:label "ha logo"@it ,
                    "has logo"@en ;
         owl:versionInfo "stabile"@it ,
                         "stable"@en .


###  http://dati.gov.it/onto/covapit#hasParticipationType
:hasParticipationType rdf:type owl:ObjectProperty ;
                      rdfs:domain :Participation ;
                      rdfs:range :ParticipationType ;
                      rdfs:comment "Questa proprietà collega la partecipazione al suo tipo (diretta o indiretta)."@it ,
                                   "This property links the Participation to its type (i.e., direct or indirect)."@en ;
                      rdfs:isDefinedBy <http://dati.gov.it/onto/covapit> ;
                      rdfs:label "ha tipo di partecipazione"@it ,
                                 "has participation type"@en ;
                      owl:versionInfo "stabile"@it ,
                                      "stable"@en .


###  http://dati.gov.it/onto/covapit#hasSubOrganization
:hasSubOrganization rdf:type owl:ObjectProperty ;
                    owl:inverseOf :subOrganizationOf ;
                    rdfs:domain :Organization ;
                    rdfs:range :Organization ;
                    rdfs:comment "Organizations are often large and complex and may be a collection of smaller organizations, each of which has a specific identity that may be legally defined.  This property, together with its inverse subOrganizationOf allows one to model a hierarchical structure between organizations."@en ,
                                 "Organizzazioni sono spesso grandi e complesse e possono essere viste come una collezione di organizzazioni più piccole, ognuna delle quali con una specifica identità legamente definita. Questa proprietà, insieme alla sua inversa sotto-organizzazione di, consente di modellare una struttura gerarchica tra organizzazioni. Si pensi per esempio ai concetti di holding per organizazioni private."@it ;
                    rdfs:isDefinedBy <http://dati.gov.it/onto/covapit> ;
                    rdfs:label "ha sotto-organizzazione"@it ,
                               "has sub-organization"@en ;
                    owl:versionInfo "stabile"@it ,
                                    "stable"@en .


###  http://dati.gov.it/onto/covapit#hasSupportUnit
:hasSupportUnit rdf:type owl:ObjectProperty ;
                owl:inverseOf :isSupportUnitOf ;
                rdfs:domain :Organization ;
                rdfs:range :SupportUnit ;
                rdfs:comment "Questa proprietà lega un'organizzazione alle sue unità di supporto (o divisioni o dipartimenti) che possono essere areee organizzative omogenee o uffici"@it ,
                             "This property links an organization to its support units (or dividision or departments) that can be homogeneous organizational areas or offices."@en ;
                rdfs:isDefinedBy <http://dati.gov.it/onto/covapit> ;
                rdfs:label "ha unità di supperto (dipartimento)"@it ,
                           "has support unit"@en ;
                owl:versionInfo "stabile"@it ,
                                "stable"@en .


