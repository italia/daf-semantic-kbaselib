             rdfs:range :PublicOrganizationCategory ;
             rdfs:comment "Questa proprietà lega l'organizzazione pubblica alla propria categoria."@it ,
                          "This property links the public organization to its category."@en ;
             rdfs:isDefinedBy <http://dati.gov.it/onto/covapit> ;
             rdfs:label "ha categoria"@it ,
