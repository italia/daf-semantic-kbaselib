:hasTypology rdf:type owl:ObjectProperty ;
             rdfs:domain :Organization ;
             rdfs:range :OrganizationTypology ;
             rdfs:comment "Questa proprietà lega l'organizzazione alla sua tipologia."@it ,
                          "This property links the organization to its typology"@en ;
             rdfs:isDefinedBy <http://dati.gov.it/onto/covapit> ;
             rdfs:label "ha tipologia"@it ,
                        "has typology"@en ;
