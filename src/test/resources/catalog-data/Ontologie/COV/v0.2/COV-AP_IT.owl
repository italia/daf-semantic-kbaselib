AnnotationAssertion(rdfs:isDefinedBy :Closing <http://dati.gov.it/onto/covapit/>)
AnnotationAssertion(rdfs:label :Closing "Chiusura"@it)
AnnotationAssertion(owl:versionInfo :Closing "stable"@en)
AnnotationAssertion(owl:versionInfo :Closing "stabile"@it)
AnnotationAssertion(rdfs:comment :Closing "This class represents the Closing event for an organization."@en)
AnnotationAssertion(rdfs:comment :Closing "Questa classe rappresenta un evento di Chiusra per un'organizzazione."@it)
SubClassOf(:Closing :ChangeEvent)
