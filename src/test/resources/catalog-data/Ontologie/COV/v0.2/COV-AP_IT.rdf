    <rdfs:label xml:lang="it">ha email</rdfs:label>
    <rdfs:label xml:lang="en">has email</rdfs:label>
    <rdfs:domain rdf:resource="http://dati.gov.it/onto/covapit#ContactPoint"/>
    <rdfs:comment xml:lang="it">Questa proprietà collega il punto di contatto con uno o più indirizzi email di vario tipo. Almeno un indirizzo email deve essere specificato per il punto di contatto di un'organizzazione.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/covapit/"/>
    <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/covapit#actsonBehalfOf">
