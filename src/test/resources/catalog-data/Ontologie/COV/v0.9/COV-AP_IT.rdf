            <owl:Restriction>
                <owl:onProperty rdf:resource="http://dati.gov.it/onto/covapit#isPartOf"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="http://dati.gov.it/onto/covapit#HomogeneousOrganizationalArea"/>
