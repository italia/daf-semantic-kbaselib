        <rdfs:label xml:lang="it">Fusione</rdfs:label>
        <rdfs:label xml:lang="en">Merger</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:Class>
    


    <!-- http://dati.gov.it/onto/covapit#Office -->

    <owl:Class rdf:about="http://dati.gov.it/onto/covapit#Office">
