        <rdfs:comment xml:lang="en">This property is used to model the total tax burden of the year which impacts on the balance of the organization. This data is required by specific Italian law.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/covapit"/>
        <rdfs:label xml:lang="it">onere complessivo</rdfs:label>
        <rdfs:label xml:lang="en">total tax burden</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:DatatypeProperty>
    


