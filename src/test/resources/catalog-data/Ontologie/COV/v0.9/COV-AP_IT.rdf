        <rdfs:comment xml:lang="en">This property represents the last validation date for the tax code associated with the eInvoiceService.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/covapit"/>
        <rdfs:label xml:lang="en">tax code validation date</rdfs:label>
        <rdfs:label xml:lang="it">ultima data validazione codice fiscale</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
