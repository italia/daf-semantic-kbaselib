<http://dati.gov.it/onto/clvapit> rdf:type owl:Ontology ;
                                   owl:versionIRI <http://dati.gov.it/onto/clvapit/0.6> ;
                                   owl:imports ti: ,
                                               l0: ;
                                   owl:versionInfo "Versione 0.6 - 18 Ottobre 2017 - Aggiunta proprietà hasSpatialCoverage a Feature e aggiunto assiome: hasAddress sotto proprietà di hasSpatialCoverage."@it ;
                                   dc:creator "Institute of Cognitive Sciences and Technologies of the Italian Research Council (CNR) - Semantic Technology Laboratory (STLab)"@en ,
                                              "Italian Digital Transformation Team"@en ;
                                   rdfs:label "Address(Location)"@en ;
                                   dct:issued "2017-08-07"^^xsd:date ;
                                   dct:modified "2017-10-18" ;
                                   prov:wasDerivedFrom <http://dati.gov.it/onto/clvapit/0.5> ;
                                   rdfs:isDefinedBy <http://dati.gov.it/onto/clvapit> ;
                                   owl:versionInfo "Version 0.6 - 18 October 2017 - Added a property hasSpatialCoverage to Feature and transformed hasAddress as subproperty of hasSpatialCoverage"@en ;
                                   dc:creator "Agency for Digital Italy - AgID"@en ;
                                   rdfs:label "Indirizzi(Luoghi)"@it ;
                                   rdfs:comment "Questa è l'ontologia del profilo di italiano sugli indirizzi che hanno anche una connotazione geografica (Italian Core Location Vocabulary - Application Profile  - CLV-AP_IT)"@it ,
                                                "This is the ontology of the Italian application profile for Location and Addresses"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://dati.gov.it/onto/clvapit#hasRankOrder
:hasRankOrder rdf:type owl:AnnotationProperty .


###  http://www.w3.org/ns/prov#wasDerivedFrom
prov:wasDerivedFrom rdf:type owl:AnnotationProperty .


###  http://www.w3.org/ns/prov#wasInfluencedBy
prov:wasInfluencedBy rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://dati.gov.it/onto/clvapit#hasAddress
:hasAddress rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :hasSpatialCoverage ;
            owl:inverseOf :isAddressFor ;
            rdfs:domain owl:Thing ;
            rdfs:range :Address ;
            rdfs:comment "Questa proprietà specializza la proprietà ha caratteristica per collegare una cosa del dominio da modellare con un indirizzo."@it ,
                         "This property specializes the property has feature in order to link a thing to an address."@en ;
            rdfs:isDefinedBy <http://dati.gov.it/onto/clvapit> ;
            rdfs:label "ha indirizzo"@it ,
                       "has address"@en ;
            owl:versionInfo "stabile"@it ,
                            "stable"@en .


###  http://dati.gov.it/onto/clvapit#hasAddressArea
:hasAddressArea rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :hasAddressComponent ;
                rdfs:domain :Address ;
                rdfs:range :AddressArea ;
                rdfs:comment "Questa proprietà specializza la proprietà ha componente indirizzo per collegare l'indirizzo a un'area geografica o località che raggruppa diverse areee geografiche."@it ,
                             "This property specializes the property has address component in order to link the address to a geographical area or a locality that groups different addressable objects."@en ;
                rdfs:isDefinedBy <http://dati.gov.it/onto/clvapit> ;
                rdfs:label "ha area indirizzo"@it ,
                           "has address area"@en ;
                owl:versionInfo "provvisoria"@it ,
                                "unstable"@en .


###  http://dati.gov.it/onto/clvapit#hasAddressComponent
:hasAddressComponent rdf:type owl:ObjectProperty ;
                     rdfs:domain :Address ;
                     rdfs:range :AddressComponent ;
                     rdfs:comment "Property that links an address to its components (street, city, province, number, etc.)"@en ,
                                  "Proprietà che lega l'indirizzo alle sue componenti che lo formano (e.g., via, città, provincia, numero civico, ecc.)"@it ;
                     rdfs:isDefinedBy <http://dati.gov.it/onto/clvapit> ;
                     rdfs:label "ha componente indirizzo"@it ,
                                "has address component"@en ;
                     owl:versionInfo "stabile"@it ,
                                     "stable"@en .


###  http://dati.gov.it/onto/clvapit#hasAdressinTime
:hasAdressinTime rdf:type owl:ObjectProperty ;
                 rdfs:domain owl:Thing ;
                 rdfs:range :AddressInTime ;
                 rdfs:comment "Property that links any thing of the modelled domain with the situation of an address that changes over time."@en ,
                              "Proprietà che lega una qualsiasi cosa del dominio modellato alla situazione utilizzata per esprimere il cambiamento di un indirizzo nel tempo."@it ;
                 rdfs:isDefinedBy <http://dati.gov.it/onto/clvapit> ;
                 rdfs:label "ha indirizzo nel tempo"@it ,
                            "has address in time"@en ;
                 owl:versionInfo "provvisoria"@it ,
                                 "unstable"@en .


###  http://dati.gov.it/onto/clvapit#hasCensusSection
:hasCensusSection rdf:type owl:ObjectProperty ,
                           owl:InverseFunctionalProperty ;
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( :AddressArea
                                              :City
                                            )
                              ] ;
                  rdfs:range :CensusSection ;
                  rdfs:comment "Property that links a city or an address area to its census section."@en ,
                               "Proprietà che collega una città o un area dell'indirizzo a una sezione di censimento."@it ;
                  rdfs:isDefinedBy <http://dati.gov.it/onto/clvapit> ;
                  rdfs:label "ha sezione di censimento"@it ,
                             "has census section"@en ;
                  owl:versionInfo "provvisoria"@it ,
                                  "unstable"@en .


###  http://dati.gov.it/onto/clvapit#hasCity
:hasCity rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :hasAddressComponent ;
         rdfs:domain :Address ;
         rdfs:range :City ;
         rdfs:comment "Questa proprietà specializza la proprietà ha componente indirzzo per collegare l'indirizzo alla componente Comune dello stesso.  Esempio di comune: Roma, Bologna, Palermo, Milano, ecc."@it ,
                      "This property speciaizes the property has address component in order to link the address to the city component of the address itself."@en ;
         rdfs:isDefinedBy <http://dati.gov.it/onto/clvapit> ;
         rdfs:label "ha comune"@it ,
                    "has city"@en ;
         owl:versionInfo "provvisoria"@it ,
                         "unstable"@en .


###  http://dati.gov.it/onto/clvapit#hasCountry
:hasCountry rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :hasAddressComponent ;
            rdfs:domain :Address ;
            rdfs:range :Country ;
            rdfs:comment "La proprietà specializza la proprietà ha componente indirizzo per collegare l'indirizzo allo stato, parte dello stesso. Esempio: Italia, Francia, ecc."@it ,
                         "This property specializes the property has address component in order to link the address with its component representing the country."@en ;
            rdfs:isDefinedBy <http://dati.gov.it/onto/clvapit> ;
            rdfs:label "ha stato"@it ,
                       "has country"@en ;
            owl:versionInfo "provvisoria"@it ,
                            "unstable"@en .


###  http://dati.gov.it/onto/clvapit#hasDirectHigherRank
:hasDirectHigherRank rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :hasHigherRank ;
                     rdfs:domain :NamedAddressComponent ;
                     rdfs:range :NamedAddressComponent ;
                     rdfs:comment "Questa proprietà mette in relazione componenti dell'indirizzo con un nome, dove la prima è più specifica della seconda e non c'è un altra componente con nome che è sia più generale della prima che più specifica della seconda."@it ,
                                  "This property relates two named address components, where the first is more specific then the second and there is no other named address component that is both more general than the first and more specific than the second."@en ;
                     rdfs:isDefinedBy <http://dati.gov.it/onto/clvapit> ;
                     rdfs:label "ha livello più alto diretto"@it ,
                                "has direct higher rank"@en ;
                     owl:versionInfo "provvisoria"@it ,
                                     "unstable"@en .


###  http://dati.gov.it/onto/clvapit#hasDirectLowerRank
:hasDirectLowerRank rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :hasLowerRank ;
                    rdfs:domain :NamedAddressComponent ;
                    rdfs:range :NamedAddressComponent ;
                    rdfs:comment "Questa proprietà mette in relazione componenti dell'indirizzo con un nome, dove la prima è meno specifica della seconda e non esise un'altra componente dell'indirizzo con nome che è sia più specifica della prima che più generale della seconda."@it ,
                                 "This property relates two named address components, where the first is less specific then the second and there is no other named address component that is both more specific than the first and more general than the second."@en ;
                    rdfs:isDefinedBy <http://dati.gov.it/onto/clvapit> ;
                    rdfs:label "ha livello diretto più basso"@it ,
                               "has direct lower rank"@en ;
                    owl:versionInfo "provvisoria"@it ,
                                    "unstable"@en .


###  http://dati.gov.it/onto/clvapit#hasGeometry
:hasGeometry rdf:type owl:ObjectProperty ;
             owl:inverseOf :isGeometryFor ;
             rdfs:domain owl:Thing ;
             rdfs:range :Geometry ;
             rdfs:comment "Questa proprietà lega una qualsiasi cosa del dominio a una geometria."@it ,
                          "This property links any thing of the domain to the geometry."@en ;
             rdfs:isDefinedBy <http://dati.gov.it/onto/clvapit> ;
             rdfs:label "ha geometria"@it ,
                        "has geometry"@en ;
             owl:versionInfo "provvisoria"@it ,
                             "unstable"@en .


###  http://dati.gov.it/onto/clvapit#hasGeometryType
:hasGeometryType rdf:type owl:ObjectProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain :Geometry ;
                 rdfs:range :GeometryType ;
                 rdfs:comment "Property that links the Geometry to its Type (Polygon, Point, Line)"@en ,
                              "Proprietà che collega la geometria al suo tipo (poligono, punto, linea)."@it ;
                 rdfs:isDefinedBy <http://dati.gov.it/onto/clvapit> ;
                 rdfs:label "ha tipo geometria"@it ,
                            "has geometry type"@en ;
                 owl:versionInfo "provvisoria"@it ,
                                 "unstable"@en .


###  http://dati.gov.it/onto/clvapit#hasHigherRank
:hasHigherRank rdf:type owl:ObjectProperty ;
               rdfs:domain :NamedAddressComponent ;
               rdfs:range :NamedAddressComponent ;
               rdfs:comment "Questa proprietà è utilizzata per mettere in relazione due componenti dell'indirizzo che hanno un nome, dove la prima è più specifica della seconda."@it ,
                            "This property is used to  relate two named address component, where the first is more specific than the second."@en ;
               rdfs:isDefinedBy <http://dati.gov.it/onto/clvapit> ;
               rdfs:label "ha livello più alto"@it ,
                          "has higher rank"@en ;
               owl:versionInfo "provvisoria"@it ,
                               "unstable"@en .


###  http://dati.gov.it/onto/clvapit#hasIdentifier
:hasIdentifier rdf:type owl:ObjectProperty ;
               rdfs:domain :SpatialObject ;
               rdfs:range :Identifier ;
               rdfs:comment "Questa proprietà collega un oggetto spaziale alla classe identificativo che rappresenta uno o più identificativi associabili a un oggetto spaziale. La proprietà è ereditata da tutte le sotto classi di oggetto spaziale. Esempi di identificativi sono il codice istat, l'identificativo di un indirizzo secondo le specifiche inspire, il codice assegnato dal comune, ecc."@it ,
                            "This property allows one to specify an identifier for the spatial object. The property is inherited by all the subclasses of spatial object."@en ;
               rdfs:isDefinedBy <http://dati.gov.it/onto/clvapit> ;
               rdfs:label "ha identificativo (ha codice)"@it ,
                          "has identifier"@en ;
               owl:versionInfo "stabile"@it ,
                               "stable"@en .


###  http://dati.gov.it/onto/clvapit#hasLowerRank
:hasLowerRank rdf:type owl:ObjectProperty ;
              rdfs:domain :NamedAddressComponent ;
              rdfs:range :NamedAddressComponent ;
              rdfs:comment "Questa proprietà mette in relazione due componenti dell'indirizzo con nomi, dove la prima è meno specifica della seconda."@it ,
                           "This property relates two named address components, where the first is less specific than the second."@en ;
              rdfs:isDefinedBy <http://dati.gov.it/onto/clvapit> ;
              rdfs:label "ha livello più basso"@it ,
                         "has lower rank"@en ;
              owl:versionInfo "provvisoria"@it ,
                              "unstable"@en .


###  http://dati.gov.it/onto/clvapit#hasNumber
:hasNumber rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :hasAddressComponent ;
           rdfs:domain :Address ;
           rdfs:range :AddressNumber ;
           rdfs:comment "Questa proprietà specializza la proprietà ha componente indirizzo per collegare l'indirizzo al numero civico parte dello stesso."@it ,
                        "This property specializes the property has address component in order to link the address with its part representing the street number (or housing number)."@en ;
           rdfs:isDefinedBy <http://dati.gov.it/onto/clvapit> ;
           rdfs:label "ha numero civico"@it ,
                      "has street number"@en ;
           owl:versionInfo "provvisoria"@it ,
                           "unstable"@en .


###  http://dati.gov.it/onto/clvapit#hasPrimaryAddress
:hasPrimaryAddress rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :hasAddress ;
                   rdf:type owl:FunctionalProperty ;
                   rdfs:domain owl:Thing ;
                   rdfs:range :Address ;
                   rdfs:comment "Questa proprietà è sotto proprietà di ha indirizzo ed è utilizzata per modellare lo specifico indirizzo primario di una cosa del dominio a cui è collegata (e.g., l'indirizzo primario di un'organizzazione, l'indirizzo primario di una persona, ecc.)."@it ,
                                "This property is sub-property of has address and it is used to model the specific primary address of a thing of the modelled domain to which it is linked (e.g., the primary address of an organization or a person)."@en ;
                   rdfs:isDefinedBy <http://dati.gov.it/onto/clvapit> ;
                   rdfs:label "ha indirizzo primario"@it ,
                              "has primary address"@en ;
                   owl:versionInfo "provvisoria"@it ,
                                   "unstable"@en .


###  http://dati.gov.it/onto/clvapit#hasProvince
:hasProvince rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :hasAddressComponent ;
             rdfs:domain :Address ;
             rdfs:range :Province ;
             rdfs:comment "Questa proprietà specializza la proprietà ha componente indirizzo per collegare l'indirizzo alla componente Provincia dello stesso. Esempio di Provincia: \"PA\", \"BO\", \"RM\", ecc."@it ,
                          "This property specializes the property has address component in order to link the address with the Province, part of the address itself."@en ;
             rdfs:isDefinedBy <http://dati.gov.it/onto/clvapit> ;
             rdfs:label "ha provincia"@it ,
                        "has province"@en ;
             owl:versionInfo "provvisoria"@it ,
                             "unstable"@en .


###  http://dati.gov.it/onto/clvapit#hasRegion
:hasRegion rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :hasAddressComponent ;
           rdfs:domain :Address ;
           rdfs:range :Region ;
           rdfs:comment "Questa proprietà specializza la proprietà ha componente indirizzo per collegare l'indirizzo con una sua parte, i.e., la Regione del paese. Regione può essere \"Lazio\", \"Emilia Romagna\", ecc."@it ,
                        "This property specializes the property has address component in order to link the address with the Region of the country that is part of the address."@en ;
           rdfs:isDefinedBy <http://dati.gov.it/onto/clvapit> ;
           rdfs:label "ha regione"@it ,
                      "has region"@en ;
           owl:versionInfo "provvisoria"@it ,
                           "unstable"@en .


###  http://dati.gov.it/onto/clvapit#hasSpatialCoverage
:hasSpatialCoverage rdf:type owl:ObjectProperty ;
                    rdfs:domain owl:Thing ;
                    rdfs:range :Feature ;
                    rdfs:comment "Property used to model the spatial coverage of a thing of the modelled domain."@en ,
                                 "Proprietà che consente di esprimere la copertura spaziale di una cosa del dominio da modellare."@it ;
                    rdfs:isDefinedBy <http://dati.gov.it/onto/clvapit> ;
                    rdfs:label "ha copertura spaziale"@it ,
                               "has spatial coverage"@en ;
                    owl:versionInfo "provvisoria"@it ,
                                    "unstable"@en .


###  http://dati.gov.it/onto/clvapit#hasStreet
:hasStreet rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :hasAddressComponent ;
           rdfs:domain :Address ;
           rdfs:range :Street ;
           rdfs:comment "Questa proprietà specializza la proprietù ha componente indirizzo per collegare l'indirizzo alla componente via dell'indirizzo stesso."@it ,
                        "This property specilizes the property has address component in order to link an address to the street component of it."@en ;
           rdfs:isDefinedBy <http://dati.gov.it/onto/clvapit> ;
           rdfs:label "ha via"@it ,
                      "has street"@en ;
           owl:versionInfo "provvisoria"@it ,
                           "unstable"@en .


###  http://dati.gov.it/onto/clvapit#hasValidity
:hasValidity rdf:type owl:ObjectProperty ;
             rdfs:domain :SpatialObject ;
             rdfs:range ti:TimeInterval ;
             rdfs:comment "Questa proprietà associa una validità temporale a un oggetto spaziale."@it ,
                          "This property associate a temporal validity to a spatial object."@en ;
             rdfs:isDefinedBy <http://dati.gov.it/onto/clvapit> ;
             rdfs:label "ha validità"@it ,
                        "has validity"@en ;
             owl:versionInfo "provvisoria"@it ,
                             "unstable"@en .


###  http://dati.gov.it/onto/clvapit#isAddressFor
:isAddressFor rdf:type owl:ObjectProperty ;
              rdfs:domain :Address ;
              rdfs:range owl:Thing ;
              rdfs:comment "Inverse property of has address that links the address to a thing."@en ,
                           "Proprietà inversa di ha indirizzo che collega l'indirizzo a una cosa del dominio da modellare."@it ;
              rdfs:isDefinedBy <http://dati.gov.it/onto/clvapit> ;
              rdfs:label "is address for"@en ,
                         "è indirizzo per"@it ;
              owl:versionInfo "stabile"@it ,
                              "stable"@en .


###  http://dati.gov.it/onto/clvapit#isGeometryFor
:isGeometryFor rdf:type owl:ObjectProperty ;
               rdfs:domain :Geometry ;
               rdfs:range owl:Thing ;
               rdfs:comment "Inverse property of \"hasGeometry\" that links a geometry to the related Thing of the domain."@en ,
                            "Proprietà inversa di \"ha geometria\" che lega una geometria a una cosa del dominio modellato."@it ;
               rdfs:isDefinedBy <http://dati.gov.it/onto/clvapit> ;
               rdfs:label "is geometry for"@en ,
                          "è geometria per"@it ;
               owl:versionInfo "provvisoria"@it ,
                               "unstable"@en .


###  http://dati.gov.it/onto/clvapit#isSpatialCoverageOf
:isSpatialCoverageOf rdf:type owl:ObjectProperty ;
                     rdfs:domain :Feature ;
                     rdfs:range owl:Thing ;
                     rdfs:comment "Inverse property of has feature that links a feature to a thing."@en ,
                                  "Proprietà inversa di ha caratteristica che collega una caratteristica a una cosa."@it ;
                     rdfs:isDefinedBy <http://dati.gov.it/onto/clvapit> ;
                     rdfs:label "is feature of"@en ,
                                "è caratteristica di"@it ;
                     owl:versionInfo "stabile"@it ,
                                     "stable"@en .


###  http://dati.gov.it/onto/clvapit#issuedBy
:issuedBy rdf:type owl:ObjectProperty ;
          rdfs:domain :Identifier ;
          rdfs:range :Organization ;
          rdfs:comment "Questa proprietà collega un identificato all'autorità che l'ha rilasciato."@it ,
                       "This property links an Identifier with the Authority that issued it"@en ;
          rdfs:isDefinedBy <http://dati.gov.it/onto/clvapit> ;
          rdfs:label "issued by"@en ,
                     "rilasciato da"@it ;
          owl:versionInfo "stabile"@it ,
                          "stable"@en .


###  http://dati.gov.it/onto/clvapit#next
:next rdf:type owl:ObjectProperty ;
      rdfs:domain :AddressInTime ;
      rdfs:range :AddressInTime ;
      rdfs:comment "Questa proprietà consente di modellare una sequenza ordinata di indirizzi (che potrebbero essere relazionati l'uno con l'altro come conseguenza di un cambio nella denominaziona avvenuta nel tempo). Nel caso specifico di questa proprietà, essa rappresenta la situazione 'address in time' successiva nella sequenza."@it ,
                   "This property allows one to model an ordered sequence of addresses (that may be related to each other as a consequence of a change occured over time). In this specific case the property represents the next 'address in time' situation in the sequence."@en ;
      rdfs:isDefinedBy <http://dati.gov.it/onto/clvapit> ;
      rdfs:label "next"@en ,
                 "successivo"@it ;
      owl:versionInfo "provvisoria"@it ,
                      "unstable"@en .


###  http://dati.gov.it/onto/clvapit#prev
:prev rdf:type owl:ObjectProperty ;
      rdfs:domain :AddressInTime ;
      rdfs:range :AddressInTime ;
      rdfs:comment "Questa proprietà consente di modellare una sequenza ordinata di indirizzi (che potrebbero essere relazionati l'uno con l'altro come conseguenza di un cambio nella denominaziona avvenuta nel tempo). Nel caso specifico di questa proprietà, essa rappresenta la situazione 'address in time' precedente nella sequenza."@it ,
                   "This property allows one to model an ordered sequence of addresses (that may be related to each other as a consequence of a change occured over time). In this specific case the property represents the previous 'address in time' situation in the sequence."@en ;
      rdfs:isDefinedBy <http://dati.gov.it/onto/clvapit> ;
      rdfs:label "precedente"@it ,
                 "previous"@en ;
      owl:versionInfo "provvisoria"@it ,
                      "unstable"@en .


###  http://dati.gov.it/onto/clvapit#refersToExternalNumber
:refersToExternalNumber rdf:type owl:ObjectProperty ;
                        rdfs:domain :InternalAddressNumber ;
                        rdfs:range :ExternalAddressNumber ;
                        rdfs:comment "Questa proprietà collega il numero interno (accesso interno) al numero civico dell'indirizzo (accesso esterno)."@it ,
                                     "This property links the internal number of the address and the street number (external number)."@en ;
                        rdfs:isDefinedBy <http://dati.gov.it/onto/clvapit> ;
                        rdfs:label "refers to external number"@en ,
                                   "si riferisce a numbero esterno"@it ;
                        owl:versionInfo "provvisoria"@it ,
                                        "unstable"@en .


###  http://dati.gov.it/onto/clvapit#situatedWithin
:situatedWithin rdf:type owl:ObjectProperty ,
                         owl:TransitiveProperty ;
                rdfs:domain :AddressComponent ;
                rdfs:range :AddressComponent ;
                rdfs:comment "It is used to enable the model to express that the subtypes of address components in the dataset form a hierarchy e.g. like: a municipality within a province, which is in turn within a region, in turn within country."@en ,
                             "La proprietà è utilizzata per abilitare il modello nell'esprimere che una certa componente dell'indirizzo nei dati forma una gerarchia. Esempio: un comune è situato in una provincia che è situata in una regione che è situata in uno stato."@it ;
                rdfs:isDefinedBy <http://dati.gov.it/onto/clvapit> ;
                rdfs:label "situated within"@en ,
                           "situato in"@it ;
                owl:versionInfo "stabile"@it ,
                                "stable"@en .


###  http://dati.gov.it/onto/clvapit#withAddress
:withAddress rdf:type owl:ObjectProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain :AddressInTime ;
             rdfs:range :Address ;
             rdfs:comment "Property that links the situation of an address that changes over time to the address itself."@en ,
                          "Proprietà che collega la sitauzione di un indirizzo che cambia nel tempo all'indirizzo stesso."@it ;
             rdfs:isDefinedBy <http://dati.gov.it/onto/clvapit> ;
             rdfs:label "con indirizzo"@it ,
                        "with address"@en ;
             owl:versionInfo "stabile"@it ,
                             "stable"@en .


###  http://dati.gov.it/onto/tiapit/atTime
ti:atTime rdf:type owl:ObjectProperty .


