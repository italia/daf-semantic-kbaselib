###  http://dati.gov.it/onto/clvapit#Province
:Province rdf:type owl:Class ;
          rdfs:subClassOf :NamedAddressComponent ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasDirectLowerRank ;
                            owl:allValuesFrom :City
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasDirectHigherRank ;
                            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :Region
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasHigherRank ;
                            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass [ rdf:type owl:Class ;
                                          owl:unionOf ( :Country
                                                        :Region
                                                      )
                                        ]
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :acronym ;
                            owl:maxCardinality "1"^^xsd:nonNegativeInteger
                          ] ;
          :hasRankOrder 3 ;
          rdfs:comment "La classe rappresenta la componente Provincia dell'indirizzo."@it ,
                       "The class represents the Province of the Address."@en ;
          rdfs:isDefinedBy <http://dati.gov.it/onto/clvapit> ;
          rdfs:label "Province"@en ,
                     "Provincia"@it ;
          owl:versionInfo "stabile"@it ,
                          "stable"@en .


