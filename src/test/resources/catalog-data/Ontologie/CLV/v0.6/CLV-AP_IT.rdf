  <rdf:Description rdf:about="http://dati.gov.it/onto/clvapit#yard">
    <rdfs:comment xml:lang="it">Numero romano assegnato a ciascun spazio privato (cortile, corte e simili) cui si accede agli edifici o agli accessi interni, ove presente;</rdfs:comment>
    <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
    <rdfs:label xml:lang="it">cortile</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/clvapit"/>
    <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    <rdfs:domain rdf:resource="http://dati.gov.it/onto/clvapit#InternalAddressNumber"/>
    <rdfs:label xml:lang="en">yard</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:comment xml:lang="en">If available, the property represents the romanic number assigned to each private space (yard and similar) to which access buildings or other internal accesses,</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/clvapit#metric">
    <rdfs:label xml:lang="it">metrico</rdfs:label>
    <rdfs:comment xml:lang="it">Valore del numero civico assegnato all’accesso esterno espresso in metri, utilizzato in caso di adozione del sistema metrico anziché della successione naturale dei numeri</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
    <rdfs:label xml:lang="en">metric</rdfs:label>
    <rdfs:domain rdf:resource="http://dati.gov.it/onto/clvapit#ExternalAddressNumber"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/clvapit"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="ub14bL1288C40">
    <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Restriction"/>
    <owl:onProperty rdf:resource="http://dati.gov.it/onto/clvapit#name"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/clvapit#hasAdressinTime">
    <rdfs:label xml:lang="en">has address in time</rdfs:label>
    <rdfs:comment xml:lang="en">Property that links any thing of the modelled domain with the situation of an address that changes over time.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="it">ha indirizzo nel tempo</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/clvapit"/>
    <rdfs:comment xml:lang="it">Proprietà che lega una qualsiasi cosa del dominio modellato alla situazione utilizzata per esprimere il cambiamento di un indirizzo nel tempo.</rdfs:comment>
    <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    <rdfs:range rdf:resource="http://dati.gov.it/onto/clvapit#AddressInTime"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/clvapit#flatNumber">
    <rdfs:comment xml:lang="en">Value of the internal number assigned to the internal access according to the natural sequence of numbers</rdfs:comment>
    <rdfs:label xml:lang="en">flat number</rdfs:label>
    <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    <rdfs:domain rdf:resource="http://dati.gov.it/onto/clvapit#InternalAddressNumber"/>
    <rdfs:label xml:lang="it">interno</rdfs:label>
    <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/clvapit"/>
    <rdfs:comment xml:lang="it">Valore del numero interno assegnato all’accesso interno secondo la successione naturale dei numeri;</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/clvapit#Region">
    <rdfs:subClassOf rdf:resource="http://dati.gov.it/onto/clvapit#NamedAddressComponent"/>
    <rdfs:label xml:lang="en">Region</rdfs:label>
    <rdfs:subClassOf rdf:nodeID="ub14bL1370C25"/>
    <rdfs:comment xml:lang="it">Questa classe rappresenta la componente Regione dell'Indirizzo.</rdfs:comment>
    <rdfs:label xml:lang="it">Regione</rdfs:label>
    <hasRankOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</hasRankOrder>
    <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
    <rdfs:subClassOf rdf:nodeID="ub14bL1358C25"/>
    <rdfs:comment xml:lang="en">This class represents the Region of the Address.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/clvapit"/>
    <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    <rdfs:subClassOf rdf:nodeID="ub14bL1362C25"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/clvapit#isGeometryFor">
    <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
    <rdfs:domain rdf:resource="http://dati.gov.it/onto/clvapit#Geometry"/>
    <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/clvapit"/>
    <rdfs:comment xml:lang="en">Inverse property of "hasGeometry" that links a geometry to the related Thing of the domain.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">is geometry for</rdfs:label>
    <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    <rdfs:comment xml:lang="it">Proprietà inversa di "ha geometria" che lega una geometria a una cosa del dominio modellato.</rdfs:comment>
    <rdfs:label xml:lang="it">è geometria per</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:nodeID="ub14bL1184C27">
    <owl:onProperty rdf:resource="http://dati.gov.it/onto/clvapit#serialization"/>
    <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Restriction"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/clvapit#hasRegion">
    <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    <rdfs:subPropertyOf rdf:resource="http://dati.gov.it/onto/clvapit#hasAddressComponent"/>
    <rdfs:domain rdf:resource="http://dati.gov.it/onto/clvapit#Address"/>
    <rdfs:comment xml:lang="it">Questa proprietà specializza la proprietà ha componente indirizzo per collegare l'indirizzo con una sua parte, i.e., la Regione del paese. Regione può essere "Lazio", "Emilia Romagna", ecc.</rdfs:comment>
    <rdfs:label xml:lang="it">ha regione</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/clvapit"/>
    <rdfs:label xml:lang="en">has region</rdfs:label>
    <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range rdf:resource="http://dati.gov.it/onto/clvapit#Region"/>
    <rdfs:comment xml:lang="en">This property specializes the property has address component in order to link the address with the Region of the country that is part of the address.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/clvapit#NamedAddressComponent">
    <rdfs:label xml:lang="en">Named Address Component</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/clvapit"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:nodeID="ub14bL1288C40"/>
    <rdfs:comment xml:lang="it">Classe astratta a supporto nella modellazione per identificare tutte le parti dell'indirizzo che hanno necessariamente un nome.</rdfs:comment>
    <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
    <rdfs:subClassOf rdf:resource="http://dati.gov.it/onto/clvapit#AddressComponent"/>
    <rdfs:comment xml:lang="en">Abstract class used to for modelling reason in order to identify all those components of the address that are associated with a name.</rdfs:comment>
    <rdfs:label xml:lang="it">Componente Indirizzo con Nome</rdfs:label>
    <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
  </rdf:Description>
  <rdf:Description rdf:nodeID="ub14bL984C32">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Restriction"/>
    <owl:onProperty rdf:resource="http://dati.gov.it/onto/clvapit#withAddress"/>
    <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
    <owl:onClass rdf:resource="http://dati.gov.it/onto/clvapit#Address"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/clvapit#hasPrimaryAddress">
    <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:label xml:lang="it">ha indirizzo primario</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:comment xml:lang="en">This property is sub-property of has address and it is used to model the specific primary address of a thing of the modelled domain to which it is linked (e.g., the primary address of an organization or a person).</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/clvapit"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="it">Questa proprietà è sotto proprietà di ha indirizzo ed è utilizzata per modellare lo specifico indirizzo primario di una cosa del dominio a cui è collegata (e.g., l'indirizzo primario di un'organizzazione, l'indirizzo primario di una persona, ecc.).</rdfs:comment>
    <rdfs:range rdf:resource="http://dati.gov.it/onto/clvapit#Address"/>
    <rdfs:label xml:lang="en">has primary address</rdfs:label>
    <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
    <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    <rdfs:subPropertyOf rdf:resource="http://dati.gov.it/onto/clvapit#hasAddress"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/clvapit#ExternalAddressNumber">
    <rdfs:subClassOf rdf:nodeID="ub14bL1099C40"/>
    <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
    <rdfs:subClassOf rdf:nodeID="ub14bL1103C40"/>
    <rdfs:subClassOf rdf:resource="http://dati.gov.it/onto/clvapit#AddressNumber"/>
    <rdfs:label xml:lang="en">Street Number</rdfs:label>
    <rdfs:label xml:lang="it">Numero Civico</rdfs:label>
    <owl:disjointWith rdf:resource="http://dati.gov.it/onto/clvapit#InternalAddressNumber"/>
    <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/clvapit"/>
    <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    <rdfs:subClassOf rdf:nodeID="ub14bL1107C40"/>
    <rdfs:comment xml:lang="en">This class represents the street number for the external accesses to the buildings.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="it">Questa classe rappresenta il numero civico e tutte le informazioni a esso collegate così come definite nell'ambito dell'anagrafe nazionale numeri civici e strate urbane.  I numeri civici individuano gli ACCESSI ESTERNI, cioè quelli che dall’area di circolazione immettono, direttamente o indirettamente, alle unità ecografiche semplici ossia abitazioni, esercizi commerciali, uffici e simili.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:nodeID="fada5794f77974d17b37b1d7866a93428b10">
    <rdf:first rdf:resource="http://dati.gov.it/onto/clvapit#Region"/>
    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/clvapit#hasAddressComponent">
    <rdfs:domain rdf:resource="http://dati.gov.it/onto/clvapit#Address"/>
    <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    <rdfs:range rdf:resource="http://dati.gov.it/onto/clvapit#AddressComponent"/>
    <rdfs:comment xml:lang="it">Proprietà che lega l'indirizzo alle sue componenti che lo formano (e.g., via, città, provincia, numero civico, ecc.)</rdfs:comment>
    <rdfs:label xml:lang="it">ha componente indirizzo</rdfs:label>
    <rdfs:label xml:lang="en">has address component</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="en">Property that links an address to its components (street, city, province, number, etc.)</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/clvapit"/>
    <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/clvapit#floor">
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    <rdfs:domain rdf:resource="http://dati.gov.it/onto/clvapit#InternalAddressNumber"/>
    <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
    <rdfs:label xml:lang="it">piano</rdfs:label>
    <rdfs:label xml:lang="en">floor</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment xml:lang="it">Numero del piano su cui è ubicato l’accesso interno, ove presente;</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/clvapit"/>
    <rdfs:comment xml:lang="en">When available, the property represents the floor number on which the internal access is located.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/clvapit#hasDirectLowerRank">
    <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="it">ha livello diretto più basso</rdfs:label>
    <rdfs:comment xml:lang="en">This property relates two named address components, where the first is less specific then the second and there is no other named address component that is both more specific than the first and more general than the second.</rdfs:comment>
    <rdfs:domain rdf:resource="http://dati.gov.it/onto/clvapit#NamedAddressComponent"/>
    <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/clvapit"/>
    <rdfs:range rdf:resource="http://dati.gov.it/onto/clvapit#NamedAddressComponent"/>
    <rdfs:label xml:lang="en">has direct lower rank</rdfs:label>
    <rdfs:comment xml:lang="it">Questa proprietà mette in relazione componenti dell'indirizzo con un nome, dove la prima è meno specifica della seconda e non esise un'altra componente dell'indirizzo con nome che è sia più specifica della prima che più generale della seconda.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://dati.gov.it/onto/clvapit#hasLowerRank"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/clvapit#building">
    <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    <rdfs:domain rdf:resource="http://dati.gov.it/onto/clvapit#InternalAddressNumber"/>
    <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
    <rdfs:label xml:lang="en">building</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/clvapit"/>
    <rdfs:comment xml:lang="en">When available, it represents the arabic number assigned to the building accessed by the stairs or internal accesses.</rdfs:comment>
    <rdfs:label xml:lang="it">edificio</rdfs:label>
    <rdfs:comment xml:lang="it">Numero arabo assegnato all’edificio cui si accede alle scale o agli accessi interni, ove presente. Questa proprietà potrebbe essere utilizzata anche per identificare un numero di padiglione.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="ub14bL1099C40">
    <owl:onProperty rdf:resource="http://dati.gov.it/onto/clvapit#streetNumber"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Restriction"/>
    <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/clvapit#streetNumber">
    <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/clvapit"/>
    <rdfs:domain rdf:resource="http://dati.gov.it/onto/clvapit#ExternalAddressNumber"/>
    <rdfs:comment xml:lang="it">Valore del numero civico assegnato all’accesso esterno secondo la successione naturale dei numeri;</rdfs:comment>
    <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
    <rdfs:comment xml:lang="en">Value of the street number that is assigned to the external access according to the natural sequence of numbers</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdfs:label xml:lang="it">civico</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/clvapit#Country">
    <hasRankOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</hasRankOrder>
    <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/clvapit"/>
    <rdfs:subClassOf rdf:resource="http://dati.gov.it/onto/clvapit#NamedAddressComponent"/>
    <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
    <rdfs:comment xml:lang="it">Questa classe rappresenta lo stato dell'indirizzo. Esempio: "Italia - Italy" oppure "United Kingdom", ecc.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Country</rdfs:label>
    <rdfs:label xml:lang="it">Stato</rdfs:label>
    <rdfs:subClassOf rdf:nodeID="ub14bL1074C26"/>
    <rdfs:subClassOf rdf:nodeID="ub14bL1070C26"/>
    <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    <rdfs:comment xml:lang="en">This class represents the  uppermost administrative unit for the address, almost always a country. "United Kingdom", "Italy", etc. In the Core Location Vocabulary this is modelled as adminUnit1.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/clvapit#Geometry">
    <rdfs:comment xml:lang="it">Questa classe rappresenta la geometria associabile a un'entità spaziale.</rdfs:comment>
    <rdfs:label xml:lang="en">Geometry</rdfs:label>
    <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
    <rdfs:subClassOf rdf:nodeID="ub14bL1168C27"/>
    <rdfs:subClassOf rdf:resource="http://dati.gov.it/onto/clvapit#SpatialObject"/>
    <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    <rdfs:label xml:lang="it">Geometria</rdfs:label>
    <rdfs:subClassOf rdf:nodeID="ub14bL1184C27"/>
    <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/clvapit"/>
    <prov:wasInfluencedBy rdf:resource="http://www.opengis.net/ont/geosparql#Geometry"/>
    <prov:wasDerivedFrom rdf:resource="https://www.w3.org/ns/locn#Geometry"/>
    <rdfs:subClassOf rdf:nodeID="ub14bL1180C27"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:nodeID="ub14bL1176C27"/>
    <rdfs:subClassOf rdf:nodeID="ub14bL1163C27"/>
    <rdfs:subClassOf rdf:nodeID="ub14bL1172C27"/>
    <rdfs:comment xml:lang="en">This class represents the geometry of a spatial entity.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/clvapit#Organization">
    <rdfs:subClassOf rdf:resource="http://dati.gov.it/onto/l0/Agent"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Otganization</rdfs:label>
    <rdfs:comment xml:lang="it">Questa classe è utilizzata per rappresentare l'organizzazione che ha rilasciato gli identificativi/codici per l'indirizzo (o l'area di circolazione). LLa classe è equivalente alla classe Organizzazione dell'ontologia COV-AP_IT (Core Organization Vocabulary - Italian Application Profile).</rdfs:comment>
    <prov:wasDerivedFrom rdf:resource="http://dati.gov.it/onto/covapit#Organization"/>
    <rdfs:comment xml:lang="en">This class is used to represent the Organization tht issued the identifier for the address or circulation area. This class is equivalent to the Organization class of the COV-AP_IT (Core Organization Vocabulary - Italian Application Profile)</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/clvapit"/>
    <rdfs:label xml:lang="it">Organizzazione</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/l0/Event">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/clvapit#hasGeometryType">
    <rdfs:domain rdf:resource="http://dati.gov.it/onto/clvapit#Geometry"/>
    <rdfs:label xml:lang="en">has geometry type</rdfs:label>
