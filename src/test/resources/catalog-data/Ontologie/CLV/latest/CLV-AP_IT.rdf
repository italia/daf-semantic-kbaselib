     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:dct="http://purl.org/dc/terms/"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:ti="https://w3id.org/italia/onto/TI/"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:covapit="https://w3id.org/italia/onto/COV/"
     xmlns:prov="http://www.w3.org/ns/prov#"
     xmlns:dc="http://purl.org/dc/elements/1.1/">
    <owl:Ontology rdf:about="https://w3id.org/italia/onto/CLV">
        <rdfs:label xml:lang="en">Address(Location)</rdfs:label>
        <rdfs:label xml:lang="it">Indirizzi(Luoghi)</rdfs:label>
        <dct:issued rdf:datatype="&xsd;date">2017-08-07</dct:issued>
        <dct:modified>2018-03-03</dct:modified>
        <dc:creator xml:lang="en">Agency for Digital Italy - AgID</dc:creator>
        <dc:creator xml:lang="en">Institute of Cognitive Sciences and Technologies of the Italian Research Council (CNR) - Semantic Technology Laboratory (STLab)</dc:creator>
        <dc:creator xml:lang="en">Italian Digital Transformation Team</dc:creator>
        <rdfs:comment xml:lang="en">This is the ontology of the Italian application profile for Location and Addresses</rdfs:comment>
        <owl:versionInfo xml:lang="en">Version 0.8 - 03 March 2018 - Use of w3id.org/italia/onto as base URI.</owl:versionInfo>
        <rdfs:comment xml:lang="it">Questa è l&apos;ontologia del profilo di italiano sugli indirizzi che hanno anche una connotazione geografica (Italian Core Location Vocabulary - Application Profile  - CLV-AP_IT)</rdfs:comment>
        <owl:versionInfo xml:lang="it">Versione 0.8 - 03 Marzo 2018 - Uso di w3id.org/italia/onto come base URI</owl:versionInfo>
        <dct:license rdf:resource="https://creativecommons.org/licenses/by/4.0/"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/CLV"/>
        <owl:imports rdf:resource="https://w3id.org/italia/onto/TI"/>
        <owl:imports rdf:resource="https://w3id.org/italia/onto/l0"/>
        <prov:wasDerivedFrom rdf:resource="https://w3id.org/italia/onto/CLV/0.7"/>
        <owl:versionIRI rdf:resource="https://w3id.org/italia/onto/CLV/0.8"/>
    </owl:Ontology>
    


