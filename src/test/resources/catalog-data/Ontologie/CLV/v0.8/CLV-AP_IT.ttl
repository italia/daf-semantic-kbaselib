#################################################################
#
#    Annotation properties
#
#################################################################


###  http://purl.org/dc/terms/license

dct:license rdf:type owl:AnnotationProperty .



###  http://www.w3.org/ns/prov#wasDerivedFrom

prov:wasDerivedFrom rdf:type owl:AnnotationProperty .



###  http://www.w3.org/ns/prov#wasInfluencedBy

prov:wasInfluencedBy rdf:type owl:AnnotationProperty .



###  https://w3id.org/italia/onto/CLV/hasRankOrder

:hasRankOrder rdf:type owl:AnnotationProperty ;
              
              rdfs:domain owl:Thing .



###  https://w3id.org/italia/onto/l0/controlledVocabulary

l0:controlledVocabulary rdf:type owl:AnnotationProperty .





#################################################################
#
#    Object Properties
#
#################################################################


###  https://w3id.org/italia/onto/CLV/hasAddress

:hasAddress rdf:type owl:ObjectProperty ;
            
            rdfs:label "has address"@en ,
                       "ha indirizzo"@it ;
            
            rdfs:comment "This property specializes the property has feature in order to link a thing to an address."@en ;
            
            owl:versionInfo "stable"@en ;
            
            rdfs:comment "Questa proprietà specializza la proprietà ha caratteristica per collegare una cosa del dominio da modellare con un indirizzo."@it ;
            
            owl:versionInfo "stabile"@it ;
            
            rdfs:domain owl:Thing ;
            
            rdfs:isDefinedBy <https://w3id.org/italia/onto/CLV> ;
            
            rdfs:range :Address ;
            
            rdfs:subPropertyOf :hasSpatialCoverage ;
            
            owl:inverseOf :isAddressFor .



###  https://w3id.org/italia/onto/CLV/hasAddressArea

:hasAddressArea rdf:type owl:ObjectProperty ;
                
                rdfs:label "has address area"@en ,
                           "has locality"@en ,
                           "ha area indirizzo"@it ,
                           "ha località"@it ;
                
                rdfs:comment "This property specializes the property has address component in order to link the address to a geographical area or a locality that groups different addressable objects."@en ;
                
                owl:versionInfo "unstable"@en ;
                
                rdfs:comment "Questa proprietà specializza la proprietà ha componente indirizzo per collegare l'indirizzo a un'area geografica o località che raggruppa diverse areee geografiche."@it ;
                
                owl:versionInfo "provvisoria"@it ;
                
                rdfs:isDefinedBy <https://w3id.org/italia/onto/CLV> ;
                
                rdfs:domain :Address ;
                
                rdfs:range :AddressArea ;
                
                rdfs:subPropertyOf :hasAddressComponent .



###  https://w3id.org/italia/onto/CLV/hasAddressComponent

:hasAddressComponent rdf:type owl:ObjectProperty ;
                     
                     rdfs:label "has address component"@en ,
                                "ha componente indirizzo"@it ;
                     
                     rdfs:comment "Property that links an address to its components (street, city, province, number, etc.)"@en ;
                     
                     owl:versionInfo "stable"@en ;
                     
                     rdfs:comment "Proprietà che lega l'indirizzo alle sue componenti che lo formano (e.g., via, città, provincia, numero civico, ecc.)"@it ;
                     
                     owl:versionInfo "stabile"@it ;
                     
                     rdfs:isDefinedBy <https://w3id.org/italia/onto/CLV> ;
                     
                     rdfs:domain :Address ;
                     
                     rdfs:range :AddressComponent .



###  https://w3id.org/italia/onto/CLV/hasAdressinTime

:hasAdressinTime rdf:type owl:ObjectProperty ;
                 
                 rdfs:label "has address in time"@en ,
                            "ha indirizzo nel tempo"@it ;
                 
                 rdfs:comment "Property that links any thing of the modelled domain with the situation of an address that changes over time."@en ;
                 
                 owl:versionInfo "unstable"@en ;
                 
                 rdfs:comment "Proprietà che lega una qualsiasi cosa del dominio modellato alla situazione utilizzata per esprimere il cambiamento di un indirizzo nel tempo."@it ;
                 
                 owl:versionInfo "provvisoria"@it ;
                 
                 rdfs:domain owl:Thing ;
                 
                 rdfs:isDefinedBy <https://w3id.org/italia/onto/CLV> ;
                 
                 rdfs:range :AddressInTime .



###  https://w3id.org/italia/onto/CLV/hasCensusSection

:hasCensusSection rdf:type owl:InverseFunctionalProperty ,
                           owl:ObjectProperty ;
                  
                  rdfs:label "has census section"@en ,
                             "ha sezione di censimento"@it ;
                  
                  rdfs:comment "Property that links a city or an address area to its census section."@en ;
                  
                  owl:versionInfo "unstable"@en ;
                  
                  rdfs:comment "Proprietà che collega una città o un area dell'indirizzo a una sezione di censimento."@it ;
                  
                  owl:versionInfo "provvisoria"@it ;
                  
                  rdfs:isDefinedBy <https://w3id.org/italia/onto/CLV> ;
                  
                  rdfs:range :CensusSection ;
                  
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( :AddressArea
                                              :City
                                            )
                              ] .



###  https://w3id.org/italia/onto/CLV/hasCity

:hasCity rdf:type owl:ObjectProperty ;
         
         rdfs:label "has city"@en ,
                    "has municipality"@en ,
                    "ha città"@it ,
                    "ha comune"@it ;
         
         rdfs:comment "This property speciaizes the property has address component in order to link the address to the city component of the address itself."@en ;
         
         owl:versionInfo "unstable"@en ;
         
         rdfs:comment "Questa proprietà specializza la proprietà ha componente indirzzo per collegare l'indirizzo alla componente Comune dello stesso.  Esempio di comune: Roma, Bologna, Palermo, Milano, ecc."@it ;
         
         owl:versionInfo "provvisoria"@it ;
         
         rdfs:isDefinedBy <https://w3id.org/italia/onto/CLV> ;
         
         rdfs:domain :Address ;
         
         rdfs:range :City ;
         
         rdfs:subPropertyOf :hasAddressComponent .



###  https://w3id.org/italia/onto/CLV/hasCountry

:hasCountry rdf:type owl:ObjectProperty ;
            
            rdfs:label "has country"@en ,
                       "ha stato"@it ;
            
            rdfs:comment "This property specializes the property has address component in order to link the address with its component representing the country."@en ;
            
            owl:versionInfo "unstable"@en ;
            
            rdfs:comment "La proprietà specializza la proprietà ha componente indirizzo per collegare l'indirizzo allo stato, parte dello stesso. Esempio: Italia, Francia, ecc."@it ;
            
            owl:versionInfo "provvisoria"@it ;
            
            rdfs:isDefinedBy <https://w3id.org/italia/onto/CLV> ;
            
            rdfs:domain :Address ;
            
            rdfs:range :Country ;
            
            rdfs:subPropertyOf :hasAddressComponent .



###  https://w3id.org/italia/onto/CLV/hasDirectHigherRank

:hasDirectHigherRank rdf:type owl:ObjectProperty ;
                     
                     rdfs:label "has direct higher rank"@en ,
                                "ha livello più alto diretto"@it ;
                     
                     rdfs:comment "This property relates two named address components, where the first is more specific then the second and there is no other named address component that is both more general than the first and more specific than the second."@en ;
                     
                     owl:versionInfo "unstable"@en ;
                     
                     rdfs:comment "Questa proprietà mette in relazione componenti dell'indirizzo con un nome, dove la prima è più specifica della seconda e non c'è un altra componente con nome che è sia più generale della prima che più specifica della seconda."@it ;
                     
                     owl:versionInfo "provvisoria"@it ;
                     
                     rdfs:isDefinedBy <https://w3id.org/italia/onto/CLV> ;
                     
                     rdfs:range :NamedAddressComponent ;
                     
                     rdfs:domain :NamedAddressComponent ;
                     
                     rdfs:subPropertyOf :hasHigherRank .



###  https://w3id.org/italia/onto/CLV/hasDirectLowerRank

:hasDirectLowerRank rdf:type owl:ObjectProperty ;
                    
                    rdfs:label "has direct lower rank"@en ,
                               "ha livello diretto più basso"@it ;
                    
                    rdfs:comment "This property relates two named address components, where the first is less specific then the second and there is no other named address component that is both more specific than the first and more general than the second."@en ;
                    
                    owl:versionInfo "unstable"@en ;
                    
                    rdfs:comment "Questa proprietà mette in relazione componenti dell'indirizzo con un nome, dove la prima è meno specifica della seconda e non esise un'altra componente dell'indirizzo con nome che è sia più specifica della prima che più generale della seconda."@it ;
                    
                    owl:versionInfo "provvisoria"@it ;
                    
                    rdfs:isDefinedBy <https://w3id.org/italia/onto/CLV> ;
                    
                    rdfs:range :NamedAddressComponent ;
                    
                    rdfs:domain :NamedAddressComponent ;
                    
                    rdfs:subPropertyOf :hasLowerRank .



###  https://w3id.org/italia/onto/CLV/hasGeometry

:hasGeometry rdf:type owl:ObjectProperty ;
             
             rdfs:label "has geometry"@en ,
                        "ha geometria"@it ;
             
             rdfs:comment "This property links any thing of the domain to the geometry."@en ;
             
             owl:versionInfo "unstable"@en ;
             
             rdfs:comment "Questa proprietà lega una qualsiasi cosa del dominio a una geometria."@it ;
             
             owl:versionInfo "provvisoria"@it ;
             
             rdfs:domain owl:Thing ;
             
             rdfs:isDefinedBy <https://w3id.org/italia/onto/CLV> ;
             
             rdfs:range :Geometry ;
             
             owl:inverseOf :isGeometryFor .



###  https://w3id.org/italia/onto/CLV/hasGeometryType

:hasGeometryType rdf:type owl:FunctionalProperty ,
                          owl:ObjectProperty ;
                 
                 rdfs:label "has geometry type"@en ,
                            "ha tipo geometria"@it ;
                 
                 rdfs:comment "Property that links the Geometry to its Type (Polygon, Point, Line)"@en ;
                 
                 owl:versionInfo "unstable"@en ;
                 
                 rdfs:comment "Proprietà che collega la geometria al suo tipo (poligono, punto, linea)."@it ;
                 
                 owl:versionInfo "provvisoria"@it ;
                 
                 rdfs:isDefinedBy <https://w3id.org/italia/onto/CLV> ;
                 
                 rdfs:domain :Geometry ;
                 
                 rdfs:range :GeometryType .



###  https://w3id.org/italia/onto/CLV/hasHigherRank

:hasHigherRank rdf:type owl:ObjectProperty ;
               
               rdfs:label "has higher rank"@en ,
                          "ha livello più alto"@it ;
               
               rdfs:comment "This property is used to  relate two named address component, where the first is more specific than the second."@en ;
               
               owl:versionInfo "unstable"@en ;
               
               rdfs:comment "Questa proprietà è utilizzata per mettere in relazione due componenti dell'indirizzo che hanno un nome, dove la prima è più specifica della seconda."@it ;
               
               owl:versionInfo "provvisoria"@it ;
               
               rdfs:isDefinedBy <https://w3id.org/italia/onto/CLV> ;
               
               rdfs:domain :NamedAddressComponent ;
               
               rdfs:range :NamedAddressComponent .



###  https://w3id.org/italia/onto/CLV/hasIdentifier

:hasIdentifier rdf:type owl:ObjectProperty ;
               
               rdfs:label "has identifier"@en ,
                          "ha identificativo (ha codice)"@it ;
               
               rdfs:comment "This property allows one to specify an identifier for the spatial object. The property is inherited by all the subclasses of spatial object."@en ;
               
               owl:versionInfo "stable"@en ;
               
               rdfs:comment "Questa proprietà collega un oggetto spaziale alla classe identificativo che rappresenta uno o più identificativi associabili a un oggetto spaziale. La proprietà è ereditata da tutte le sotto classi di oggetto spaziale. Esempi di identificativi sono il codice istat, l'identificativo di un indirizzo secondo le specifiche inspire, il codice assegnato dal comune, ecc."@it ;
               
               owl:versionInfo "stabile"@it ;
               
               rdfs:isDefinedBy <https://w3id.org/italia/onto/CLV> ;
               
               rdfs:range :Identifier ;
               
               rdfs:domain :SpatialObject .



###  https://w3id.org/italia/onto/CLV/hasLowerRank

:hasLowerRank rdf:type owl:ObjectProperty ;
              
              rdfs:label "has lower rank"@en ,
                         "ha livello più basso"@it ;
              
              rdfs:comment "This property relates two named address components, where the first is less specific than the second."@en ;
              
              owl:versionInfo "unstable"@en ;
              
              rdfs:comment "Questa proprietà mette in relazione due componenti dell'indirizzo con nomi, dove la prima è meno specifica della seconda."@it ;
              
              owl:versionInfo "provvisoria"@it ;
              
              rdfs:isDefinedBy <https://w3id.org/italia/onto/CLV> ;
              
              rdfs:range :NamedAddressComponent ;
              
              rdfs:domain :NamedAddressComponent .



###  https://w3id.org/italia/onto/CLV/hasNumber

:hasNumber rdf:type owl:ObjectProperty ;
           
           rdfs:label "has street number"@en ,
                      "ha numero civico"@it ;
           
           rdfs:comment "This property specializes the property has address component in order to link the address with its part representing the street number (or housing number)."@en ;
           
           owl:versionInfo "unstable"@en ;
           
           rdfs:comment "Questa proprietà specializza la proprietà ha componente indirizzo per collegare l'indirizzo al numero civico parte dello stesso."@it ;
           
           owl:versionInfo "provvisoria"@it ;
           
           rdfs:isDefinedBy <https://w3id.org/italia/onto/CLV> ;
           
           rdfs:domain :Address ;
           
           rdfs:range :AddressNumber ;
           
           rdfs:subPropertyOf :hasAddressComponent .



###  https://w3id.org/italia/onto/CLV/hasPrimaryAddress

:hasPrimaryAddress rdf:type owl:FunctionalProperty ,
                            owl:ObjectProperty ;
                   
                   rdfs:label "has primary address"@en ,
                              "ha indirizzo primario"@it ;
                   
                   rdfs:comment "This property is sub-property of has address and it is used to model the specific primary address of a thing of the modelled domain to which it is linked (e.g., the primary address of an organization or a person)."@en ;
                   
                   owl:versionInfo "unstable"@en ;
                   
                   rdfs:comment "Questa proprietà è sotto proprietà di ha indirizzo ed è utilizzata per modellare lo specifico indirizzo primario di una cosa del dominio a cui è collegata (e.g., l'indirizzo primario di un'organizzazione, l'indirizzo primario di una persona, ecc.)."@it ;
                   
                   owl:versionInfo "provvisoria"@it ;
                   
                   rdfs:domain owl:Thing ;
                   
                   rdfs:isDefinedBy <https://w3id.org/italia/onto/CLV> ;
                   
                   rdfs:range :Address ;
                   
                   rdfs:subPropertyOf :hasAddress .



###  https://w3id.org/italia/onto/CLV/hasProvince

:hasProvince rdf:type owl:ObjectProperty ;
             
             rdfs:label "has province"@en ,
                        "ha provincia"@it ;
             
             rdfs:comment "This property specializes the property has address component in order to link the address with the Province, part of the address itself."@en ;
             
             owl:versionInfo "unstable"@en ;
             
             rdfs:comment "Questa proprietà specializza la proprietà ha componente indirizzo per collegare l'indirizzo alla componente Provincia dello stesso. Esempio di Provincia: \"PA\", \"BO\", \"RM\", ecc."@it ;
             
             owl:versionInfo "provvisoria"@it ;
             
             rdfs:isDefinedBy <https://w3id.org/italia/onto/CLV> ;
             
             rdfs:domain :Address ;
             
             rdfs:range :Province ;
             
             rdfs:subPropertyOf :hasAddressComponent .



###  https://w3id.org/italia/onto/CLV/hasRegion

:hasRegion rdf:type owl:ObjectProperty ;
           
           rdfs:label "has region"@en ,
                      "ha regione"@it ;
           
           rdfs:comment "This property specializes the property has address component in order to link the address with the Region of the country that is part of the address."@en ;
           
           owl:versionInfo "unstable"@en ;
           
           rdfs:comment "Questa proprietà specializza la proprietà ha componente indirizzo per collegare l'indirizzo con una sua parte, i.e., la Regione del paese. Regione può essere \"Lazio\", \"Emilia Romagna\", ecc."@it ;
           
           owl:versionInfo "provvisoria"@it ;
           
           rdfs:isDefinedBy <https://w3id.org/italia/onto/CLV> ;
           
           rdfs:domain :Address ;
           
           rdfs:range :Region ;
           
           rdfs:subPropertyOf :hasAddressComponent .



###  https://w3id.org/italia/onto/CLV/hasSpatialCoverage

:hasSpatialCoverage rdf:type owl:ObjectProperty ;
                    
                    rdfs:label "has spatial coverage"@en ,
                               "ha copertura spaziale"@it ;
                    
                    rdfs:comment "Property used to model the spatial coverage of a thing of the modelled domain."@en ;
                    
                    owl:versionInfo "unstable"@en ;
                    
                    rdfs:comment "Proprietà che consente di esprimere la copertura spaziale di una cosa del dominio da modellare."@it ;
                    
                    owl:versionInfo "provvisoria"@it ;
                    
                    rdfs:domain owl:Thing ;
                    
                    rdfs:isDefinedBy <https://w3id.org/italia/onto/CLV> ;
                    
                    rdfs:range :Feature .



###  https://w3id.org/italia/onto/CLV/hasStreet

:hasStreet rdf:type owl:ObjectProperty ;
           
           rdfs:label "has street"@en ,
                      "ha via"@it ;
           
           rdfs:comment "This property specilizes the property has address component in order to link an address to the street component of it."@en ;
           
           owl:versionInfo "unstable"@en ;
           
           rdfs:comment "Questa proprietà specializza la proprietù ha componente indirizzo per collegare l'indirizzo alla componente via dell'indirizzo stesso."@it ;
           
           owl:versionInfo "provvisoria"@it ;
           
           rdfs:isDefinedBy <https://w3id.org/italia/onto/CLV> ;
           
           rdfs:domain :Address ;
           
           rdfs:range :Street ;
           
           rdfs:subPropertyOf :hasAddressComponent .



###  https://w3id.org/italia/onto/CLV/hasValidity

:hasValidity rdf:type owl:ObjectProperty ;
             
             rdfs:label "has validity"@en ,
                        "ha validità"@it ;
             
             rdfs:comment "This property associate a temporal validity to a spatial object."@en ;
             
             owl:versionInfo "unstable"@en ;
             
             rdfs:comment "Questa proprietà associa una validità temporale a un oggetto spaziale."@it ;
             
             owl:versionInfo "provvisoria"@it ;
             
             rdfs:isDefinedBy <https://w3id.org/italia/onto/CLV> ;
             
             rdfs:domain :SpatialObject ;
             
             rdfs:range ti:TimeInterval .



###  https://w3id.org/italia/onto/CLV/isAddressFor

:isAddressFor rdf:type owl:ObjectProperty ;
              
              rdfs:label "is address for"@en ,
                         "è indirizzo per"@it ;
              
              rdfs:comment "Inverse property of has address that links the address to a thing."@en ;
              
              owl:versionInfo "stable"@en ;
              
              rdfs:comment "Proprietà inversa di ha indirizzo che collega l'indirizzo a una cosa del dominio da modellare."@it ;
              
              owl:versionInfo "stabile"@it ;
              
              rdfs:range owl:Thing ;
              
              rdfs:isDefinedBy <https://w3id.org/italia/onto/CLV> ;
              
              rdfs:domain :Address .



###  https://w3id.org/italia/onto/CLV/isGeometryFor

:isGeometryFor rdf:type owl:ObjectProperty ;
               
               rdfs:label "is geometry for"@en ,
                          "è geometria per"@it ;
               
               rdfs:comment "Inverse property of \"hasGeometry\" that links a geometry to the related Thing of the domain."@en ;
               
               owl:versionInfo "unstable"@en ;
               
               rdfs:comment "Proprietà inversa di \"ha geometria\" che lega una geometria a una cosa del dominio modellato."@it ;
               
               owl:versionInfo "provvisoria"@it ;
               
               rdfs:range owl:Thing ;
               
               rdfs:isDefinedBy <https://w3id.org/italia/onto/CLV> ;
               
               rdfs:domain :Geometry .



###  https://w3id.org/italia/onto/CLV/isSpatialCoverageOf

:isSpatialCoverageOf rdf:type owl:ObjectProperty ;
                     
                     rdfs:label "is feature of"@en ,
                                "è caratteristica di"@it ;
                     
                     rdfs:comment "Inverse property of has feature that links a feature to a thing."@en ;
                     
                     owl:versionInfo "stable"@en ;
                     
                     rdfs:comment "Proprietà inversa di ha caratteristica che collega una caratteristica a una cosa."@it ;
                     
                     owl:versionInfo "stabile"@it ;
                     
                     rdfs:range owl:Thing ;
                     
                     rdfs:isDefinedBy <https://w3id.org/italia/onto/CLV> ;
                     
                     rdfs:domain :Feature .



###  https://w3id.org/italia/onto/CLV/issuedBy

:issuedBy rdf:type owl:ObjectProperty ;
          
          rdfs:label "issued by"@en ,
                     "rilasciato da"@it ;
          
          rdfs:comment "This property links an Identifier with the Authority that issued it"@en ;
          
          owl:versionInfo "stable"@en ;
          
          rdfs:comment "Questa proprietà collega un identificato all'autorità che l'ha rilasciato."@it ;
          
          owl:versionInfo "stabile"@it ;
          
          rdfs:isDefinedBy <https://w3id.org/italia/onto/CLV> ;
          
          rdfs:domain :Identifier ;
          
          rdfs:range covapit:Organization .



###  https://w3id.org/italia/onto/CLV/next

:next rdf:type owl:ObjectProperty ;
      
      rdfs:label "next"@en ,
                 "successivo"@it ;
      
      rdfs:comment "This property allows one to model an ordered sequence of addresses (that may be related to each other as a consequence of a change occured over time). In this specific case the property represents the next 'address in time' situation in the sequence."@en ;
      
      owl:versionInfo "unstable"@en ;
      
      rdfs:comment "Questa proprietà consente di modellare una sequenza ordinata di indirizzi (che potrebbero essere relazionati l'uno con l'altro come conseguenza di un cambio nella denominaziona avvenuta nel tempo). Nel caso specifico di questa proprietà, essa rappresenta la situazione 'address in time' successiva nella sequenza."@it ;
      
      owl:versionInfo "provvisoria"@it ;
      
      rdfs:isDefinedBy <https://w3id.org/italia/onto/CLV> ;
      
      rdfs:domain :AddressInTime ;
      
      rdfs:range :AddressInTime .



###  https://w3id.org/italia/onto/CLV/prev

:prev rdf:type owl:ObjectProperty ;
      
      rdfs:label "previous"@en ,
                 "precedente"@it ;
      
      rdfs:comment "This property allows one to model an ordered sequence of addresses (that may be related to each other as a consequence of a change occured over time). In this specific case the property represents the previous 'address in time' situation in the sequence."@en ;
      
      owl:versionInfo "unstable"@en ;
      
