  <rdf:Description rdf:about="http://dati.gov.it/onto/clvapit#acronym">
    <rdfs:subPropertyOf rdf:resource="http://dati.gov.it/onto/l0/name"/>
    <l0:controlledVocabulary rdf:resource="http://dati.gov.it/onto/controlledVocabulary/Territory"/>
    <rdfs:label xml:lang="en">acronym</rdfs:label>
    <rdfs:domain rdf:resource="http://dati.gov.it/onto/clvapit#Province"/>
    <rdfs:comment xml:lang="en">The acronym of the province that is typically used over the Italian territory for the license plate.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/clvapit"/>
    <rdfs:comment xml:lang="it">La sigla automobilistica con cui viene indicata la provincia.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
