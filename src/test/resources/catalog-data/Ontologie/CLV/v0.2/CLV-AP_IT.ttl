###  http://dati.gov.it/onto/clvapit#alt

:alt rdf:type owl:DatatypeProperty ;
     
     rdfs:label "altitude"@en ,
                "quota"@it ;
     
     rdfs:comment "The property represents the altitude, i.e., the height of an object or point in relation to sea level or ground level. It is associated with the geometry."@en ;
     
     owl:versionInfo "stable"@en ;
     
