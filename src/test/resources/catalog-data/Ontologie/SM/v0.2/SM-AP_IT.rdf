  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/smapit/Telephone">
    <rdfs:subClassOf rdf:nodeID="ub153bL607C28"/>
    <rdfs:subClassOf rdf:resource="http://dati.gov.it/onto/l0/Object"/>
    <rdfs:subClassOf rdf:nodeID="ub153bL597C28"/>
    <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/smapit/"/>
    <rdfs:label xml:lang="en">Telephone</rdfs:label>
    <rdfs:label xml:lang="it">Telefono</rdfs:label>
    <rdfs:comment xml:lang="en">This class represents the Telephone that is characterized by a type (Voice, Fax) and a telephone number.</rdfs:comment>
    <prov:wasInfluencedBy rdf:resource="http://www.w3.org/2006/vcard/ns#TelephoneType"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="it">Questa classe rappresenta un telefono che a sua volta è caratterizzato da un tipo (Voce, Fax) e da un numero.</rdfs:comment>
    <rdfs:subClassOf rdf:nodeID="ub153bL602C28"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/smapit/hasImage">
    <rdfs:comment xml:lang="it">Questa proprietà collega qualsiasi cosa ad un'immagine.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:comment xml:lang="en">This property links anything to an image.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
