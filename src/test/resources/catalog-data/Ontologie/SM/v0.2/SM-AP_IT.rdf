    <rdfs:label xml:lang="it">Social media</rdfs:label>
    <rdfs:comment xml:lang="en">This class represents a social media like a social network (e.g. Facebook, Twitter, etc.).</rdfs:comment>
    <rdfs:comment xml:lang="it">Questa  classe rappresenta un social media come, ad esempio, un social network (ex. Facebook, Twitter, ecc...).</rdfs:comment>
    <rdfs:label xml:lang="en">Social media</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://dati.gov.it/onto/l0/Object"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/smapit/issuesAccount">
    <rdfs:domain rdf:resource="http://dati.gov.it/onto/smapit/SocialMedia"/>
    <rdfs:range rdf:resource="http://dati.gov.it/onto/smapit/UserAccount"/>
    <rdfs:comment xml:lang="it">Questa proprietà rappresenta il rilascio di un account utente da parte di un social media.</rdfs:comment>
    <rdfs:comment xml:lang="en">This property means that a Social Media issues a user account.</rdfs:comment>
    <rdfs:label xml:lang="it">rilascia account</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">issues account</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/smapit/"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/smapit/isTelephoneTypeOf">
    <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/smapit/"/>
    <rdfs:label xml:lang="it">è tipo di telefono di</rdfs:label>
    <rdfs:comment xml:lang="it">Proprietà inversa di "ha tipo di telefono".</rdfs:comment>
    <rdfs:label xml:lang="en">is telephone type of</rdfs:label>
    <rdfs:comment xml:lang="en">Inverse property of "has telephone type".</rdfs:comment>
    <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
    <rdfs:domain rdf:resource="http://dati.gov.it/onto/smapit/TelephoneType"/>
    <rdfs:range rdf:resource="http://dati.gov.it/onto/smapit/Telephone"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/smapit/hasEmailType">
    <rdfs:label xml:lang="en">has email type</rdfs:label>
    <rdfs:domain rdf:resource="http://dati.gov.it/onto/smapit/Email"/>
    <rdfs:comment xml:lang="en">This property links the Email to its type.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="it">Questa proprietà collega l'email al suo tipo.</rdfs:comment>
    <owl:inverseOf rdf:resource="http://dati.gov.it/onto/smapit/isEmailTypeOf"/>
    <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/smapit/"/>
    <rdfs:range rdf:resource="http://dati.gov.it/onto/smapit/EmailType"/>
    <rdfs:label xml:lang="it">ha tipo email</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/smapit/OnlineContactPoint">
    <rdfs:subClassOf rdf:nodeID="ub153bL526C37"/>
    <rdfs:subClassOf rdf:nodeID="ub153bL538C37"/>
    <rdfs:label xml:lang="it">Punto di Contatto Online</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Online Contact Point</rdfs:label>
    <prov:wasInfluencedBy rdf:resource="http://schema.org/ContactPoint"/>
    <rdfs:subClassOf rdf:nodeID="ub153bL534C37"/>
    <rdfs:comment xml:lang="it">Questa classe rappresenta un punto di contatto online. Essa raggruppa un insieme di tipi di canali di contatto online come account di social network, email, siti web, ecc.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://dati.gov.it/onto/l0/Object"/>
    <rdfs:subClassOf rdf:nodeID="ub153bL530C37"/>
    <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
    <rdfs:comment xml:lang="en">This class represents an online contact point; that is, an object that groups a set of types of online (social) media channels such as email, social network contacts, web site, ecc.</rdfs:comment>
    <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/smapit"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/smapit/isWebSiteOf">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:comment xml:lang="it">Relazione tra un sito Web e l'entità a cui il sito Web è associato. Questa proprietà è l'inversa di 'ha sito Web'.</rdfs:comment>
    <rdfs:domain rdf:resource="http://dati.gov.it/onto/smapit/WebSite"/>
    <rdfs:label xml:lang="en">is Web site of</rdfs:label>
    <rdfs:comment xml:lang="en">Relation between a Web site and the entity this Web site is associated with. This property is the inverse of 'has Web site'.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/smapit/"/>
    <rdfs:label xml:lang="it">è sito Web di</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:nodeID="ub153bL538C37">
    <owl:allValuesFrom rdf:resource="http://dati.gov.it/onto/smapit/WebSite"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Restriction"/>
    <owl:onProperty rdf:resource="http://dati.gov.it/onto/smapit/hasWebSite"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/smapit/WebSite">
    <rdfs:comment xml:lang="it">Questa classe rappresenta il sito web di riferimento dell'organizzazione.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://dati.gov.it/onto/l0/Object"/>
    <prov:wasInfluencedBy rdf:resource="http://xmlns.com/foaf/0.1/Document"/>
    <rdfs:label xml:lang="it">Sito Web</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/smapit/"/>
    <rdfs:label xml:lang="en">Web site</rdfs:label>
    <rdfs:comment xml:lang="en">This class represents the web site of refernce of the organization.</rdfs:comment>
    <prov:wasInfluencedBy rdf:resource="http://schema.org/WebSite"/>
    <rdfs:subClassOf rdf:nodeID="ub153bL673C26"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/smapit/hasWebSite">
    <rdfs:comment xml:lang="it">Relazione tra una qualsiasi entità e la sua pagina Web.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/smapit/"/>
    <rdfs:label xml:lang="en">has Web site</rdfs:label>
    <rdfs:label xml:lang="it">ha pagina Web</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <owl:inverseOf rdf:resource="http://dati.gov.it/onto/smapit/isWebSiteOf"/>
    <rdfs:range rdf:resource="http://dati.gov.it/onto/smapit/WebSite"/>
    <rdfs:comment xml:lang="en">Relation between anything and its web site.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="ub153bL597C28">
    <owl:onProperty rdf:resource="http://dati.gov.it/onto/smapit/hasTelephoneType"/>
    <owl:onClass rdf:resource="http://dati.gov.it/onto/smapit/TelephoneType"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Restriction"/>
    <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/smapit/hasCreator">
    <owl:inverseOf rdf:resource="http://dati.gov.it/onto/smapit/isCreatorOf"/>
    <rdfs:domain rdf:resource="http://dati.gov.it/onto/smapit/Post"/>
    <rdfs:range rdf:resource="http://dati.gov.it/onto/smapit/UserAccount"/>
    <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/smapit/"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <rdfs:label xml:lang="it">ha autore</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <prov:wasInfluencedBy rdf:resource="http://rdfs.org/sioc/ns"/>
    <rdfs:comment xml:lang="en">This property states that a post has been created by a certain user account.</rdfs:comment>
    <rdfs:comment xml:lang="it">Questa proprietà asserisce che un post è stato creato da un certo account utente.</rdfs:comment>
