    <rdfs:comment xml:lang="en">The name associated with a social media entity.</rdfs:comment>
    <rdfs:label xml:lang="it">nome di social media</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/smapit/"/>
    <rdfs:label xml:lang="en">social media name</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/smapit/Post">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">A post on a social media.</rdfs:comment>
    <rdfs:subClassOf rdf:nodeID="ub241bL557C23"/>
    <rdfs:label xml:lang="it">Post</rdfs:label>
    <rdfs:label xml:lang="en">Post</rdfs:label>
