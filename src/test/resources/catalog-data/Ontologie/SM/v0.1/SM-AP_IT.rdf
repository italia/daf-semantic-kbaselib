    <rdfs:comment xml:lang="it">Questa proprietà collega un agente al suo account utente.</rdfs:comment>
    <rdfs:comment xml:lang="en">This property links an agent to its user account.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/smapit/"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/smapit/socialMediaName">
