>
  <rdf:Description rdf:about="http://dati.gov.it/onto/smapit/isUserAccountOf">
    <rdfs:label xml:lang="it">è account utente di</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/smapit/"/>
    <owl:inverseOf rdf:resource="http://dati.gov.it/onto/smapit/hasUserAccount"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">is user account of</rdfs:label>
    <rdfs:comment xml:lang="it">Questa proprietà collega un account utente al suo proprietario, che è un istanza di agente. E' la proprietà inversa di 'ha account utente'.</rdfs:comment>
    <rdfs:comment xml:lang="en">This property links a user account to its owner, which, in turn, is an agent. This property is the inverse of 'has user account'.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/prov#wasInfluencedBy">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/smapit/Email">
    <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/smapit/"/>
    <rdfs:subClassOf rdf:resource="http://dati.gov.it/onto/l0/Object"/>
    <rdfs:subClassOf rdf:nodeID="ub241bL496C24"/>
    <rdfs:label xml:lang="it">Email</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">Email</rdfs:label>
    <rdfs:comment xml:lang="it">Questa classe rappresenta l'indirizzo email del punto di contatto di un'organizzazione. La classe è collegata al tipo di email. Nell'ambito itailano si identificano due tipi principali di email, la PEC e l'email tradizionale.</rdfs:comment>
    <rdfs:comment xml:lang="en">This class represents the Email address of the contact point of an organization. The class is linked to the type of email that can be specified. In the Italian context we identify two types of Emails; namely, Certified Electronic Email and Traditional Email addresses.</rdfs:comment>
    <prov:wasInfluencedBy rdf:resource="http://www.w3.org/2006/vcard/ns#Email"/>
    <rdfs:subClassOf rdf:nodeID="ub241bL491C24"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/smapit/userAccountName">
    <rdfs:domain rdf:resource="http://dati.gov.it/onto/smapit/UserAccount"/>
    <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/smapit/"/>
    <rdfs:isDefinedBy xml:lang="it">nome dell'account utente</rdfs:isDefinedBy>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:isDefinedBy xml:lang="it">Questa proprietà collega un account utente al suo nome (un letterale).</rdfs:isDefinedBy>
    <rdfs:isDefinedBy xml:lang="en">user account name</rdfs:isDefinedBy>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:isDefinedBy xml:lang="en">This property links a user account to its name (i.e. a literal).</rdfs:isDefinedBy>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/smapit/UserAccount">
    <rdfs:subClassOf rdf:nodeID="ub241bL609C30"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="it">Questa classe rappresenta un account come quello che può possedere un utente su un social network.</rdfs:comment>
    <rdfs:label xml:lang="it">Account utente</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/smapit/"/>
    <rdfs:subClassOf rdf:nodeID="ub241bL604C30"/>
    <rdfs:comment xml:lang="en">This class represents a user account, such as the account owned by a person on a social network.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://dati.gov.it/onto/l0/Object"/>
    <prov:wasInfluencedBy rdf:resource="http://rdfs.org/sioc/ns#"/>
    <rdfs:label xml:lang="en">User account</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/smapit/hasEmail">
    <rdfs:label xml:lang="it">ha email</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="en">This property links an agent with one or more email addresses of various types.</rdfs:comment>
    <rdfs:comment xml:lang="it">Questa proprietà collega un agente con uno o più indirizzi email di vario tipo.</rdfs:comment>
    <rdfs:range rdf:resource="http://dati.gov.it/onto/smapit/Email"/>
    <rdfs:comment xml:lang="en">This property links an email with an agent.</rdfs:comment>
    <rdfs:comment xml:lang="it">Questa proprietà collega un'email con un agente.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/smapit/"/>
    <rdfs:domain rdf:resource="http://dati.gov.it/onto/l0/Agent"/>
    <rdfs:label xml:lang="en">has email</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/smapit/hasEmailType">
    <rdfs:range rdf:resource="http://dati.gov.it/onto/smapit/EmailType"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="it">Questa proprietà collega l'email al suo tipo.</rdfs:comment>
    <rdfs:label xml:lang="it">ha tipo email</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/smapit/"/>
    <rdfs:domain rdf:resource="http://dati.gov.it/onto/smapit/Email"/>
    <rdfs:label xml:lang="en">has email type</rdfs:label>
    <rdfs:comment xml:lang="en">This property links the Email to its type.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/smapit/isAccountIssuedBy">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="it">è account rilasciato da</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/smapit/"/>
    <rdfs:label xml:lang="en">is accunt issued by</rdfs:label>
    <rdfs:comment xml:lang="en">This property represents the relation between a social media and the user account issued by such a social media.</rdfs:comment>
    <rdfs:range rdf:resource="http://dati.gov.it/onto/smapit/SocialMedia"/>
    <rdfs:comment xml:lang="it">Questa proprietà rappresenta la relazione tra un social media e gli account utenti che sono stati rilasciati su tale media.</rdfs:comment>
    <rdfs:domain rdf:resource="http://dati.gov.it/onto/smapit/UserAccount"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/smapit/isReplyOf">
    <rdfs:comment xml:lang="it">La proprietà 'è risposta di' permette di asserire che un certo post è la risposta ad un altro post.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/smapit/"/>
    <prov:wasInfluencedBy rdf:resource="http://rdfs.org/sioc/ns#reply_of"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">is reply of</rdfs:label>
    <rdfs:label xml:lang="it">è risposta di</rdfs:label>
    <rdfs:comment xml:lang="en">The property 'is reply of' asserts that a given post is the answer to another post.</rdfs:comment>
    <owl:inverseOf rdf:resource="http://dati.gov.it/onto/smapit/hasReply"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/smapit/hasCreator">
    <rdfs:range rdf:resource="http://dati.gov.it/onto/smapit/UserAccount"/>
    <prov:wasInfluencedBy rdf:resource="http://rdfs.org/sioc/ns"/>
    <rdfs:label xml:lang="it">ha autore</rdfs:label>
    <rdfs:label xml:lang="en">has creator</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="en">This property states that a post has been created by a certain user account.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/smapit/"/>
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <rdfs:comment xml:lang="it">Questa proprietà asserisce che un post è stato creato da un certo account utente.</rdfs:comment>
    <rdfs:domain rdf:resource="http://dati.gov.it/onto/smapit/Post"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://dati.gov.it/onto/smapit/hasUserAccount">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    <rdfs:label xml:lang="it">ha account utente</rdfs:label>
    <rdfs:label xml:lang="en">has user account</rdfs:label>
