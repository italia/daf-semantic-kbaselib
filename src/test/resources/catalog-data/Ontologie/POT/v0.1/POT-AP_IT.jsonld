    {
      "@id": "http://dati.gov.it/onto/potapit/Card",
      "@type": "owl:Class",
      "dc:description": {
        "@language": "en",
        "@value": "Un esempio di utilizzo della classe cis:Card  è il seguente:\n\n<pre>\n\ncard:Parco_archeologico_di_Liternum_Card\n    a cis:Card ;\n    rdfs:label \"Ingresso per due giorni al Parco archeologico di Liternum\"@it ; \n    rdfs:label \"Two-day access to Parco archeologico di Liternum\"@en ;\n    cis:name \"Ingresso per due giorni al Parco archeologico di Liternum\"@it ; \n    cis:name \"Two-day access to the Parco archeologico di Liternum\"@en ;\n    cis:hasValidity Parco_archeologico_di_Liternum_Card_Validity ;\n    cis:forAccessTo cissite:Parco_archeologico_di_Liternum ;\n    cis:description \"With this card a tourist can visit Parco archeologico di Liternum for 2 consecutive days\"@en ; \n    cis:description \"La card permette l'accesso al Parco archeologico di Liternum per 2 giorni consecutivi\"@it .\n\nvalidity:Parco_archeologico_di_Liternum_Card_Validity\n    a cis:Validity ;\n    rdfs:label \"Two-day validity\"@en ;\n    rdfs:label \"Validità per due giorni\"@it ;\n    cis:hasDuration Parco_archeologico_di_Liternum_Card_Validity_Duration .\n \nquantitativevalue:Parco_archeologico_di_Liternum_Card_Validity_Duration\n    a cis:QuantitativeValue ;\n    rdfs:label \"2\" ;\n    cis:hasMeasurementUnit cis:Day ;\n    cis:hasValue \"2\"^^xsd:positiveInteger .\n\ncis:Day a cis:MeasurementUnit .\n\n</pre>"
      },
      "rdfs:comment": [
        {
          "@language": "it",
          "@value": "Questa classe rappresenta una carta d'ingresso che dà diritto a più ingressi in più luoghi della cultura. E' sottoclasse di ticket."
        },
        {
          "@language": "en",
          "@value": "This class represents a card valid for the entrance to more than one Cultural Institute or Site. It is a subclass of Ticket."
        }
      ],
      "rdfs:isDefinedBy": {
        "@id": "http://dati.gov.it/onto/potapit"
      },
      "rdfs:label": [
        {
          "@language": "en",
          "@value": "Card"
        },
        {
          "@language": "it",
          "@value": "Carta d'ingresso"
        }
      ],
      "rdfs:subClassOf": {
        "@id": "http://dati.gov.it/onto/potapit/Ticket"
      }
    },
