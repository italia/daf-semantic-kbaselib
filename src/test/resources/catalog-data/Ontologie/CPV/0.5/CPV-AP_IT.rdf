<rdf:RDF xmlns="https://w3id.org/italia/onto/CPV/"
     xml:base="https://w3id.org/italia/onto/CPV/"
     xmlns:l0="https://w3id.org/italia/onto/l0/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:clv="https://w3id.org/italia/onto/CLV/"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:ti="https://w3id.org/italia/onto/TI/"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:dcterms="http://purl.org/dc/terms/"
     xmlns:prov="http://www.w3.org/ns/prov#"
     xmlns:dc="http://purl.org/dc/elements/1.1/">
    <owl:Ontology rdf:about="https://w3id.org/italia/onto/CPV">
        <rdfs:label xml:lang="en">Person ontology</rdfs:label>
        <rdfs:label xml:lang="it">Ontologia delle persone (individui)</rdfs:label>
        <dcterms:issued rdf:datatype="&xsd;date">2017-07-19</dcterms:issued>
        <dcterms:modified>2018-03-03</dcterms:modified>
        <dc:creator xml:lang="en">Agency for Digital Italy - AgID</dc:creator>
        <dc:creator xml:lang="en">Institute of Cognitive Sciences and Technologies of the Italian Research Council (CNR) - Semantic Technology Laboratory (STLab)</dc:creator>
        <dc:creator xml:lang="en">Italian Digital Transformation Team</dc:creator>
        <dc:creator xml:lang="en">Italian Institute of Statistics - ISTAT</dc:creator>
        <rdfs:comment xml:lang="en">This is the ontology of the Italian application profile for Persons</rdfs:comment>
        <owl:versionInfo xml:lang="en">Version 0.5 - 03  March 2018 - use of w3id.org/italia as base URI</owl:versionInfo>
        <dc:creator xml:lang="it">Agenzia per l&apos;italia Digitale (AgID)</dc:creator>
        <dc:creator xml:lang="it">Istituto Nazionale di Statistica - ISTAT</dc:creator>
        <dc:creator xml:lang="it">Istituto di Scienze e Tecnologie della Cognizione dell&apos;Istituto nazionale delle Ricerche (CNR) - Laboratorio di Tecnologie Semantiche (STlab)</dc:creator>
        <rdfs:comment xml:lang="it">Questa è l&apos;ontologia del profilo applicativo italiano sulle persone.</rdfs:comment>
        <dc:creator xml:lang="it">Team per la Trasformazione Digitale</dc:creator>
        <owl:versionInfo xml:lang="it">Versione 0.5 - 03 Marzo 2018 - uso di w3id.org/italia come base URI</owl:versionInfo>
        <prov:wasDerivedFrom rdf:resource="http://dati.gov.it/onto/cpvapit/0.4/"/>
        <dcterms:license rdf:resource="https://creativecommons.org/licenses/by/4.0/"/>
        <owl:imports rdf:resource="https://w3id.org/italia/onto/CLV"/>
        <owl:imports rdf:resource="https://w3id.org/italia/onto/TI"/>
        <owl:imports rdf:resource="https://w3id.org/italia/onto/l0"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/CPV/"/>
        <owl:versionIRI rdf:resource="https://w3id.org/italia/onto/CPV/0.5/"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://w3id.org/italia/onto/l0/controlledVocabulary -->

    <owl:AnnotationProperty rdf:about="&l0;controlledVocabulary"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2001/XMLSchema#date -->

    <rdfs:Datatype rdf:about="&xsd;date"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://w3id.org/italia/onto/CPV/belongsToFamily -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/CPV/belongsToFamily">
        <rdfs:label xml:lang="en">belongs to family</rdfs:label>
        <rdfs:label xml:lang="it">appartiene a famiglia</rdfs:label>
        <rdfs:comment xml:lang="en">The relationship that links a person who is resident in a country to a family</rdfs:comment>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <rdfs:comment xml:lang="it">La relazione che lega una persona residente in un paese a una famiglia</rdfs:comment>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/CPV/"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/CPV/Family"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/CPV/Resident"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/italia/onto/CPV/hasBirthPlace -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/CPV/hasBirthPlace">
        <rdfs:label xml:lang="en">birth place</rdfs:label>
        <rdfs:label xml:lang="it">luogo di nascita</rdfs:label>
        <rdfs:comment xml:lang="en">The relation asserts that a person has a certain place of birth.</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">La relazione che permette di asserire che una persona è nata in un certo luogo.</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/CPV/"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/CPV/Person"/>
        <owl:inverseOf rdf:resource="https://w3id.org/italia/onto/CPV/isBirthPlaceOf"/>
        <rdfs:range rdf:resource="&l0;Location"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/italia/onto/CPV/hasCitizenship -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/CPV/hasCitizenship">
        <rdfs:label xml:lang="en">citizenship</rdfs:label>
        <rdfs:label xml:lang="it">ha cittadinanza</rdfs:label>
        <rdfs:comment xml:lang="en">The relation that links a person to its citizenship (e.g. Italian)</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">La relazione che lega una persona alla sua cittadinanza (ex. cittadinanza italiana).</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/CPV/"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/CPV/Person"/>
        <rdfs:range rdf:resource="&l0;Location"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/italia/onto/CPV/hasCurrentResidence -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/CPV/hasCurrentResidence">
        <rdfs:label xml:lang="en">has current residence</rdfs:label>
        <rdfs:label xml:lang="it">ha residenza attuale</rdfs:label>
        <rdfs:comment xml:lang="en">The relation that links a person who is resident in a country to his/her location of residence. It is the current residence of the person.</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Relazione che lega una persona residente in un paese al proprio luogo di residenza Rappresenta la residenza corrente della persona</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:range rdf:resource="&clv;Address"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/CPV/"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/CPV/Resident"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/italia/onto/CPV/hasDeathPlace -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/CPV/hasDeathPlace">
        <rdfs:label xml:lang="en">death place</rdfs:label>
        <rdfs:label xml:lang="it">luogo di morte</rdfs:label>
        <rdfs:comment xml:lang="en">The relation that links a person who is dead to its place of death.</rdfs:comment>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <rdfs:comment xml:lang="it">La relazione che lega una persona morta al luogo di morte.</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/CPV/"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/CPV/Dead"/>
        <owl:inverseOf rdf:resource="https://w3id.org/italia/onto/CPV/isDeathPlaceOf"/>
        <rdfs:range rdf:resource="&l0;Location"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/italia/onto/CPV/hasGender -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/CPV/hasGender">
        <rdfs:label xml:lang="en">has gender</rdfs:label>
        <rdfs:label xml:lang="it">ha sesso</rdfs:label>
        <rdfs:comment xml:lang="en">The relation that links a person to his/her sex</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">La relazione che lega una persona al suo sesso</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/CPV/"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/CPV/Gender"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/CPV/Person"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/italia/onto/CPV/hasLevelOfEducation -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/CPV/hasLevelOfEducation">
        <rdfs:label xml:lang="en">has level of education</rdfs:label>
        <rdfs:label xml:lang="it">ha grado di istruzione</rdfs:label>
        <rdfs:comment xml:lang="en">The relation that links an alive person to his/her education level</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">La relazione che lega una persona in vita al suo grado di istruzione</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/CPV/"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/CPV/Alive"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/CPV/EducationLevel"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/italia/onto/CPV/hasParentalRelationshipWith -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/CPV/hasParentalRelationshipWith">
        <rdfs:label xml:lang="en">has parental relationship with</rdfs:label>
        <rdfs:label xml:lang="it">ha relazione parentale con</rdfs:label>
        <rdfs:comment xml:lang="en">A parental relationship between two persons.</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Una relazione di parentela tra due persone.</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/CPV/"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/CPV/Person"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/CPV/Person"/>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/italia/onto/CPV/hasRelationshipWith"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/italia/onto/CPV/hasRelationshipWith -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/CPV/hasRelationshipWith">
        <rdfs:label xml:lang="en">has relationship with</rdfs:label>
        <rdfs:label xml:lang="it">ha relazione con</rdfs:label>
        <rdfs:comment xml:lang="en">Any relationship between people.</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Qualsiasi relazione tra persone.</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/CPV/"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/CPV/Person"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/CPV/Person"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/italia/onto/CPV/hasResidenceInTime -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/CPV/hasResidenceInTime">
        <rdfs:label xml:lang="en">has residence in time</rdfs:label>
        <rdfs:label xml:lang="it">ha stato residenza (ha residenza nel tempo)</rdfs:label>
        <rdfs:comment xml:lang="en">Property that links a person to his/her residence in time</rdfs:comment>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Proprietà che lega una persona allo storico della sua residenza (lega quindi la persona alla relazione ennaria residenza nel tempo)</rdfs:comment>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/CPV/"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/CPV/Person"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/CPV/ResidenceInTime"/>
        <owl:inverseOf rdf:resource="https://w3id.org/italia/onto/CPV/isResidenceInTimeOf"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/italia/onto/CPV/inRegisteredFamily -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/CPV/inRegisteredFamily">
        <rdfs:label xml:lang="en">in registered family</rdfs:label>
        <rdfs:label xml:lang="it">in famiglia anagrafica</rdfs:label>
        <rdfs:comment xml:lang="en">The relation that links the different types of Parental Relationship to the related Registered Family</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Relazione che lega un tipo di relazione parentale con la relativa famiglia anagrafica</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/CPV/"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/CPV/ParentalRelationship"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/CPV/RegisteredFamily"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/italia/onto/CPV/isBirthPlaceOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/CPV/isBirthPlaceOf">
        <rdfs:label xml:lang="en">is birth place of</rdfs:label>
        <rdfs:label xml:lang="it">è luogo di nascita di</rdfs:label>
        <rdfs:comment xml:lang="en">The relation between a place and a person. The relation asserts that a place is the place of birth of the person object of the relation.</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">La relazione tra un luogo ed una persona dove il luogo è il luogo di nascit della persona oggetto della relazione.</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/CPV/"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/CPV/Person"/>
        <rdfs:domain rdf:resource="&l0;Location"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/italia/onto/CPV/isChildOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/CPV/isChildOf">
        <rdfs:label xml:lang="en">child of</rdfs:label>
        <rdfs:label xml:lang="it">figlio di</rdfs:label>
        <rdfs:comment xml:lang="en">The relation child-parent (e.g. John is the son of Peter).</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">La relazione figlio-padre (ex. Mario è figlio di Carlo).</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/CPV/"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/CPV/Person"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/CPV/Person"/>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/italia/onto/CPV/hasParentalRelationshipWith"/>
        <owl:inverseOf rdf:resource="https://w3id.org/italia/onto/CPV/isParentOf"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/italia/onto/CPV/isConsortOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/CPV/isConsortOf">
        <rdfs:label xml:lang="en">is consort of</rdfs:label>
        <rdfs:label xml:lang="it">è coniuge di</rdfs:label>
        <rdfs:comment xml:lang="en">The marriage relation between persons</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">La relazione di matrimonio tra due persone.</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/CPV/"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/CPV/Person"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/CPV/Person"/>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/italia/onto/CPV/hasParentalRelationshipWith"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/italia/onto/CPV/isCurrentResidenceOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/CPV/isCurrentResidenceOf">
        <rdfs:label xml:lang="en">is current residence of</rdfs:label>
        <rdfs:label xml:lang="it">è residenza attuale di</rdfs:label>
        <rdfs:comment xml:lang="en">The inverse relation that links a person who is resident in a country to his/her location of residence. It is the current residence of the person.</rdfs:comment>
        <rdfs:comment xml:lang="it">Relazione inversa che lega una persona residente in un paese al proprio luogo di residenza Rappresenta la residenza corrente della persona</rdfs:comment>
        <rdfs:domain rdf:resource="&clv;Address"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/CPV/Resident"/>
        <owl:inverseOf rdf:resource="https://w3id.org/italia/onto/CPV/hasCurrentResidence"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/italia/onto/CPV/isDeathPlaceOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/CPV/isDeathPlaceOf">
        <rdfs:label xml:lang="en">death place of</rdfs:label>
        <rdfs:label xml:lang="it">è luogo di morte di</rdfs:label>
        <rdfs:comment xml:lang="en">The property to link the death place (i.e. a location) to a person</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">La proprietà per associare un luogo di morte (i.e. una località) alla persona morta.</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/CPV/"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/CPV/Dead"/>
        <rdfs:domain rdf:resource="&l0;Location"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/italia/onto/CPV/isGrandChildOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/CPV/isGrandChildOf">
        <rdfs:label xml:lang="en">is granchild of</rdfs:label>
        <rdfs:label xml:lang="it">è nipote lato nonni di</rdfs:label>
        <rdfs:comment xml:lang="en">It represents the parental relationship grandchild-grandfather</rdfs:comment>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Rappresenta la relazione di parentela nipote-nonno</rdfs:comment>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/CPV/"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/CPV/Person"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/CPV/Person"/>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/italia/onto/CPV/hasParentalRelationshipWith"/>
        <owl:inverseOf rdf:resource="https://w3id.org/italia/onto/CPV/isGrandFatherOf"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/italia/onto/CPV/isGrandFatherOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/CPV/isGrandFatherOf">
        <rdfs:label xml:lang="en">is grandfather of</rdfs:label>
        <rdfs:label xml:lang="it">è nonno di</rdfs:label>
        <rdfs:comment xml:lang="en">The grandfathership relation</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">La relazione parentale tra nonno-nipote</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/CPV/"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/CPV/Person"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/CPV/Person"/>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/italia/onto/CPV/hasParentalRelationshipWith"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/italia/onto/CPV/isNephewOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/CPV/isNephewOf">
        <rdfs:label xml:lang="en">is nephew of</rdfs:label>
        <rdfs:label xml:lang="it">è nipote lato zii di</rdfs:label>
        <rdfs:comment xml:lang="en">The nephew relation between a newphew and an uncle.</rdfs:comment>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <rdfs:comment xml:lang="it">La relazione di parentela tra un nipote ed uno zio.</rdfs:comment>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
