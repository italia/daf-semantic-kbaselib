                    rdfs:domain :EducationLevel ;
                    
                    rdfs:subPropertyOf l0:description .



###  https://w3id.org/italia/onto/CPV/educationLevelID

:educationLevelID rdf:type owl:DatatypeProperty ,
                           owl:FunctionalProperty ;
                  
                  rdfs:label "education level identifier"@en ,
                             "codice grado di istruzione"@it ;
                  
                  rdfs:comment "Unique code used to identify the education level in the controlled vocabulary EducationLevel http://dati.gov.it/onto/controlledvocabulary/EducationLevel"@en ;
                  
                  owl:versionInfo "stable"@en ;
                  
