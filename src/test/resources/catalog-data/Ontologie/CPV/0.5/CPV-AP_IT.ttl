###  https://w3id.org/italia/onto/CPV/belongsToFamily

:belongsToFamily rdf:type owl:ObjectProperty ;
                 
                 rdfs:label "belongs to family"@en ,
                            "appartiene a famiglia"@it ;
                 
                 rdfs:comment "The relationship that links a person who is resident in a country to a family"@en ;
                 
                 owl:versionInfo "unstable"@en ;
                 
                 rdfs:comment "La relazione che lega una persona residente in un paese a una famiglia"@it ;
                 
                 owl:versionInfo "provvisoria"@it ;
                 
                 rdfs:isDefinedBy : ;
                 
                 rdfs:range :Family ;
                 
                 rdfs:domain :Resident .



###  https://w3id.org/italia/onto/CPV/hasBirthPlace

:hasBirthPlace rdf:type owl:ObjectProperty ;
               
               rdfs:label "birth place"@en ,
                          "luogo di nascita"@it ;
               
               rdfs:comment "The relation asserts that a person has a certain place of birth."@en ;
               
               owl:versionInfo "stable"@en ;
               
               rdfs:comment "La relazione che permette di asserire che una persona è nata in un certo luogo."@it ;
               
               owl:versionInfo "stabile"@it ;
               
               rdfs:isDefinedBy : ;
               
               rdfs:domain :Person ;
               
               owl:inverseOf :isBirthPlaceOf ;
               
               rdfs:range l0:Location .



###  https://w3id.org/italia/onto/CPV/hasCitizenship

:hasCitizenship rdf:type owl:ObjectProperty ;
                
                rdfs:label "citizenship"@en ,
                           "ha cittadinanza"@it ;
                
                rdfs:comment "The relation that links a person to its citizenship (e.g. Italian)"@en ;
                
                owl:versionInfo "stable"@en ;
                
                rdfs:comment "La relazione che lega una persona alla sua cittadinanza (ex. cittadinanza italiana)."@it ;
                
                owl:versionInfo "stabile"@it ;
                
                rdfs:isDefinedBy : ;
                
                rdfs:domain :Person ;
                
                rdfs:range l0:Location .



###  https://w3id.org/italia/onto/CPV/hasCurrentResidence

:hasCurrentResidence rdf:type owl:ObjectProperty ;
                     
                     rdfs:label "has current residence"@en ,
                                "ha residenza attuale"@it ;
                     
                     rdfs:comment "The relation that links a person who is resident in a country to his/her location of residence. It is the current residence of the person."@en ;
                     
                     owl:versionInfo "stable"@en ;
                     
                     rdfs:comment "Relazione che lega una persona residente in un paese al proprio luogo di residenza Rappresenta la residenza corrente della persona"@it ;
                     
                     owl:versionInfo "stabile"@it ;
                     
                     rdfs:range clv:Address ;
                     
                     rdfs:isDefinedBy : ;
                     
                     rdfs:domain :Resident .



###  https://w3id.org/italia/onto/CPV/hasDeathPlace

:hasDeathPlace rdf:type owl:ObjectProperty ;
               
               rdfs:label "death place"@en ,
                          "luogo di morte"@it ;
               
               rdfs:comment "The relation that links a person who is dead to its place of death."@en ;
               
               owl:versionInfo "unstable"@en ;
               
               rdfs:comment "La relazione che lega una persona morta al luogo di morte."@it ;
               
               owl:versionInfo "stabile"@it ;
               
               rdfs:isDefinedBy : ;
               
               rdfs:domain :Dead ;
               
               owl:inverseOf :isDeathPlaceOf ;
               
               rdfs:range l0:Location .



###  https://w3id.org/italia/onto/CPV/hasGender

:hasGender rdf:type owl:ObjectProperty ;
           
           rdfs:label "has gender"@en ,
                      "ha sesso"@it ;
           
           rdfs:comment "The relation that links a person to his/her sex"@en ;
           
           owl:versionInfo "stable"@en ;
           
           rdfs:comment "La relazione che lega una persona al suo sesso"@it ;
           
           owl:versionInfo "stabile"@it ;
           
           rdfs:isDefinedBy : ;
           
           rdfs:range :Gender ;
           
           rdfs:domain :Person .



###  https://w3id.org/italia/onto/CPV/hasLevelOfEducation

:hasLevelOfEducation rdf:type owl:ObjectProperty ;
                     
                     rdfs:label "has level of education"@en ,
                                "ha grado di istruzione"@it ;
                     
                     rdfs:comment "The relation that links an alive person to his/her education level"@en ;
                     
                     owl:versionInfo "stable"@en ;
                     
                     rdfs:comment "La relazione che lega una persona in vita al suo grado di istruzione"@it ;
                     
                     owl:versionInfo "stabile"@it ;
                     
                     rdfs:isDefinedBy : ;
                     
                     rdfs:domain :Alive ;
                     
                     rdfs:range :EducationLevel .



###  https://w3id.org/italia/onto/CPV/hasParentalRelationshipWith

:hasParentalRelationshipWith rdf:type owl:ObjectProperty ;
                             
                             rdfs:label "has parental relationship with"@en ,
                                        "ha relazione parentale con"@it ;
                             
                             rdfs:comment "A parental relationship between two persons."@en ;
                             
                             owl:versionInfo "stable"@en ;
                             
                             rdfs:comment "Una relazione di parentela tra due persone."@it ;
                             
                             owl:versionInfo "stabile"@it ;
                             
                             rdfs:isDefinedBy : ;
                             
                             rdfs:domain :Person ;
                             
                             rdfs:range :Person ;
                             
                             rdfs:subPropertyOf :hasRelationshipWith .



###  https://w3id.org/italia/onto/CPV/hasRelationshipWith

:hasRelationshipWith rdf:type owl:ObjectProperty ;
                     
                     rdfs:label "has relationship with"@en ,
                                "ha relazione con"@it ;
                     
                     rdfs:comment "Any relationship between people."@en ;
                     
                     owl:versionInfo "stable"@en ;
                     
                     rdfs:comment "Qualsiasi relazione tra persone."@it ;
                     
                     owl:versionInfo "stabile"@it ;
                     
                     rdfs:isDefinedBy : ;
                     
                     rdfs:range :Person ;
                     
                     rdfs:domain :Person .



###  https://w3id.org/italia/onto/CPV/hasResidenceInTime

:hasResidenceInTime rdf:type owl:ObjectProperty ;
                    
                    rdfs:label "has residence in time"@en ,
                               "ha stato residenza (ha residenza nel tempo)"@it ;
                    
                    rdfs:comment "Property that links a person to his/her residence in time"@en ;
                    
                    owl:versionInfo "unstable"@en ;
                    
                    rdfs:comment "Proprietà che lega una persona allo storico della sua residenza (lega quindi la persona alla relazione ennaria residenza nel tempo)"@it ;
                    
                    owl:versionInfo "provvisoria"@it ;
                    
                    rdfs:isDefinedBy : ;
                    
                    rdfs:domain :Person ;
                    
                    rdfs:range :ResidenceInTime ;
                    
                    owl:inverseOf :isResidenceInTimeOf .



###  https://w3id.org/italia/onto/CPV/inRegisteredFamily

:inRegisteredFamily rdf:type owl:ObjectProperty ;
                    
                    rdfs:label "in registered family"@en ,
                               "in famiglia anagrafica"@it ;
                    
                    rdfs:comment "The relation that links the different types of Parental Relationship to the related Registered Family"@en ;
                    
                    owl:versionInfo "stable"@en ;
                    
                    rdfs:comment "Relazione che lega un tipo di relazione parentale con la relativa famiglia anagrafica"@it ;
                    
                    owl:versionInfo "stabile"@it ;
                    
                    rdfs:isDefinedBy : ;
                    
                    rdfs:domain :ParentalRelationship ;
                    
                    rdfs:range :RegisteredFamily .



###  https://w3id.org/italia/onto/CPV/isBirthPlaceOf

:isBirthPlaceOf rdf:type owl:ObjectProperty ;
                
                rdfs:label "is birth place of"@en ,
                           "è luogo di nascita di"@it ;
                
                rdfs:comment "The relation between a place and a person. The relation asserts that a place is the place of birth of the person object of the relation."@en ;
                
                owl:versionInfo "stable"@en ;
                
                rdfs:comment "La relazione tra un luogo ed una persona dove il luogo è il luogo di nascit della persona oggetto della relazione."@it ;
                
                owl:versionInfo "stabile"@it ;
                
                rdfs:isDefinedBy : ;
                
                rdfs:range :Person ;
                
                rdfs:domain l0:Location .



###  https://w3id.org/italia/onto/CPV/isChildOf

:isChildOf rdf:type owl:ObjectProperty ;
           
           rdfs:label "child of"@en ,
                      "figlio di"@it ;
           
           rdfs:comment "The relation child-parent (e.g. John is the son of Peter)."@en ;
           
           owl:versionInfo "stable"@en ;
           
           rdfs:comment "La relazione figlio-padre (ex. Mario è figlio di Carlo)."@it ;
           
           owl:versionInfo "stabile"@it ;
           
           rdfs:isDefinedBy : ;
           
           rdfs:domain :Person ;
           
           rdfs:range :Person ;
           
           rdfs:subPropertyOf :hasParentalRelationshipWith ;
           
           owl:inverseOf :isParentOf .



###  https://w3id.org/italia/onto/CPV/isConsortOf

:isConsortOf rdf:type owl:ObjectProperty ;
             
             rdfs:label "is consort of"@en ,
                        "è coniuge di"@it ;
             
             rdfs:comment "The marriage relation between persons"@en ;
             
             owl:versionInfo "stable"@en ;
             
             rdfs:comment "La relazione di matrimonio tra due persone."@it ;
             
             owl:versionInfo "stabile"@it ;
             
             rdfs:isDefinedBy : ;
             
             rdfs:domain :Person ;
             
             rdfs:range :Person ;
             
             rdfs:subPropertyOf :hasParentalRelationshipWith .



###  https://w3id.org/italia/onto/CPV/isCurrentResidenceOf

:isCurrentResidenceOf rdf:type owl:ObjectProperty ;
                      
                      rdfs:label "is current residence of"@en ,
                                 "è residenza attuale di"@it ;
                      
                      rdfs:comment "The inverse relation that links a person who is resident in a country to his/her location of residence. It is the current residence of the person."@en ,
                                   "Relazione inversa che lega una persona residente in un paese al proprio luogo di residenza Rappresenta la residenza corrente della persona"@it ;
                      
                      rdfs:domain clv:Address ;
                      
                      rdfs:range :Resident ;
                      
                      owl:inverseOf :hasCurrentResidence .



###  https://w3id.org/italia/onto/CPV/isDeathPlaceOf

:isDeathPlaceOf rdf:type owl:ObjectProperty ;
                
                rdfs:label "death place of"@en ,
                           "è luogo di morte di"@it ;
                
                rdfs:comment "The property to link the death place (i.e. a location) to a person"@en ;
                
                owl:versionInfo "stable"@en ;
                
                rdfs:comment "La proprietà per associare un luogo di morte (i.e. una località) alla persona morta."@it ;
                
                owl:versionInfo "stabile"@it ;
                
                rdfs:isDefinedBy : ;
                
                rdfs:range :Dead ;
                
                rdfs:domain l0:Location .



###  https://w3id.org/italia/onto/CPV/isGrandChildOf

:isGrandChildOf rdf:type owl:ObjectProperty ;
                
                rdfs:label "is granchild of"@en ,
                           "è nipote lato nonni di"@it ;
                
                rdfs:comment "It represents the parental relationship grandchild-grandfather"@en ;
                
                owl:versionInfo "unstable"@en ;
                
                rdfs:comment "Rappresenta la relazione di parentela nipote-nonno"@it ;
                
                owl:versionInfo "provvisoria"@it ;
                
                rdfs:isDefinedBy : ;
                
                rdfs:domain :Person ;
                
                rdfs:range :Person ;
                
                rdfs:subPropertyOf :hasParentalRelationshipWith ;
                
                owl:inverseOf :isGrandFatherOf .



###  https://w3id.org/italia/onto/CPV/isGrandFatherOf

:isGrandFatherOf rdf:type owl:ObjectProperty ;
                 
                 rdfs:label "is grandfather of"@en ,
                            "è nonno di"@it ;
                 
                 rdfs:comment "The grandfathership relation"@en ;
                 
                 owl:versionInfo "stable"@en ;
                 
                 rdfs:comment "La relazione parentale tra nonno-nipote"@it ;
                 
                 owl:versionInfo "stabile"@it ;
                 
                 rdfs:isDefinedBy : ;
                 
                 rdfs:range :Person ;
                 
                 rdfs:domain :Person ;
                 
                 rdfs:subPropertyOf :hasParentalRelationshipWith .



###  https://w3id.org/italia/onto/CPV/isNephewOf

:isNephewOf rdf:type owl:ObjectProperty ;
            
            rdfs:label "is nephew of"@en ,
                       "è nipote lato zii di"@it ;
            
            rdfs:comment "The nephew relation between a newphew and an uncle."@en ;
            
            owl:versionInfo "unstable"@en ;
            
            rdfs:comment "La relazione di parentela tra un nipote ed uno zio."@it ;
            
            owl:versionInfo "provvisoria"@it ;
            
            rdfs:isDefinedBy : ;
            
            rdfs:range :Person ;
            
            rdfs:domain :Person ;
            
            rdfs:subPropertyOf :hasParentalRelationshipWith ;
            
            owl:inverseOf :isUncleOf .



###  https://w3id.org/italia/onto/CPV/isParentOf

:isParentOf rdf:type owl:ObjectProperty ;
            
            rdfs:label "is parent of"@en ,
                       "è genitore di"@it ;
            
            rdfs:comment "The parent-child relationship."@en ;
            
            owl:versionInfo "stable"@en ;
            
            rdfs:comment "La relazione padre-figlio."@it ;
            
            owl:versionInfo "stabile"@it ;
            
            rdfs:isDefinedBy : ;
            
            rdfs:domain :Person ;
            
            rdfs:range :Person ;
            
            rdfs:subPropertyOf :hasParentalRelationshipWith .



###  https://w3id.org/italia/onto/CPV/isResidenceInTimeOf

:isResidenceInTimeOf rdf:type owl:ObjectProperty ;
                     
                     rdfs:label "is residence in time of"@en ,
                                "è stato residenza di (è residenza nel tempo di)"@it ;
                     
                     rdfs:comment "Inverse property of \"has residence in time\" which links residence in time with the person"@en ;
                     
                     owl:versionInfo "unstable"@en ;
                     
                     rdfs:comment "Proprietà inversa di \"ha residenza nel tempo (ha stato residenza)\" che collega il concetto di residenza nel tempo con la persona."@it ;
                     
                     owl:versionInfo "provvisoria"@it ;
                     
                     rdfs:isDefinedBy : ;
                     
                     rdfs:range :Person ;
                     
                     rdfs:domain :ResidenceInTime .



###  https://w3id.org/italia/onto/CPV/isResidenceOf

:isResidenceOf rdf:type owl:ObjectProperty ;
               
               owl:inverseOf :residentIn .



###  https://w3id.org/italia/onto/CPV/isUncleOf

:isUncleOf rdf:type owl:ObjectProperty ;
           
           rdfs:label "is uncle of"@en ,
                      "è zio di"@it ;
           
           rdfs:comment "The uncleship relation."@en ;
           
           owl:versionInfo "unstable"@en ;
           
           rdfs:comment "La relazione di parentela tra zio e nipote."@it ;
           
           owl:versionInfo "provvisoria"@it ;
           
           rdfs:isDefinedBy : ;
           
           rdfs:range :Person ;
           
           rdfs:domain :Person ;
           
           rdfs:subPropertyOf :hasParentalRelationshipWith .



###  https://w3id.org/italia/onto/CPV/knows

:knows rdf:type owl:ObjectProperty ,
                owl:SymmetricProperty ;
       
       rdfs:label "knows"@en ,
                  "conosce"@it ;
       
       rdfs:comment "The relationship of knowledge between two people. E.g. 'John White' knows 'Bob Black'."@en ;
       
       owl:versionInfo "stable"@en ;
       
       rdfs:comment "La relazione di conoscenza che intercorre tra due persone. Ex. 'Mario Rossi' conosce 'Paolo Neri'."@it ;
       
       owl:versionInfo "stabile"@it ;
       
       rdfs:isDefinedBy : ;
       
       rdfs:range :Person ;
       
       rdfs:domain :Person ;
       
       rdfs:subPropertyOf :hasRelationshipWith .



###  https://w3id.org/italia/onto/CPV/residenceInLocation

:residenceInLocation rdf:type owl:ObjectProperty ;
                     
                     rdfs:label "residence in location"@en ,
                                "residenza in luogo"@it ;
                     
                     rdfs:comment "Property that links the residence in time (the n-ray relationship) to the location of the residence."@en ;
                     
                     owl:versionInfo "unstable"@en ;
                     
                     rdfs:comment "Proprietà che lega la residenza nel tempo (lo storico della residenza attraverso la definizione della relazione ennaria) al luogo della residenza."@it ;
                     
                     owl:versionInfo "provvisoria"@it ;
                     
                     rdfs:isDefinedBy : ;
                     
                     rdfs:domain :ResidenceInTime ;
                     
                     rdfs:range l0:Location .



###  https://w3id.org/italia/onto/CPV/residentIn

:residentIn rdf:type owl:ObjectProperty ;
            
            rdfs:label "resident in"@en ,
                       "residente in"@it ;
            
            rdfs:comment "This property represents the residence expressed as binary relation between a person and an address."@en ,
                         "Questa proprietà rappresenta la residenza espressa come relazione binaria tra una persona ed un indirizzo."@it ;
            
            rdfs:range clv:Address ;
            
            rdfs:domain :Person ;
            
            owl:propertyChainAxiom ( :hasResidenceInTime
                                     :residenceInLocation
                                   ) .





