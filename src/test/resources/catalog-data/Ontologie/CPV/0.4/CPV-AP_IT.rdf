                <owl:onClass rdf:resource="http://dati.gov.it/onto/l0/Location"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://dati.gov.it/onto/cpvapit/isResidenceInTimeOf"/>
