    <!-- http://dati.gov.it/onto/cpvapit/hasResidenceInTime -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/cpvapit/hasResidenceInTime">
        <owl:inverseOf rdf:resource="http://dati.gov.it/onto/cpvapit/isResidenceInTimeOf"/>
