    <!-- http://dati.gov.it/onto/cpvapit/belongsToFamily -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/cpvapit/belongsToFamily">
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/cpvapit/Resident"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/cpvapit/Family"/>
        <rdfs:comment xml:lang="it">La relazione che lega una persona residente in un paese a una famiglia</rdfs:comment>
        <rdfs:comment xml:lang="en">The relationship that links a person who is resident in a country to a family</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/cpvapit/"/>
        <rdfs:label xml:lang="it">appartiene a famiglia</rdfs:label>
        <rdfs:label xml:lang="en">belongs to family</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>



    <!-- http://dati.gov.it/onto/cpvapit/hasBirthPlace -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/cpvapit/hasBirthPlace">
        <owl:inverseOf rdf:resource="http://dati.gov.it/onto/cpvapit/isBirthPlaceOf"/>
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/cpvapit/Person"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/l0/Location"/>
        <rdfs:comment xml:lang="it">La relazione che permette di asserire che una persona è nata in un certo luogo.</rdfs:comment>
        <rdfs:comment xml:lang="en">The relation asserts that a person has a certain place of birth.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/cpvapit/"/>
        <rdfs:label xml:lang="en">birth place</rdfs:label>
        <rdfs:label xml:lang="it">luogo di nascita</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:ObjectProperty>



    <!-- http://dati.gov.it/onto/cpvapit/hasCitizenship -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/cpvapit/hasCitizenship">
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/cpvapit/Person"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/l0/Location"/>
        <rdfs:comment xml:lang="it">La relazione che lega una persona alla sua cittadinanza (ex. cittadinanza italiana).</rdfs:comment>
        <rdfs:comment xml:lang="en">The relation that links a person to its citizenship (e.g. Italian)</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/cpvapit/"/>
        <rdfs:label xml:lang="en">citizenship</rdfs:label>
        <rdfs:label xml:lang="it">ha cittadinanza</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:ObjectProperty>



    <!-- http://dati.gov.it/onto/cpvapit/hasCurrentResidence -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/cpvapit/hasCurrentResidence">
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/cpvapit/Resident"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/l0/Location"/>
        <rdfs:comment xml:lang="it">Relazione che lega una persona residente in un paese al proprio luogo di residenza Rappresenta la residenza corrente della persona</rdfs:comment>
        <rdfs:comment xml:lang="en">The relation that links a person who is resident in a country to his/her location of residence. It is the current residence of the person.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/cpvapit/"/>
        <rdfs:label xml:lang="it">ha residenza attuale</rdfs:label>
        <rdfs:label xml:lang="en">has current residence</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:ObjectProperty>



    <!-- http://dati.gov.it/onto/cpvapit/hasDeathPlace -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/cpvapit/hasDeathPlace">
        <owl:inverseOf rdf:resource="http://dati.gov.it/onto/cpvapit/isDeathPlaceOf"/>
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/cpvapit/Dead"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/l0/Location"/>
        <rdfs:comment xml:lang="it">La relazione che lega una persona morta al luogo di morte.</rdfs:comment>
        <rdfs:comment xml:lang="en">The relation that links a person who is dead to its place of death.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/cpvapit/"/>
        <rdfs:label xml:lang="en">death place</rdfs:label>
        <rdfs:label xml:lang="it">luogo di morte</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>



    <!-- http://dati.gov.it/onto/cpvapit/hasGender -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/cpvapit/hasGender">
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/cpvapit/Person"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/cpvapit/Gender"/>
        <rdfs:comment xml:lang="it">La relazione che lega una persona al suo sesso</rdfs:comment>
        <rdfs:comment xml:lang="en">The relation that links a person to his/her sex</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/cpvapit/"/>
        <rdfs:label xml:lang="it">ha sesso</rdfs:label>
        <rdfs:label xml:lang="en">has gender</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:ObjectProperty>



    <!-- http://dati.gov.it/onto/cpvapit/hasLevelOfEducation -->

