    <!-- http://dati.gov.it/onto/cpvapit/Alive -->

    <owl:Class rdf:about="http://dati.gov.it/onto/cpvapit/Alive">
        <rdfs:subClassOf rdf:resource="http://dati.gov.it/onto/cpvapit/Person"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://dati.gov.it/onto/cpvapit/hasLevelOfEducation"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="http://dati.gov.it/onto/cpvapit/EducationLevel"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://dati.gov.it/onto/cpvapit/age"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://dati.gov.it/onto/cpvapit/Dead"/>
