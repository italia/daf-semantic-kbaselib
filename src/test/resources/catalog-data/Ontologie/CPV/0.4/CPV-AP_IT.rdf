        <rdfs:comment xml:lang="en">The relation that links an alive person to his/her education level</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/cpvapit/"/>
        <rdfs:label xml:lang="it">ha grado di istruzione</rdfs:label>
