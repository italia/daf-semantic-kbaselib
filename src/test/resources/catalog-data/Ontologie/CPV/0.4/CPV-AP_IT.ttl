###  http://dati.gov.it/onto/cpvapit/belongsToFamily
:belongsToFamily rdf:type owl:ObjectProperty ;
                 rdfs:domain :Resident ;
                 rdfs:range :Family ;
                 rdfs:comment "La relazione che lega una persona residente in un paese a una famiglia"@it ,
                              "The relationship that links a person who is resident in a country to a family"@en ;
                 rdfs:isDefinedBy : ;
                 rdfs:label "appartiene a famiglia"@it ,
                            "belongs to family"@en ;
                 owl:versionInfo "provvisoria"@it ,
                                 "unstable"@en .


###  http://dati.gov.it/onto/cpvapit/hasBirthPlace
:hasBirthPlace rdf:type owl:ObjectProperty ;
               owl:inverseOf :isBirthPlaceOf ;
               rdfs:domain :Person ;
               rdfs:range l0:Location ;
               rdfs:comment "La relazione che permette di asserire che una persona è nata in un certo luogo."@it ,
                            "The relation asserts that a person has a certain place of birth."@en ;
               rdfs:isDefinedBy : ;
               rdfs:label "birth place"@en ,
                          "luogo di nascita"@it ;
               owl:versionInfo "stabile"@it ,
                               "stable"@en .


###  http://dati.gov.it/onto/cpvapit/hasCitizenship
:hasCitizenship rdf:type owl:ObjectProperty ;
                rdfs:domain :Person ;
                rdfs:range l0:Location ;
                rdfs:comment "La relazione che lega una persona alla sua cittadinanza (ex. cittadinanza italiana)."@it ,
                             "The relation that links a person to its citizenship (e.g. Italian)"@en ;
                rdfs:isDefinedBy : ;
                rdfs:label "citizenship"@en ,
                           "ha cittadinanza"@it ;
                owl:versionInfo "stabile"@it ,
                                "stable"@en .


###  http://dati.gov.it/onto/cpvapit/hasCurrentResidence
:hasCurrentResidence rdf:type owl:ObjectProperty ;
                     rdfs:domain :Resident ;
                     rdfs:range l0:Location ;
                     rdfs:comment "Relazione che lega una persona residente in un paese al proprio luogo di residenza Rappresenta la residenza corrente della persona"@it ,
                                  "The relation that links a person who is resident in a country to his/her location of residence. It is the current residence of the person."@en ;
                     rdfs:isDefinedBy : ;
                     rdfs:label "ha residenza attuale"@it ,
                                "has current residence"@en ;
                     owl:versionInfo "stabile"@it ,
                                     "stable"@en .


###  http://dati.gov.it/onto/cpvapit/hasDeathPlace
:hasDeathPlace rdf:type owl:ObjectProperty ;
               owl:inverseOf :isDeathPlaceOf ;
               rdfs:domain :Dead ;
               rdfs:range l0:Location ;
               rdfs:comment "La relazione che lega una persona morta al luogo di morte."@it ,
                            "The relation that links a person who is dead to its place of death."@en ;
               rdfs:isDefinedBy : ;
               rdfs:label "death place"@en ,
                          "luogo di morte"@it ;
               owl:versionInfo "stabile"@it ,
                               "unstable"@en .


###  http://dati.gov.it/onto/cpvapit/hasGender
:hasGender rdf:type owl:ObjectProperty ;
           rdfs:domain :Person ;
           rdfs:range :Gender ;
           rdfs:comment "La relazione che lega una persona al suo sesso"@it ,
                        "The relation that links a person to his/her sex"@en ;
           rdfs:isDefinedBy : ;
           rdfs:label "ha sesso"@it ,
                      "has gender"@en ;
           owl:versionInfo "stabile"@it ,
                           "stable"@en .


###  http://dati.gov.it/onto/cpvapit/hasLevelOfEducation
:hasLevelOfEducation rdf:type owl:ObjectProperty ;
                     rdfs:domain :Alive ;
                     rdfs:range :EducationLevel ;
                     rdfs:comment "La relazione che lega una persona in vita al suo grado di istruzione"@it ,
                                  "The relation that links an alive person to his/her education level"@en ;
                     rdfs:isDefinedBy : ;
                     rdfs:label "ha grado di istruzione"@it ,
                                "has level of education"@en ;
                     owl:versionInfo "stabile"@it ,
                                     "stable"@en .


###  http://dati.gov.it/onto/cpvapit/hasParentalRelationshipWith
:hasParentalRelationshipWith rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf :hasRelationshipWith ;
                             rdfs:domain :Person ;
                             rdfs:range :Person ;
                             rdfs:comment "A parental relationship between two persons."@en ,
                                          "Una relazione di parentela tra due persone."@it ;
                             rdfs:isDefinedBy : ;
                             rdfs:label "ha relazione parentale con"@it ,
                                        "has parental relationship with"@en ;
                             owl:versionInfo "stabile"@it ,
                                             "stable"@en .


###  http://dati.gov.it/onto/cpvapit/hasRelationshipWith
:hasRelationshipWith rdf:type owl:ObjectProperty ;
                     rdfs:domain :Person ;
                     rdfs:range :Person ;
                     rdfs:comment "Any relationship between people."@en ,
                                  "Qualsiasi relazione tra persone."@it ;
                     rdfs:isDefinedBy : ;
                     rdfs:label "ha relazione con"@it ,
                                "has relationship with"@en ;
                     owl:versionInfo "stabile"@it ,
                                     "stable"@en .


###  http://dati.gov.it/onto/cpvapit/hasResidenceInTime
:hasResidenceInTime rdf:type owl:ObjectProperty ;
                    owl:inverseOf :isResidenceInTimeOf ;
                    rdfs:domain :Person ;
                    rdfs:range :ResidenceInTime ;
                    rdfs:comment "Property that links a person to his/her residence in time"@en ,
                                 "Proprietà che lega una persona allo storico della sua residenza (lega quindi la persona alla relazione ennaria residenza nel tempo)"@it ;
                    rdfs:isDefinedBy : ;
                    rdfs:label "ha stato residenza (ha residenza nel tempo)"@it ,
                               "has residence in time"@en ;
                    owl:versionInfo "provvisoria"@it ,
                                    "unstable"@en .


###  http://dati.gov.it/onto/cpvapit/inRegisteredFamily
:inRegisteredFamily rdf:type owl:ObjectProperty ;
                    rdfs:domain :ParentalRelationship ;
                    rdfs:range :RegisteredFamily ;
                    rdfs:comment "Relazione che lega un tipo di relazione parentale con la relativa famiglia anagrafica"@it ,
                                 "The relation that links the different types of Parental Relationship to the related Registered Family"@en ;
                    rdfs:isDefinedBy : ;
                    rdfs:label "in famiglia anagrafica"@it ,
                               "in registered family"@en ;
                    owl:versionInfo "stabile"@it ,
                                    "stable"@en .


###  http://dati.gov.it/onto/cpvapit/isBirthPlaceOf
:isBirthPlaceOf rdf:type owl:ObjectProperty ;
                rdfs:domain l0:Location ;
                rdfs:range :Person ;
                rdfs:comment "La relazione tra un luogo ed una persona dove il luogo è il luogo di nascit della persona oggetto della relazione."@it ,
                             "The relation between a place and a person. The relation asserts that a place is the place of birth of the person object of the relation."@en ;
                rdfs:isDefinedBy : ;
                rdfs:label "is birth place of"@en ,
                           "è luogo di nascita di"@it ;
                owl:versionInfo "stabile"@it ,
                                "stable"@en .


