                                               rdfs:isDefinedBy clvapit: .



###  https://w3id.org/italia/onto/IoT/consistsOf

:consistsOf rdf:type owl:ObjectProperty ;
            
            rdfs:label "consists of"@en ,
                       "consiste di"@it ;
            
            rdfs:comment "Property that links a collection of observations with its observations."@en ;
            
            owl:versionInfo "stable"@en ;
            
            rdfs:comment "Proprietà che collega una collezione di osservazioni con le sue osservazioni."@it ;
            
            owl:versionInfo "stabile"@it ;
            
            rdfs:isDefinedBy <https://w3id.org/italia/onto/IoT> ;
            
            rdfs:range :Observation ;
            
            rdfs:domain :ObservationCollection ;
            
            owl:inverseOf :isPartOf .



###  https://w3id.org/italia/onto/IoT/generationTime

:generationTime rdf:type owl:ObjectProperty ;
                
                rdfs:label "generation time"@en ,
                           "tempo di generazione"@it ;
                
                rdfs:comment "This property is a subproperty of atTime and defines the generation time of the observation."@en ;
                
                owl:versionInfo "stable"@en ;
                
                rdfs:comment "Questa proprietà è sottoproprietà di atTime e consente di modellare il tempo di generazione della osservazione."@it ;
                
                owl:versionInfo "stabile"@it ;
                
                rdfs:isDefinedBy <https://w3id.org/italia/onto/IoT> ;
                
                rdfs:domain :Observation ;
                
                rdfs:range tiapit:TimeInterval ;
                
                rdfs:subPropertyOf tiapit:atTime .



###  https://w3id.org/italia/onto/IoT/hasFeatureOfInterest

:hasFeatureOfInterest rdf:type owl:ObjectProperty ;
                      
                      rdfs:label "has feature of interest"@en ,
                                 "ha caratteristica di interesse"@it ;
                      
                      rdfs:comment "Property that links the Observation to a feature of interest."@en ;
                      
                      owl:versionInfo "stable"@en ;
                      
                      rdfs:comment "Proprietà che lega un'osservazione a una caratteristica di interesse."@it ;
                      
                      owl:versionInfo "stabile"@it ;
                      
                      rdfs:isDefinedBy <https://w3id.org/italia/onto/IoT> ;
                      
                      rdfs:range :FeatureOfInterest ;
                      
                      rdfs:domain :Observation ;
                      
                      owl:inverseOf :isFeatureOfInterestFor .



###  https://w3id.org/italia/onto/IoT/hasMeasurementQualityValue

:hasMeasurementQualityValue rdf:type owl:ObjectProperty ;
                            
                            rdfs:label "has measurement quality value"@en ,
                                       "ha valore di qualità di misurazione"@it ;
                            
                            rdfs:comment "Property that links the Measrurement Quality with its value."@en ;
                            
                            owl:versionInfo "stable"@en ;
                            
                            rdfs:comment "Proprietà che collega la Misurazione di qualità con il relativo valore."@it ;
                            
                            owl:versionInfo "stabile"@it ;
                            
                            rdfs:isDefinedBy <https://w3id.org/italia/onto/IoT> ;
                            
                            rdfs:domain :MeasurementQuality ;
                            
                            rdfs:range :MeasurementQualityValue ;
                            
                            owl:inverseOf :isMeasurementQualityValueOf .



###  https://w3id.org/italia/onto/IoT/hasObservationParameter

:hasObservationParameter rdf:type owl:ObjectProperty ;
                         
                         rdfs:label "has observation parameter"@en ,
                                    "ha parametro dell'osservazione"@it ;
                         
                         rdfs:comment "Property that links an observation to its parameter being observed."@en ;
                         
                         owl:versionInfo "stable"@en ;
                         
                         rdfs:comment "Proprietà che collega un'osservazione al suo parametro osservato."@it ;
                         
                         owl:versionInfo "stabile"@it ;
                         
                         rdfs:isDefinedBy <https://w3id.org/italia/onto/IoT> ;
                         
                         rdfs:domain :Observation ;
                         
                         rdfs:range :ObservationParameter ;
                         
                         owl:inverseOf :isObservationParameterFor .



###  https://w3id.org/italia/onto/IoT/hasObservationValue

:hasObservationValue rdf:type owl:ObjectProperty ;
                     
                     rdfs:label "has observation value"@en ,
                                "ha valore dell'osservazione"@it ;
                     
                     rdfs:comment "Property that links an Observation to its value."@en ;
                     
                     owl:versionInfo "stable"@en ;
                     
                     rdfs:comment "Proprietà che lega un'osservazione al suo valore."@it ;
                     
                     owl:versionInfo "stabile"@it ;
                     
                     rdfs:isDefinedBy <https://w3id.org/italia/onto/IoT> ;
                     
                     rdfs:domain :Observation ;
                     
                     rdfs:range :ObservationValue ;
                     
                     owl:inverseOf :isObservationValueOf .



###  https://w3id.org/italia/onto/IoT/hasQuality

:hasQuality rdf:type owl:ObjectProperty ;
            
            rdfs:label "has quality"@en ,
                       "ha qualità"@it ;
            
            rdfs:comment "Property that links an Observation to a number of measurement capabilities."@en ;
            
            owl:versionInfo "stable"@en ;
            
            rdfs:comment "Proprietà che lega un'osservazione a una serie di parametri di qualità."@it ;
            
            owl:versionInfo "stabile"@it ;
            
            rdfs:isDefinedBy <https://w3id.org/italia/onto/IoT> ;
            
            rdfs:range :MeasurementQuality ;
            
            rdfs:domain :Observation .



###  https://w3id.org/italia/onto/IoT/hasValueForObservationParameter

:hasValueForObservationParameter rdf:type owl:ObjectProperty ;
                                 
                                 rdfs:label "This property links the Observation Parameter to the Observation Value."@en ,
                                            "has value for observation parameter"@en ,
                                            "Questa proprietà connette l'attributo (parametro) dell'osservazione a rispettivo valore dell'osservazione."@it ,
                                            "ha valore per parametro di osservazione"@it ;
                                 
                                 owl:versionInfo "stable"@en ,
                                                 "stabile"@it ;
                                 
                                 rdfs:isDefinedBy <https://w3id.org/italia/onto/IoT> ;
                                 
                                 rdfs:domain :ObservationParameter ;
                                 
                                 rdfs:range :ObservationValue ;
                                 
                                 owl:inverseOf :isValueForObservationParameter .



###  https://w3id.org/italia/onto/IoT/isFeatureOfInterestFor

:isFeatureOfInterestFor rdf:type owl:ObjectProperty ;
                        
                        rdfs:label "is feature of interest for"@en ,
                                   "è caratteristica di interesse per"@it ;
                        
                        rdfs:comment "Inverse property of has feature of interest that links a feature of interest to an observation."@en ;
                        
                        owl:versionInfo "stable"@en ;
                        
                        rdfs:comment "Proprietà inversa di ha caratteristica di interesse che lega una caratteristica di interesse a un'osservaizione."@it ;
                        
                        owl:versionInfo "stabile"@it ;
                        
                        rdfs:isDefinedBy <https://w3id.org/italia/onto/IoT> ;
                        
                        rdfs:domain :FeatureOfInterest ;
                        
                        rdfs:range :Observation .



###  https://w3id.org/italia/onto/IoT/isMeasurementQualityValueOf

:isMeasurementQualityValueOf rdf:type owl:ObjectProperty ;
                             
                             rdfs:label "is measurement quality value of"@en ,
                                        "è valore di qualità di misurazione di"@it ;
                             
                             rdfs:comment "Inverse property of has measurement quality value that links the measurement quality value to the measurment quality."@en ;
                             
                             owl:versionInfo "stable"@en ;
                             
                             rdfs:comment "Proprietà inversa che lega un valore della qualità di misurazione con la qualità di misurazione."@it ;
                             
                             owl:versionInfo "stabile"@it ;
                             
                             rdfs:isDefinedBy <https://w3id.org/italia/onto/IoT> ;
                             
                             rdfs:range :MeasurementQuality ;
                             
                             rdfs:domain :MeasurementQualityValue .



###  https://w3id.org/italia/onto/IoT/isObservationParameterFor

:isObservationParameterFor rdf:type owl:ObjectProperty ;
                           
                           rdfs:label "is observation parameter for"@en ,
                                      "è parametro dell'osservazione per"@it ;
                           
                           rdfs:comment "Inverse property of hasObservationParameter that links a parameter to its related observation."@en ;
                           
                           owl:versionInfo "stable"@en ;
                           
                           rdfs:comment "Proprietà inversa di ha attributo osservato che lega un attributo osservato alla sua relativa osservazione."@it ;
                           
                           owl:versionInfo "stabile"@it ;
                           
                           rdfs:isDefinedBy <https://w3id.org/italia/onto/IoT> ;
                           
                           rdfs:range :Observation ;
                           
                           rdfs:domain :ObservationParameter .



###  https://w3id.org/italia/onto/IoT/isObservationValueOf

:isObservationValueOf rdf:type owl:ObjectProperty ;
                      
