###  https://w3id.org/italia/onto/CLV/Geometry

clvapit:Geometry rdf:type owl:Class ;
                 
                 rdfs:isDefinedBy clvapit: .



###  https://w3id.org/italia/onto/IoT/Accuracy

:Accuracy rdf:type owl:Class ;
          
          rdfs:label "Accuracy"@en ,
                     "Accuratezza"@it ;
          
          rdfs:subClassOf :MeasurementQuality ;
          
          rdfs:comment "It represents the Accuracy; that is, the maximimum difference between the indicated value and the actual measurand being monitored, whilst taking into account all possible sources of error that are relevant to the application."@en ;
          
          owl:versionInfo "unstable"@en ;
          
          rdfs:comment "Questa classe rappresenta l'Accuratezza, cioè la differenza massima tra il valore indicato e il valore effettivo monitorato, tenuto conto di tutte le possibili fonti di errore rilevanti per l'applicazione."@it ;
          
          owl:versionInfo "instabile"@it ;
          
          rdfs:isDefinedBy <https://w3id.org/italia/onto/IoT> .



###  https://w3id.org/italia/onto/IoT/Actuator

:Actuator rdf:type owl:Class ;
          
          rdfs:label "Actuator"@en ,
                     "Attuatore"@it ;
          
          rdfs:subClassOf l0:Object ;
          
          rdfs:comment "An actuator."@en ,
                       "La classe che rappresenta un attuatore."@it .



###  https://w3id.org/italia/onto/IoT/FeatureOfInterest

:FeatureOfInterest rdf:type owl:Class ;
                   
                   rdfs:label "Feature of Interest"@en ,
                              "Caratteristica di interesse"@it ;
                   
                   rdfs:subClassOf l0:Characteristic ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty l0:name ;
                                     owl:someValuesFrom rdfs:Literal
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :isFeatureOfInterestFor ;
                                     owl:allValuesFrom :Observation
                                   ] ;
                   
                   rdfs:comment "It represents the Feature whose Observed Attribute is being observed by a Sensor in order to get an Observed Value. For instance, if we say that the temperature of the kitchen is 28 celsius degrees, the Feature of Interest is the kitchen."@en ;
                   
                   owl:versionInfo "stable"@en ;
                   
                   rdfs:comment "Rappresenta la caratteristica di interesse il cui attributo osservato è osservato da un sensor per ottenere un valore osservato. Per esempio, se diciamo che la temperatura della cucina è di 28 gradi celsius, la caratteristica di interesse è la cucina."@it ;
                   
                   owl:versionInfo "stabile"@it ;
                   
                   rdfs:isDefinedBy <https://w3id.org/italia/onto/IoT> .



###  https://w3id.org/italia/onto/IoT/Frequency

:Frequency rdf:type owl:Class ;
           
           rdfs:label "Frequency"@en ,
                      "Frequenza"@en ;
           
           rdfs:subClassOf :MeasurementQuality ;
           
           rdfs:comment "It represents the Frequency; that is, the smallest possible time between one observation and the next, under the defined conditions."@en ;
           
           owl:versionInfo "unstable"@en ;
           
           rdfs:comment "Questa classe rappresenta la frequenza ovvero il più piccolo tempo possibile tra un'osservazione e un'altra, in determinate condizioni."@it ;
           
           owl:versionInfo "instabile"@it ;
           
           rdfs:isDefinedBy <https://w3id.org/italia/onto/IoT> .



###  https://w3id.org/italia/onto/IoT/Latency

:Latency rdf:type owl:Class ;
         
         rdfs:label "Latency"@en ,
                    "Latenza"@it ;
         
         rdfs:subClassOf :MeasurementQuality ;
         
         rdfs:comment "It represents the Latency; i.e., the time delay between the cause and the effect of some change in the system being observed"@en ;
         
         owl:versionInfo "unstable"@en ;
         
         rdfs:comment "Questa classe rappresenta la Latenza, ovvero il ritardo di tempo tra la causa e l'effetto di qualche cambiamento nel sistema osservarto."@it ;
         
         owl:versionInfo "instabile"@it ;
         
         rdfs:isDefinedBy <https://w3id.org/italia/onto/IoT> .



###  https://w3id.org/italia/onto/IoT/MeasurementQuality

:MeasurementQuality rdf:type owl:Class ;
                    
                    rdfs:label "Measurement Quality"@en ,
                               "Qualità di Misurazione"@it ;
                    
                    rdfs:subClassOf l0:Characteristic ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :hasMeasurementQualityValue ;
                                      owl:onClass :MeasurementQualityValue ;
                                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger
                                    ] ;
                    
                    rdfs:comment "This class represents quality parameters or measurement capabilities/qualities of an observation. Examples include Latency, Accuracy, Repetability, etc."@en ;
                    
                    owl:versionInfo "unstable"@en ;
                    
                    rdfs:comment "Questa classe rappresenta dei parametri di qualità o capacità di misurazione dell'osservazione. Esempi di parametri sono: Latenza, Accuratezza, Ripetibilità, ecc."@it ;
                    
                    owl:versionInfo "instabile"@it ;
                    
                    rdfs:isDefinedBy <https://w3id.org/italia/onto/IoT> .



###  https://w3id.org/italia/onto/IoT/MeasurementQualityValue

