      "owl:equivalentClass": {
        "@id": "d0:Location"
      }
    },
    {
      "@id": "l0:description",
      "rdfs:subPropertyOf": {
        "@id": "http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasParameterDataValue"
      }
    },
    {
      "@id": "l0:Agent",
      "owl:equivalentClass": {
        "@id": "http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent"
      }
    },
    {
      "@id": "http://dati.gov.it/onto/l0-aligns/",
      "@type": "owl:Ontology",
      "owl:imports": [
        {
          "@id": "http://www.ontologydesignpatterns.org/ont/d0.owl"
        },
        {
          "@id": "l0:"
        }
      ],
      "owl:versionIRI": {
        "@id": "http://dati.gov.it/onto/l0-aligns/0.3"
      }
    },
    {
      "@id": "l0:hasDescription",
      "owl:equivalentProperty": {
        "@id": "http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isDescribedBy"
      }
    },
    {
      "@id": "l0:Collection",
      "owl:equivalentClass": {
        "@id": "http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Collection"
      }
    },
    {
      "@id": "l0:Characteristic",
      "owl:equivalentClass": {
        "@id": "d0:Characteristic"
      }
    },
    {
      "@id": "l0:Event",
      "owl:equivalentClass": {
        "@id": "d0:Event"
      }
    },
    {
      "@id": "l0:Activity",
      "owl:equivalentClass": {
        "@id": "d0:Activity"
      }
    },
    {
      "@id": "l0:isDescriptionOf",
      "owl:equivalentProperty": {
        "@id": "http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#describes"
      }
    },
    {
      "@id": "l0:Entity",
      "owl:equivalentClass": {
        "@id": "http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity"
      }
    },
    {
      "@id": "l0:name",
      "rdfs:subPropertyOf": {
        "@id": "http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasParameterDataValue"
      }
    },
    {
      "@id": "l0:identifier",
      "rdfs:subPropertyOf": {
        "@id": "http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasParameterDataValue"
      }
    },
    {
      "@id": "l0:Object",
      "owl:equivalentClass": {
        "@id": "http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Object"
      }
    },
    {
      "@id": "l0:Description",
      "owl:equivalentClass": {
        "@id": "http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Description"
      }
    },
    {
      "@id": "l0:Topic",
      "owl:equivalentClass": {
        "@id": "d0:Topic"
      }
    }
  ]
