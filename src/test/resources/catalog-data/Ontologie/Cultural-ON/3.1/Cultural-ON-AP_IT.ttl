            rdfs:range :CISType ;
            rdfs:comment "La proprietà è utilizzata per collegare un istituto e luogo della cultura alla sua tipologia."@it ,
                         "The property is used to link a cultural institure or site to its type."@en ;
            rdfs:isDefinedBy <http://dati.beniculturali.it/cis/> ;
            rdfs:label "ha tipo"@it ,
