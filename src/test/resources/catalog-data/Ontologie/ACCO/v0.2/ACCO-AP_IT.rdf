     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:potapit="http://dati.gov.it/onto/potapit/"
     xmlns:covapit="http://dati.gov.it/onto/covapit#"
     xmlns:muapit="http://dati.gov.it/onto/muapit/"
     xmlns:prov="http://www.w3.org/ns/prov#"
     xmlns:poiapit="http://dati.gov.it/onto/poiapit#"
