<?xml version="1.0"?>
<rdf:RDF xmlns="http://dati.gov.it/onto/accoapit#"
     xml:base="http://dati.gov.it/onto/accoapit"
     xmlns:l0="http://dati.gov.it/onto/l0/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:tiapit="http://dati.gov.it/onto/tiapit/"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:dct="http://purl.org/dc/terms/"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:covapit="http://dati.gov.it/onto/covapit#"
     xmlns:prov="http://www.w3.org/ns/prov#"
     xmlns:poiapit="http://dati.gov.it/onto/poiapit#"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:smapit="http://dati.gov.it/onto/smapit/">
    <owl:Ontology rdf:about="http://dati.gov.it/onto/accoapit">
        <owl:versionIRI rdf:resource="http://dati.gov.it/onto/accoapit/0.1"/>
        <owl:imports rdf:resource="http://dati.gov.it/onto/tiapit/"/>
        <owl:imports rdf:resource="http://dati.gov.it/onto/l0/"/>
        <owl:imports rdf:resource="http://dati.gov.it/onto/smapit/"/>
        <owl:imports rdf:resource="http://dati.gov.it/onto/poiapit#"/>
        <owl:imports rdf:resource="http://dati.gov.it/onto/covapit#"/>
        <dc:creator xml:lang="en">Institute of Cognitive Sciences and Technologies of the Italian Research Council (CNR) - Semantic Technology Laboratory (STLab)</dc:creator>
        <dct:modified>2017-10-23</dct:modified>
        <owl:versionInfo xml:lang="it">Versione 0.1 - 23 Ottobre 2017 - Creazione dell&apos;ontologia.</owl:versionInfo>
        <dc:creator xml:lang="en">Italian Digital Transformation Team</dc:creator>
        <dct:issued>2017-10-23</dct:issued>
        <dc:creator xml:lang="it">Istituto di Scienze e Tecnologie della Cognizione del CNR - Laboratorio di Tecnologie Semantiche (STLab)</dc:creator>
        <rdfs:comment xml:lang="it">Questa è l&apos;ontologia del profilo  italiano sulle Strutture Ricettive (Accomodation Facility - Italian Application Profile - ACCO-AP_IT)</rdfs:comment>
        <owl:versionInfo xml:lang="en">Version 0.1 - 23 October 2017 - Creation of the ontology </owl:versionInfo>
        <dc:creator xml:lang="it">Team per la Trasformazione Digitale</dc:creator>
        <rdfs:comment xml:lang="en">This is the ontology of the Italian application profile for Accomodation Facilities</rdfs:comment>
        <dc:creator xml:lang="en">Agency for Digital Italy - AgID</dc:creator>
        <rdfs:label xml:lang="it">Strutture ricettive</rdfs:label>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/accoapit/"/>
        <dc:creator xml:lang="it">Agenzia per l&apos;Italia Digitale - AgID</dc:creator>
        <rdfs:label xml:lang="en">Accommodation Facilities</rdfs:label>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/ns/prov#wasDerivedFrom -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/ns/prov#wasDerivedFrom"/>
    


    <!-- http://www.w3.org/ns/prov#wasInfluencedBy -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/ns/prov#wasInfluencedBy"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://dati.gov.it/onto/accoapit#hasAccommodationClassification -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/accoapit#hasAccommodationClassification">
        <owl:inverseOf rdf:resource="http://dati.gov.it/onto/accoapit#isAccommodationClassificationOf"/>
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/accoapit#Accommodation"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/accoapit#AccommodationStarRating"/>
        <rdfs:comment xml:lang="it">Questa proprietà lega una struttura ricettiva alla sua classificazione rispetto al modello a stelle. Se si vuole indicare che un albergo è a 4 stelle, si può usare questa proprietà.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property links an accommodation facility with exactly 1 accommodation star rating. If you want to represent that a hotel is 4 star superior, you can use this property.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/accoapit"/>
        <rdfs:label xml:lang="it">ha classificazione struttura ricettiva</rdfs:label>
        <rdfs:label xml:lang="en">has accommodation classification</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/accoapit#hasAccommodationTypology -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/accoapit#hasAccommodationTypology">
        <owl:inverseOf rdf:resource="http://dati.gov.it/onto/accoapit#isAccommodationTypologyOf"/>
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/accoapit#Accommodation"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/accoapit#AccommodationTypology"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega una struttura ricettiva con almeno una tipologia (hotel, motel, campeggio, albergo diffuso, ecc.)</rdfs:comment>
        <rdfs:comment xml:lang="en">This property links an accommodation with at least one typology (hotel, motel, camping, etc.).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/accoapit"/>
        <rdfs:label xml:lang="it">ha tipologia struttura ricettiva</rdfs:label>
        <rdfs:label xml:lang="en">has accommodation typology</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/accoapit#hasAssociatedOffer -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/accoapit#hasAssociatedOffer">
        <owl:inverseOf rdf:resource="http://dati.gov.it/onto/accoapit#isOfferFor"/>
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/accoapit#Offer"/>
        <rdfs:comment xml:lang="it">Questa proprietà lega qualsiasi cosa del dominio modellato con un&apos;offerta.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property links any thing of a the modelled domain with an offer.</rdfs:comment>
        <rdfs:label xml:lang="it">ha offerta</rdfs:label>
        <rdfs:label xml:lang="en">has associated offer</rdfs:label>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <owl:versionInfo xml:lang="it">stable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/accoapit#hasDuration -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/accoapit#hasDuration">
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/accoapit#Validity"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://dati.gov.it/onto/accoapit#QuantitativeValue"/>
                    <rdf:Description rdf:about="http://dati.gov.it/onto/tiapit/TimeInterval"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="it">Questa proprietà lega una validità (per esempio la validità di un&apos;offerta) a un qualsiasi intervallo temporale oppure a un qualsiasi valore quantitativo.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property links the validity with a duration that can be expressed in a time interval or with a general quantitative value.</rdfs:comment>
        <rdfs:label xml:lang="it">ha durata</rdfs:label>
        <rdfs:label xml:lang="en">has duration</rdfs:label>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <owl:versionInfo xml:lang="it">stable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/accoapit#hasMeasurementUnit -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/accoapit#hasMeasurementUnit">
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/accoapit#QuantitativeValue"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/accoapit#MeasurementUnit"/>
        <rdfs:comment xml:lang="it">Questa proprietà lega un valore quantitativo alla relativa unità di misura</rdfs:comment>
        <rdfs:comment xml:lang="en">This property links a quantitative value to its measurement unit</rdfs:comment>
        <rdfs:label xml:lang="it">ha unità di misura</rdfs:label>
        <rdfs:label xml:lang="en">has measurement unit</rdfs:label>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <owl:versionInfo xml:lang="it">stable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/accoapit#hasOSDCriterion -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/accoapit#hasOSDCriterion">
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/accoapit#OfferedServiceDescription"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/accoapit#OSDCriterion"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega la descrizione dei servizi, offerti da una struttura ricettiva o da una camera della struttura ricettiva, ai relativi criteri che possono essere usati per completare la descrizione.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property links the offered service description to its criteria .</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/accoapit"/>
        <rdfs:label xml:lang="it">ha criterio per descrizione servizi offerti</rdfs:label>
        <rdfs:label xml:lang="en">has offered service description criterion</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/accoapit#hasOSDFeature -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/accoapit#hasOSDFeature">
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/accoapit#OfferedServiceDescription"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/accoapit#OSDFeature"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega la descrizione dei servizi offerti alla caratteristica descritta. Esempi di caratteristica sono: lo spazio occupato per una stanza, camera da letto per la struttura ricettiva, unità abitative, posto letto, ecc.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property links the description of the offered services to its feature (or attribute or characteristic).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/accoapit"/>
        <rdfs:label xml:lang="it">ha caratteristica  descrizione servizi offerti</rdfs:label>
        <rdfs:label xml:lang="en">has offered service description feature</rdfs:label>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <owl:versionInfo xml:lang="it">stable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/accoapit#hasOfferedServiceDescription -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/accoapit#hasOfferedServiceDescription">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://dati.gov.it/onto/accoapit#Accommodation"/>
                    <rdf:Description rdf:about="http://dati.gov.it/onto/accoapit#AccommodationRoom"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/accoapit#OfferedServiceDescription"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega una struttura ricettiva ( o una camera della struttura ricettiva) alla descrizione di tutti i possibili servizi offerti dalla struttura (camera) stessa.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property links an accommodation facility (or an accommodation room)  to the description of the offered services.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/accoapit"/>
        <rdfs:label xml:lang="it">ha descrizione servizi offerti</rdfs:label>
        <rdfs:label xml:lang="en">has offered service description</rdfs:label>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <owl:versionInfo xml:lang="it">stable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/accoapit#hasOwner -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/accoapit#hasOwner">
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/accoapit#Accommodation"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/covapit#Organization"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega una struttura ricettiva a un&apos;organizzazione esprimento lo specifico ruolo di proprietario. Questa proprietà può essere utilizzata per mappare il &quot;codice esercente&quot; alla struttura ricettiva.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property links an accommodation facility to an organization expressing the specific role of owner</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/accoapit"/>
        <rdfs:label xml:lang="it">ha proprietario</rdfs:label>
        <rdfs:label xml:lang="en">has owner</rdfs:label>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <owl:versionInfo xml:lang="it">stable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/accoapit#hasPrice -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/accoapit#hasPrice">
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/accoapit#Offer"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/accoapit#Price"/>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/accoapit#hasRoom -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/accoapit#hasRoom">
        <owl:inverseOf rdf:resource="http://dati.gov.it/onto/accoapit#isRoomOf"/>
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/accoapit#Accommodation"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/accoapit#AccommodationRoom"/>
        <rdfs:comment xml:lang="it">Questa proprietà è l&apos;implementazione dell&apos;ontology design pattern &quot;ParteDi&quot;. Essa collega la struttura ricettiva con le sue camere che sono appunto parte della struttura stessa.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property is the implementation of the ontology design pattern &quot;PartOf&quot;. It links the accommodation facility to its rooms that are part of the facility.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/accoapit"/>
        <rdfs:label xml:lang="it">ha camera</rdfs:label>
        <rdfs:label xml:lang="en">has room</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/accoapit#hasValidity -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/accoapit#hasValidity">
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/accoapit#Offer"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/accoapit#Validity"/>
        <rdfs:comment xml:lang="it">Questa proprietà consente di specificare la validità per un&apos;offerta.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property binds the offer to its validity</rdfs:comment>
        <rdfs:label xml:lang="it">ha validità</rdfs:label>
        <rdfs:label xml:lang="en">has validity</rdfs:label>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <owl:versionInfo xml:lang="it">stable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/accoapit#includesAccommodation -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/accoapit#includesAccommodation">
        <rdfs:subPropertyOf rdf:resource="http://dati.gov.it/onto/poiapit#includesPOI"/>
        <owl:inverseOf rdf:resource="http://dati.gov.it/onto/accoapit#isIncludedInAccommodation"/>
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/accoapit#AccommodationChain"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/accoapit#Accommodation"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega una catena di strutture ricettive con tutte le strutture ricettive della catena</rdfs:comment>
        <rdfs:comment xml:lang="en">This property connects an accommodation chain with all the accommodation facility that are part of the accommodation chain</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/accoapit"/>
        <rdfs:label xml:lang="it">include struttura ricettiva</rdfs:label>
        <rdfs:label xml:lang="en">includes accommodation</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/accoapit#isAccommodationClassificationOf -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/accoapit#isAccommodationClassificationOf">
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/accoapit#AccommodationStarRating"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/accoapit#Accommodation"/>
        <rdfs:comment xml:lang="en">Inverse property of &quot;has accommodation classification&quot; that links the star rating to the accommodation facility</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà inversa di &quot;ha classificazione struttura ricettiva&quot; e collega il rating a stelle alla struttura ricettiva</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/accoapit"/>
        <rdfs:label xml:lang="en">is accommodation classification of</rdfs:label>
        <rdfs:label xml:lang="it">è classificazione di struttura ricettiva</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/accoapit#isAccommodationTypologyOf -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/accoapit#isAccommodationTypologyOf">
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/accoapit#AccommodationTypology"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/accoapit#Accommodation"/>
        <rdfs:comment xml:lang="en">Inverse property of &quot;has accommodation typology&quot; and links the typology with the accommodation facility</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà inversa di &quot;ha tipologia struttura ricettiva&quot; usata per collegare la tipologia alla struttura ricettiva</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/accoapit"/>
        <rdfs:label xml:lang="en">is accommodation typology of</rdfs:label>
        <rdfs:label xml:lang="it">è tipologia di struttura ricettiva</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/accoapit#isIncludedInAccommodation -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/accoapit#isIncludedInAccommodation">
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/accoapit#Accommodation"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/accoapit#AccommodationChain"/>
        <rdfs:comment xml:lang="en">Inverse property of &quot;includes accommodation&quot; used to link a accommodation facility to its accommodatin chain</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà inversa di &quot;include struttura ricettiva&quot; utilizzata per collegare una struttura ricettiva a una catena di strutture ricettive</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/accoapit"/>
        <rdfs:label xml:lang="en">is included in accommodation</rdfs:label>
        <rdfs:label xml:lang="it">è inclusa in struttura ricettiva</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/accoapit#isOfferFor -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/accoapit#isOfferFor">
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/accoapit#Offer"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment xml:lang="en">Inverse property of &quot;has associated offer&quot; that links an offer to anything of the modelled domain</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà inversa di &quot;ha offerta associata&quot; che collega un&apos;offerta a qualunque cosa del dominio modellato</rdfs:comment>
        <rdfs:label xml:lang="en">is offer for</rdfs:label>
        <rdfs:label xml:lang="it">è offerta per</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/accoapit#isRoomOf -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/accoapit#isRoomOf">
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/accoapit#AccommodationRoom"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/accoapit#Accommodation"/>
        <rdfs:comment xml:lang="en">Inverse property of &quot;has room&quot; that links an accommodation room to its facility</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà inversa di &quot;ha camera&quot; che collega una camera (sala) alla propria struttura ricettiva.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/accoapit"/>
        <rdfs:label xml:lang="en">is room of</rdfs:label>
        <rdfs:label xml:lang="it">è camera di</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/poiapit#includesPOI -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/poiapit#includesPOI">
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/accoapit#AccommodationChain"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/accoapit#Accommodation"/>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/poiapit"/>
    </owl:ObjectProperty>
    


    <!-- http://dati.gov.it/onto/smapit/hasOnlineContactPoint -->

    <owl:ObjectProperty rdf:about="http://dati.gov.it/onto/smapit/hasOnlineContactPoint">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/smapit/OnlineContactPoint"/>
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/smapit/"/>
    </owl:ObjectProperty>
    


    <!-- 
