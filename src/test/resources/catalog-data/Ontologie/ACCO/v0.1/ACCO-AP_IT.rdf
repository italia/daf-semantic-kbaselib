            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://dati.gov.it/onto/accoapit#hasOfferedServiceDescription"/>
                <owl:allValuesFrom rdf:resource="http://dati.gov.it/onto/accoapit#OfferedServiceDescription"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://dati.gov.it/onto/accoapit#hasRoom"/>
                <owl:allValuesFrom rdf:resource="http://dati.gov.it/onto/accoapit#AccommodationRoom"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
