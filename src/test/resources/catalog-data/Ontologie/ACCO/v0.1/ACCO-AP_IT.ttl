###  http://dati.gov.it/onto/accoapit#Validity
:Validity rdf:type owl:Class ;
          rdfs:subClassOf l0:Characteristic ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasDuration ;
                            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass [ rdf:type owl:Class ;
                                          owl:unionOf ( :QuantitativeValue
                                                        tiapit:TimeInterval
                                                      )
                                        ]
                          ] ;
          rdfs:comment "Questa classe rappresenta il concetto di validità e può essere associata a un'offerta per modellare la validità dell'offerta stessa in un lasso temporale preciso (dal giorno 30/10/2017 al 4/11/2017) o per un generico ammontare di tempo (e.g., per 3 giorni)"@it ,
                       "This class represents the concept of validity and it can be associated with an offer to model its validity in a time interval or according to a specific quantitative value."@en ;
          rdfs:label "Validity"@en ,
                     "Validità"@it ;
          owl:versionInfo "stable"@en ,
                          "stable"@it .


