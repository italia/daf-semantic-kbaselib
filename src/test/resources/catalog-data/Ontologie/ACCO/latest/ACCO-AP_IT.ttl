###  https://w3id.org/italia/onto/ACCO/hasAccommodationClassification

:hasAccommodationClassification rdf:type owl:ObjectProperty ;
                                
                                rdfs:label "has accommodation classification"@en ,
                                           "ha classificazione struttura ricettiva"@it ;
                                
                                rdfs:comment "This property links an accommodation facility with exactly 1 accommodation star rating. If you want to represent that a hotel is 4 star superior, you can use this property."@en ;
                                
                                owl:versionInfo "stable"@en ;
                                
                                rdfs:comment "Questa proprietà lega una struttura ricettiva alla sua classificazione rispetto al modello a stelle. Se si vuole indicare che un albergo è a 4 stelle, si può usare questa proprietà."@it ;
                                
                                owl:versionInfo "stabile"@it ;
                                
                                rdfs:isDefinedBy <https://w3id.org/italia/onto/ACCO> ;
                                
                                rdfs:domain :Accommodation ;
                                
                                rdfs:range :AccommodationStarRating ;
                                
                                owl:inverseOf :isAccommodationClassificationOf .



###  https://w3id.org/italia/onto/ACCO/hasAccommodationOwner

:hasAccommodationOwner rdf:type owl:ObjectProperty ;
                       
                       rdfs:label "has accommodation owner"@en ,
                                  "ha proprietario struttura ricettiva"@it ;
                       
                       rdfs:comment "This property links an accommodation facility to an organization expressing the specific role of owner"@en ;
                       
                       owl:versionInfo "stable"@en ;
                       
                       rdfs:comment "Questa proprietà collega una struttura ricettiva a un'organizzazione esprimento lo specifico ruolo di proprietario. Questa proprietà può essere utilizzata per mappare il \"codice esercente\" alla struttura ricettiva."@it ;
                       
                       owl:versionInfo "stable"@it ;
                       
                       rdfs:isDefinedBy <https://w3id.org/italia/onto/ACCO> ;
                       
                       rdfs:domain :Accommodation ;
                       
                       rdfs:range covapit:Organization .



###  https://w3id.org/italia/onto/ACCO/hasAccommodationRoom

:hasAccommodationRoom rdf:type owl:ObjectProperty ;
                      
                      rdfs:label "has accommodation room"@en ,
                                 "ha camera della struttura ricettiva"@it ;
                      
                      rdfs:comment "This property is the implementation of the ontology design pattern \"PartOf\". It links the accommodation facility to its rooms that are part of the facility."@en ;
                      
                      owl:versionInfo "stable"@en ;
                      
                      rdfs:comment "Questa proprietà è l'implementazione dell'ontology design pattern \"ParteDi\". Essa collega la struttura ricettiva con le sue camere che sono appunto parte della struttura stessa."@it ;
                      
                      owl:versionInfo "stabile"@it ;
                      
                      rdfs:isDefinedBy <https://w3id.org/italia/onto/ACCO> ;
                      
                      rdfs:domain :Accommodation ;
                      
                      rdfs:range :AccommodationRoom ;
                      
                      owl:inverseOf :isAccommodationRoomOf .



###  https://w3id.org/italia/onto/ACCO/hasAccommodationTypology

:hasAccommodationTypology rdf:type owl:ObjectProperty ;
                          
                          rdfs:label "has accommodation typology"@en ,
                                     "ha tipologia struttura ricettiva"@it ;
                          
                          rdfs:comment "This property links an accommodation with at least one typology (hotel, motel, camping, etc.)."@en ;
                          
                          owl:versionInfo "stable"@en ;
                          
                          rdfs:comment "Questa proprietà collega una struttura ricettiva con almeno una tipologia (hotel, motel, campeggio, albergo diffuso, ecc.)"@it ;
                          
                          owl:versionInfo "stabile"@it ;
                          
                          rdfs:isDefinedBy <https://w3id.org/italia/onto/ACCO> ;
                          
                          rdfs:domain :Accommodation ;
                          
                          rdfs:range :AccommodationTypology ;
                          
                          owl:inverseOf :isAccommodationTypologyOf .



###  https://w3id.org/italia/onto/ACCO/hasOSDCriterion

:hasOSDCriterion rdf:type owl:ObjectProperty ;
                 
                 rdfs:label "has offered service description criterion"@en ,
                            "ha criterio per descrizione servizi offerti"@it ;
                 
                 rdfs:comment "This property links the offered service description to its criteria ."@en ;
                 
                 owl:versionInfo "unstable"@en ;
                 
                 rdfs:comment "Questa proprietà collega la descrizione dei servizi, offerti da una struttura ricettiva o da una camera della struttura ricettiva, ai relativi criteri che possono essere usati per completare la descrizione."@it ;
                 
                 owl:versionInfo "provvisoria"@it ;
                 
                 rdfs:isDefinedBy <https://w3id.org/italia/onto/ACCO> ;
                 
                 rdfs:range :OSDCriterion ;
                 
                 rdfs:domain :OfferedServiceDescription .



###  https://w3id.org/italia/onto/ACCO/hasOSDFeature

:hasOSDFeature rdf:type owl:ObjectProperty ;
               
               rdfs:label "has offered service description feature"@en ,
                          "ha caratteristica  descrizione servizi offerti"@it ;
               
               rdfs:comment "This property links the description of the offered services to its feature (or attribute or characteristic)."@en ;
               
               owl:versionInfo "stable"@en ;
               
               rdfs:comment "Questa proprietà collega la descrizione dei servizi offerti alla caratteristica descritta. Esempi di caratteristica sono: lo spazio occupato per una stanza, camera da letto per la struttura ricettiva, unità abitative, posto letto, ecc."@it ;
               
               owl:versionInfo "stable"@it ;
               
               rdfs:isDefinedBy <https://w3id.org/italia/onto/ACCO> ;
               
               rdfs:range :OSDFeature ;
               
               rdfs:domain :OfferedServiceDescription .



###  https://w3id.org/italia/onto/ACCO/hasOfferedServiceDescription

:hasOfferedServiceDescription rdf:type owl:ObjectProperty ;
                              
                              rdfs:label "has offered service description"@en ,
                                         "ha descrizione servizi offerti"@it ;
                              
                              rdfs:comment "This property links an accommodation facility (or an accommodation room)  to the description of the offered services."@en ;
                              
                              owl:versionInfo "stable"@en ;
                              
                              rdfs:comment "Questa proprietà collega una struttura ricettiva ( o una camera della struttura ricettiva) alla descrizione di tutti i possibili servizi offerti dalla struttura (camera) stessa."@it ;
                              
                              owl:versionInfo "stable"@it ;
                              
                              rdfs:isDefinedBy <https://w3id.org/italia/onto/ACCO> ;
                              
                              rdfs:range :OfferedServiceDescription ;
                              
                              rdfs:domain [ rdf:type owl:Class ;
                                            owl:unionOf ( :Accommodation
                                                          :AccommodationRoom
                                                        )
                                          ] .



###  https://w3id.org/italia/onto/ACCO/includesAccommodation

:includesAccommodation rdf:type owl:ObjectProperty ;
                       
                       rdfs:label "includes accommodation"@en ,
                                  "include struttura ricettiva"@it ;
                       
                       rdfs:comment "This property connects an accommodation chain with all the accommodation facility that are part of the accommodation chain"@en ;
                       
                       owl:versionInfo "stable"@en ;
                       
                       rdfs:comment "Questa proprietà collega una catena di strutture ricettive con tutte le strutture ricettive della catena"@it ;
                       
                       owl:versionInfo "stabile"@it ;
                       
                       rdfs:isDefinedBy <https://w3id.org/italia/onto/ACCO> ;
                       
                       rdfs:range :Accommodation ;
                       
                       rdfs:domain :AccommodationChain ;
                       
                       owl:inverseOf :isIncludedInAccommodation ;
                       
                       rdfs:subPropertyOf poiapit:includesPOI .



###  https://w3id.org/italia/onto/ACCO/isAccommodationClassificationOf

:isAccommodationClassificationOf rdf:type owl:ObjectProperty ;
                                 
                                 rdfs:label "is accommodation classification of"@en ,
                                            "è classificazione di struttura ricettiva"@it ;
                                 
                                 rdfs:comment "Inverse property of \"has accommodation classification\" that links the star rating to the accommodation facility"@en ;
                                 
                                 owl:versionInfo "stable"@en ;
                                 
                                 rdfs:comment "Proprietà inversa di \"ha classificazione struttura ricettiva\" e collega il rating a stelle alla struttura ricettiva"@it ;
                                 
                                 owl:versionInfo "stabile"@it ;
                                 
                                 rdfs:isDefinedBy <https://w3id.org/italia/onto/ACCO> ;
                                 
                                 rdfs:range :Accommodation ;
                                 
                                 rdfs:domain :AccommodationStarRating .



###  https://w3id.org/italia/onto/ACCO/isAccommodationRoomOf

:isAccommodationRoomOf rdf:type owl:ObjectProperty ;
                       
                       rdfs:label "is accommodation room of"@en ,
                                  "è camera della struttura ricettiva"@it ;
                       
                       rdfs:comment "Inverse property of \"has room\" that links an accommodation room to its facility"@en ;
                       
                       owl:versionInfo "stable"@en ;
                       
                       rdfs:comment "Proprietà inversa di \"ha camera\" che collega una camera (sala) alla propria struttura ricettiva."@it ;
                       
                       owl:versionInfo "stabile"@it ;
                       
                       rdfs:isDefinedBy <https://w3id.org/italia/onto/ACCO> ;
                       
                       rdfs:range :Accommodation ;
                       
                       rdfs:domain :AccommodationRoom .



###  https://w3id.org/italia/onto/ACCO/isAccommodationTypologyOf

:isAccommodationTypologyOf rdf:type owl:ObjectProperty ;
                           
                           rdfs:label "is accommodation typology of"@en ,
                                      "è tipologia di struttura ricettiva"@it ;
                           
                           rdfs:comment "Inverse property of \"has accommodation typology\" and links the typology with the accommodation facility"@en ;
                           
                           owl:versionInfo "stable"@en ;
                           
                           rdfs:comment "Proprietà inversa di \"ha tipologia struttura ricettiva\" usata per collegare la tipologia alla struttura ricettiva"@it ;
                           
                           owl:versionInfo "stabile"@it ;
                           
                           rdfs:isDefinedBy <https://w3id.org/italia/onto/ACCO> ;
                           
                           rdfs:range :Accommodation ;
                           
                           rdfs:domain :AccommodationTypology .



###  https://w3id.org/italia/onto/ACCO/isIncludedInAccommodation

:isIncludedInAccommodation rdf:type owl:ObjectProperty ;
                           
                           rdfs:label "is included in accommodation"@en ,
                                      "è inclusa in struttura ricettiva"@it ;
                           
                           rdfs:comment "Inverse property of \"includes accommodation\" used to link a accommodation facility to its accommodatin chain"@en ;
                           
                           owl:versionInfo "stable"@en ;
                           
                           rdfs:comment "Proprietà inversa di \"include struttura ricettiva\" utilizzata per collegare una struttura ricettiva a una catena di strutture ricettive"@it ;
                           
                           owl:versionInfo "stabile"@it ;
                           
                           rdfs:isDefinedBy <https://w3id.org/italia/onto/ACCO> ;
                           
                           rdfs:domain :Accommodation ;
                           
                           rdfs:range :AccommodationChain .



###  https://w3id.org/italia/onto/POI/includesPOI

poiapit:includesPOI rdf:type owl:ObjectProperty ;
                    
                    rdfs:isDefinedBy <http://dati.gov.it/onto/poiapit> .



###  https://w3id.org/italia/onto/POT/hasOffer

potapit:hasOffer rdf:type owl:ObjectProperty ;
                 
                 rdfs:isDefinedBy potapit: .



###  https://w3id.org/italia/onto/SM/hasOnlineContactPoint

smapit:hasOnlineContactPoint rdf:type owl:ObjectProperty ;
                             
                             rdfs:isDefinedBy smapit: .





