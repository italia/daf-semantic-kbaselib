###  https://w3id.org/italia/onto/ACCO/Accommodation

:Accommodation rdf:type owl:Class ;
               
               rdfs:label "Accommodation Facility"@en ,
                          "Struttura ricettiva"@it ;
               
               rdfs:subClassOf poiapit:PointOfInterest ,
                               [ rdf:type owl:Restriction ;
