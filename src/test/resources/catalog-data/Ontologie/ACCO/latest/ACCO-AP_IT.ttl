###  https://w3id.org/italia/onto/ACCO/accoStarRatingID

:accoStarRatingID rdf:type owl:DatatypeProperty ;
                  
                  rdfs:label "accomodation star rating code"@en ,
                             "codice classificazione stelle strutture ricettive"@it ;
                  
                  rdfs:comment "This property represents the identifier used for each element of the star classification of accommodation facilities. It is equivalent to skos:notation and dct:identifier of skos and dublin core ontologies, respectively."@en ;
                  
                  owl:versionInfo "stable"@en ;
                  
                  rdfs:comment "Questa proprietà rappresenta l'identificativo usato per ciascun elemento della classificazione a stelle delle strutture ricettive. E' equivalente a skos:notaiton e dct:identifier di skos e dublin core rispettivamente."@it ;
                  
                  owl:versionInfo "stabile"@it ;
                  
                  l0:controlledVocabulary <http://dati.gov.it/onto/controlledvocabulary/AccoStarRating> ;
                  
                  rdfs:range rdfs:Literal ;
                  
                  rdfs:isDefinedBy <https://w3id.org/italia/onto/ACCO> ;
                  
                  rdfs:domain :AccommodationStarRating ;
                  
                  rdfs:subPropertyOf l0:identifier .



###  https://w3id.org/italia/onto/ACCO/accoStarRatingLabel

:accoStarRatingLabel rdf:type owl:DatatypeProperty ;
                     
                     rdfs:label "accommodation star rating preferred label"@en ,
                                "etichetta classificazione stelle strutture ricettive"@it ;
                     
                     rdfs:comment "This property represents the preferred label of the elements of the accommodation star rating classification. It is equivalent to skos:prefLabel."@en ;
                     
                     owl:versionInfo "stable"@en ;
                     
                     rdfs:comment "Questa proprietà rappresenta la label principale dell'elemento della classificazione delle stelle delle strutture ricettive. E' equivalente a skos:prefLabel dell'ontologia skos."@it ;
                     
                     owl:versionInfo "stabile"@it ;
                     
                     l0:controlledVocabulary <http://dati.gov.it/onto/controlledvocabulary/AccoStarRating> ;
                     
                     rdfs:range rdfs:Literal ;
                     
                     rdfs:isDefinedBy <https://w3id.org/italia/onto/ACCO> ;
                     
                     rdfs:domain :AccommodationStarRating ;
                     
                     rdfs:subPropertyOf l0:name .



###  https://w3id.org/italia/onto/ACCO/accommodationCode

:accommodationCode rdf:type owl:DatatypeProperty ,
                            owl:FunctionalProperty ;
                   
                   rdfs:label "accommodation code"@en ,
                              "codice struttura ricettiva"@it ;
                   
                   rdfs:isDefinedBy "A code associated with the accommodation facility"@en ;
                   
                   owl:priorVersion "stable"@en ;
                   
                   rdfs:isDefinedBy "Un codice associato alla struttura ricettiva."@it ;
                   
                   owl:priorVersion "stabile"@it ;
                   
                   rdfs:range rdfs:Literal ;
                   
                   rdfs:isDefinedBy <https://w3id.org/italia/onto/ACCO> ;
                   
                   rdfs:domain :Accommodation ;
                   
                   rdfs:subPropertyOf l0:identifier .



###  https://w3id.org/italia/onto/ACCO/checkIn

:checkIn rdf:type owl:DatatypeProperty ;
         
         rdfs:label "check in"@en ,
                    "check in"@it ;
         
         rdfs:comment "The checkin time of the accommodation facility"@en ;
         
         owl:priorVersion "stable"@en ;
         
         rdfs:comment "Il checkin previsto dalla struttura ricettiva"@it ;
         
         owl:priorVersion "stabile"@it ;
         
         rdfs:range xsd:dateTime ;
         
         rdfs:isDefinedBy <https://w3id.org/italia/onto/ACCO> ;
         
         rdfs:domain :Accommodation .



###  https://w3id.org/italia/onto/ACCO/checkOut

:checkOut rdf:type owl:DatatypeProperty ;
          
          rdfs:label "check out"@en ,
                     "check out"@it ;
          
          rdfs:comment "The check out time of the accommodation facility"@en ;
          
          owl:priorVersion "stable"@en ;
          
          rdfs:comment "Il check out previsto dalla struttura ricettiva"@it ;
          
          owl:priorVersion "stabile"@it ;
          
          rdfs:range xsd:dateTime ;
          
          rdfs:isDefinedBy <https://w3id.org/italia/onto/ACCO> ;
          
          rdfs:domain :Accommodation .



###  https://w3id.org/italia/onto/ACCO/criterion

:criterion rdf:type owl:DatatypeProperty ;
           
           rdfs:label "criterion"@en ,
                      "criterio"@it ;
           
           rdfs:comment "This property allows one to specify a generic criterion for the class \"Offered Service Description Criterion\". Example: square meters"@en ;
           
           owl:versionInfo "unstable"@en ;
           
           rdfs:comment "Questa proprietà consente di specificare un generico criterio per la classe \"Criterio per Descrizione Servizi Offerti\". Esempio: metri quadrati"@it ;
           
           owl:versionInfo "provvisoria"@it ;
           
           rdfs:range rdfs:Literal ;
           
           rdfs:isDefinedBy <https://w3id.org/italia/onto/ACCO> ;
           
           rdfs:domain :OSDCriterion .



###  https://w3id.org/italia/onto/ACCO/featureDescription

:featureDescription rdf:type owl:DatatypeProperty ;
                    
                    rdfs:label "feature description"@en ,
                               "descrizione caratteristica"@it ;
                    
                    rdfs:comment "This property allows one to specify a textual description of the feature of the offered services description"@en ;
                    
                    owl:versionInfo "stable"@en ;
                    
                    rdfs:comment "Questa proprietà consente di specificare una descrizione complessiva testuale della caratteristica della descrizione dei servizi offerti"@it ;
                    
                    owl:versionInfo "stabile"@it ;
                    
                    rdfs:range rdfs:Literal ;
                    
                    rdfs:isDefinedBy <https://w3id.org/italia/onto/ACCO> ;
                    
                    rdfs:domain :OSDFeature ;
                    
                    rdfs:subPropertyOf l0:description .



###  https://w3id.org/italia/onto/ACCO/featureName

:featureName rdf:type owl:DatatypeProperty ;
             
             rdfs:label "feature name"@en ,
                        "nome caratteristica"@it ;
             
             rdfs:comment "This is the feature name associated with the class Offered Service Description Feature"@en ;
             
             owl:versionInfo "stable"@en ;
             
             rdfs:comment "Questa proprietà rappresenta il nome della caratteristica associata alla classe Caratteristica della Descrizione Servizi Offerti"@it ;
             
             owl:versionInfo "stabile"@it ;
             
             rdfs:range rdfs:Literal ;
             
             rdfs:isDefinedBy <https://w3id.org/italia/onto/ACCO> ;
             
             rdfs:domain :OSDFeature ;
             
             rdfs:subPropertyOf l0:name .



###  https://w3id.org/italia/onto/ACCO/genericValue

:genericValue rdf:type owl:DatatypeProperty ,
                       owl:FunctionalProperty ;
              
              rdfs:label "generic value"@en ,
                         "valore generico"@it ;
              
              rdfs:comment "This property represents a generic value (literal). In the modelled domain of the accommodation facilities it is associated with the description of the offered services."@en ;
              
              owl:versionInfo "unstable"@en ;
              
              rdfs:comment "Questa proprietà rappresenta un valore generico. Nel dominio modellato delle strutture ricettive essa è associata alla descrizione dei serivizi offerti"@it ;
              
              owl:versionInfo "provvisoria"@it ;
              
              rdfs:range rdfs:Literal ;
              
              rdfs:isDefinedBy <https://w3id.org/italia/onto/ACCO> ;
              
              rdfs:domain :OfferedServiceDescription .



###  https://w3id.org/italia/onto/ACCO/maxValue

:maxValue rdf:type owl:DatatypeProperty ,
                   owl:FunctionalProperty ;
          
          rdfs:label "maximum value"@en ,
                     "valore massimo"@it ;
          
          rdfs:comment "This property is used to represent a maximum integer value. In the context of the ontology on accommodation facilities, it is associates with the Offered Service Description class"@en ;
          
          owl:versionInfo "unstable"@en ;
          
          rdfs:comment "Questa proprietà è utilizzata per specificare un valore massimo intero. Nell'ambito dell'ontologia delle strutture ricettive essa è associata alla descrizione dei servizi offerti"@it ;
          
          owl:versionInfo "provvisoria"@it ;
          
          rdfs:range xsd:int ;
          
          rdfs:isDefinedBy <https://w3id.org/italia/onto/ACCO> ;
          
          rdfs:domain :OfferedServiceDescription .



###  https://w3id.org/italia/onto/ACCO/minValue

:minValue rdf:type owl:DatatypeProperty ,
                   owl:FunctionalProperty ;
          
          rdfs:label "minimum value"@en ,
                     "valore minimo"@it ;
          
          rdfs:comment "This property represents a minumun integer value. In the modelled domain it is associated with the description of the offered services."@en ;
          
          owl:versionInfo "unstable"@en ;
          
          rdfs:comment "Questa proprietà rappresenta un valore minimo intero. Nel dominio modellato è associato con la descrizione dei servizi offerti"@it ;
          
          owl:versionInfo "provvisoria"@it ;
          
          rdfs:range xsd:int ;
          
          rdfs:isDefinedBy <https://w3id.org/italia/onto/ACCO> ;
          
          rdfs:domain :OfferedServiceDescription .



###  https://w3id.org/italia/onto/ACCO/roomName

:roomName rdf:type owl:DatatypeProperty ;
          
          rdfs:label "room name"@en ,
                     "nome della camera"@it ;
          
          rdfs:comment "This property represents the name of the room."@en ;
          
          owl:versionInfo "stable"@en ;
          
          rdfs:comment "Questa proprietà rappresenta il nome di una stanza (sala) o camera. Esempio: camera \"arancione\", oppure \"sala A\", ecc."@it ;
          
          owl:versionInfo "stable"@it ;
          
          rdfs:range rdfs:Literal ;
          
          rdfs:comment <https://w3id.org/italia/onto/ACCO> ;
          
          rdfs:domain :AccommodationRoom ;
          
          rdfs:subPropertyOf l0:name .



###  https://w3id.org/italia/onto/ACCO/totalBed

:totalBed rdf:type owl:DatatypeProperty ;
          
