<rdf:RDF xmlns="https://w3id.org/italia/onto/ACCO/"
     xml:base="https://w3id.org/italia/onto/ACCO/"
     xmlns:l0="https://w3id.org/italia/onto/l0/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:tiapit="https://w3id.org/italia/onto/TI/"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:dct="http://purl.org/dc/terms/"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:potapit="https://w3id.org/italia/onto/POT/"
     xmlns:covapit="https://w3id.org/italia/onto/COV/"
     xmlns:muapit="https://w3id.org/italia/onto/MU/"
     xmlns:poiapit="https://w3id.org/italia/onto/POI/"
     xmlns:prov="http://www.w3.org/ns/prov#"
     xmlns:smapit="https://w3id.org/italia/onto/SM/"
     xmlns:dc="http://purl.org/dc/elements/1.1/">
    <owl:Ontology rdf:about="https://w3id.org/italia/onto/ACCO">
        <rdfs:label xml:lang="en">Accommodation Facilities</rdfs:label>
        <rdfs:label xml:lang="it">Strutture ricettive</rdfs:label>
        <dct:modified>2018-13-03</dct:modified>
        <dct:issued>2017-10-23</dct:issued>
        <dc:creator xml:lang="en">Agency for Digital Italy - AgID</dc:creator>
        <dc:creator xml:lang="en">Institute of Cognitive Sciences and Technologies of the Italian Research Council (CNR) - Semantic Technology Laboratory (STLab)</dc:creator>
        <dc:creator xml:lang="en">Italian Digital Transformation Team</dc:creator>
        <rdfs:comment xml:lang="en">This is the ontology of the Italian application profile for Accomodation Facilities</rdfs:comment>
        <owl:versionInfo xml:lang="en">Version 0.3 - 13 March 2018 - Migration to w3id.org/italia as namespace.</owl:versionInfo>
        <dc:creator xml:lang="it">Agenzia per l&apos;Italia Digitale - AgID</dc:creator>
        <dc:creator xml:lang="it">Istituto di Scienze e Tecnologie della Cognizione del CNR - Laboratorio di Tecnologie Semantiche (STLab)</dc:creator>
        <rdfs:comment xml:lang="it">Questa è l&apos;ontologia del profilo  italiano sulle Strutture Ricettive (Accomodation Facility - Italian Application Profile - ACCO-AP_IT)</rdfs:comment>
        <dc:creator xml:lang="it">Team per la Trasformazione Digitale</dc:creator>
        <owl:versionInfo xml:lang="it">Version 0.3 - 13 Marzo 2018 - Migrazione al namespace w3id.org/italia</owl:versionInfo>
        <dct:license rdf:resource="https://creativecommons.org/licenses/by/4.0/"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/ACCO"/>
        <owl:imports rdf:resource="https://w3id.org/italia/onto/COV"/>
        <owl:imports rdf:resource="https://w3id.org/italia/onto/POI"/>
        <owl:imports rdf:resource="https://w3id.org/italia/onto/POT"/>
        <owl:imports rdf:resource="https://w3id.org/italia/onto/SM"/>
        <owl:imports rdf:resource="https://w3id.org/italia/onto/TI"/>
        <owl:imports rdf:resource="https://w3id.org/italia/onto/l0"/>
        <prov:wasDerivedFrom rdf:resource="https://w3id.org/italia/onto/ACCO/0.1"/>
        <owl:versionIRI rdf:resource="https://w3id.org/italia/onto/ACCO/0.3"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/terms/license -->

    <owl:AnnotationProperty rdf:about="&dct;license"/>
    


    <!-- http://www.w3.org/ns/prov#wasDerivedFrom -->

    <owl:AnnotationProperty rdf:about="&prov;wasDerivedFrom"/>
    


    <!-- http://www.w3.org/ns/prov#wasInfluencedBy -->

    <owl:AnnotationProperty rdf:about="&prov;wasInfluencedBy"/>
    


    <!-- https://w3id.org/italia/onto/l0/controlledVocabulary -->

    <owl:AnnotationProperty rdf:about="&l0;controlledVocabulary"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://w3id.org/italia/onto/ACCO/hasAccommodationClassification -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/ACCO/hasAccommodationClassification">
        <rdfs:label xml:lang="en">has accommodation classification</rdfs:label>
        <rdfs:label xml:lang="it">ha classificazione struttura ricettiva</rdfs:label>
        <rdfs:comment xml:lang="en">This property links an accommodation facility with exactly 1 accommodation star rating. If you want to represent that a hotel is 4 star superior, you can use this property.</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Questa proprietà lega una struttura ricettiva alla sua classificazione rispetto al modello a stelle. Se si vuole indicare che un albergo è a 4 stelle, si può usare questa proprietà.</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/ACCO"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/ACCO/Accommodation"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/ACCO/AccommodationStarRating"/>
        <owl:inverseOf rdf:resource="https://w3id.org/italia/onto/ACCO/isAccommodationClassificationOf"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/italia/onto/ACCO/hasAccommodationOwner -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/ACCO/hasAccommodationOwner">
        <rdfs:label xml:lang="en">has accommodation owner</rdfs:label>
        <rdfs:label xml:lang="it">ha proprietario struttura ricettiva</rdfs:label>
        <rdfs:comment xml:lang="en">This property links an accommodation facility to an organization expressing the specific role of owner</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Questa proprietà collega una struttura ricettiva a un&apos;organizzazione esprimento lo specifico ruolo di proprietario. Questa proprietà può essere utilizzata per mappare il &quot;codice esercente&quot; alla struttura ricettiva.</rdfs:comment>
        <owl:versionInfo xml:lang="it">stable</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/ACCO"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/ACCO/Accommodation"/>
        <rdfs:range rdf:resource="&covapit;Organization"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/italia/onto/ACCO/hasAccommodationRoom -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/ACCO/hasAccommodationRoom">
        <rdfs:label xml:lang="en">has accommodation room</rdfs:label>
        <rdfs:label xml:lang="it">ha camera della struttura ricettiva</rdfs:label>
        <rdfs:comment xml:lang="en">This property is the implementation of the ontology design pattern &quot;PartOf&quot;. It links the accommodation facility to its rooms that are part of the facility.</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Questa proprietà è l&apos;implementazione dell&apos;ontology design pattern &quot;ParteDi&quot;. Essa collega la struttura ricettiva con le sue camere che sono appunto parte della struttura stessa.</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/ACCO"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/ACCO/Accommodation"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/ACCO/AccommodationRoom"/>
        <owl:inverseOf rdf:resource="https://w3id.org/italia/onto/ACCO/isAccommodationRoomOf"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/italia/onto/ACCO/hasAccommodationTypology -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/ACCO/hasAccommodationTypology">
        <rdfs:label xml:lang="en">has accommodation typology</rdfs:label>
        <rdfs:label xml:lang="it">ha tipologia struttura ricettiva</rdfs:label>
        <rdfs:comment xml:lang="en">This property links an accommodation with at least one typology (hotel, motel, camping, etc.).</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Questa proprietà collega una struttura ricettiva con almeno una tipologia (hotel, motel, campeggio, albergo diffuso, ecc.)</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/ACCO"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/ACCO/Accommodation"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/ACCO/AccommodationTypology"/>
        <owl:inverseOf rdf:resource="https://w3id.org/italia/onto/ACCO/isAccommodationTypologyOf"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/italia/onto/ACCO/hasOSDCriterion -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/ACCO/hasOSDCriterion">
        <rdfs:label xml:lang="en">has offered service description criterion</rdfs:label>
        <rdfs:label xml:lang="it">ha criterio per descrizione servizi offerti</rdfs:label>
        <rdfs:comment xml:lang="en">This property links the offered service description to its criteria .</rdfs:comment>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Questa proprietà collega la descrizione dei servizi, offerti da una struttura ricettiva o da una camera della struttura ricettiva, ai relativi criteri che possono essere usati per completare la descrizione.</rdfs:comment>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/ACCO"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/ACCO/OSDCriterion"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/ACCO/OfferedServiceDescription"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/italia/onto/ACCO/hasOSDFeature -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/ACCO/hasOSDFeature">
        <rdfs:label xml:lang="en">has offered service description feature</rdfs:label>
        <rdfs:label xml:lang="it">ha caratteristica  descrizione servizi offerti</rdfs:label>
        <rdfs:comment xml:lang="en">This property links the description of the offered services to its feature (or attribute or characteristic).</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Questa proprietà collega la descrizione dei servizi offerti alla caratteristica descritta. Esempi di caratteristica sono: lo spazio occupato per una stanza, camera da letto per la struttura ricettiva, unità abitative, posto letto, ecc.</rdfs:comment>
        <owl:versionInfo xml:lang="it">stable</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/ACCO"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/ACCO/OSDFeature"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/ACCO/OfferedServiceDescription"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/italia/onto/ACCO/hasOfferedServiceDescription -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/ACCO/hasOfferedServiceDescription">
        <rdfs:label xml:lang="en">has offered service description</rdfs:label>
        <rdfs:label xml:lang="it">ha descrizione servizi offerti</rdfs:label>
        <rdfs:comment xml:lang="en">This property links an accommodation facility (or an accommodation room)  to the description of the offered services.</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Questa proprietà collega una struttura ricettiva ( o una camera della struttura ricettiva) alla descrizione di tutti i possibili servizi offerti dalla struttura (camera) stessa.</rdfs:comment>
        <owl:versionInfo xml:lang="it">stable</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/ACCO"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/ACCO/OfferedServiceDescription"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/italia/onto/ACCO/Accommodation"/>
                    <rdf:Description rdf:about="https://w3id.org/italia/onto/ACCO/AccommodationRoom"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/italia/onto/ACCO/includesAccommodation -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/ACCO/includesAccommodation">
        <rdfs:label xml:lang="en">includes accommodation</rdfs:label>
        <rdfs:label xml:lang="it">include struttura ricettiva</rdfs:label>
        <rdfs:comment xml:lang="en">This property connects an accommodation chain with all the accommodation facility that are part of the accommodation chain</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Questa proprietà collega una catena di strutture ricettive con tutte le strutture ricettive della catena</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/ACCO"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/ACCO/Accommodation"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/ACCO/AccommodationChain"/>
        <owl:inverseOf rdf:resource="https://w3id.org/italia/onto/ACCO/isIncludedInAccommodation"/>
        <rdfs:subPropertyOf rdf:resource="&poiapit;includesPOI"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/italia/onto/ACCO/isAccommodationClassificationOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/ACCO/isAccommodationClassificationOf">
        <rdfs:label xml:lang="en">is accommodation classification of</rdfs:label>
        <rdfs:label xml:lang="it">è classificazione di struttura ricettiva</rdfs:label>
        <rdfs:comment xml:lang="en">Inverse property of &quot;has accommodation classification&quot; that links the star rating to the accommodation facility</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Proprietà inversa di &quot;ha classificazione struttura ricettiva&quot; e collega il rating a stelle alla struttura ricettiva</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/ACCO"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/ACCO/Accommodation"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/ACCO/AccommodationStarRating"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/italia/onto/ACCO/isAccommodationRoomOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/ACCO/isAccommodationRoomOf">
        <rdfs:label xml:lang="en">is accommodation room of</rdfs:label>
        <rdfs:label xml:lang="it">è camera della struttura ricettiva</rdfs:label>
        <rdfs:comment xml:lang="en">Inverse property of &quot;has room&quot; that links an accommodation room to its facility</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Proprietà inversa di &quot;ha camera&quot; che collega una camera (sala) alla propria struttura ricettiva.</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/ACCO"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/ACCO/Accommodation"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/ACCO/AccommodationRoom"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/italia/onto/ACCO/isAccommodationTypologyOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/ACCO/isAccommodationTypologyOf">
        <rdfs:label xml:lang="en">is accommodation typology of</rdfs:label>
        <rdfs:label xml:lang="it">è tipologia di struttura ricettiva</rdfs:label>
        <rdfs:comment xml:lang="en">Inverse property of &quot;has accommodation typology&quot; and links the typology with the accommodation facility</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Proprietà inversa di &quot;ha tipologia struttura ricettiva&quot; usata per collegare la tipologia alla struttura ricettiva</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/ACCO"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/ACCO/Accommodation"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/ACCO/AccommodationTypology"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/italia/onto/ACCO/isIncludedInAccommodation -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/ACCO/isIncludedInAccommodation">
        <rdfs:label xml:lang="en">is included in accommodation</rdfs:label>
        <rdfs:label xml:lang="it">è inclusa in struttura ricettiva</rdfs:label>
        <rdfs:comment xml:lang="en">Inverse property of &quot;includes accommodation&quot; used to link a accommodation facility to its accommodatin chain</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Proprietà inversa di &quot;include struttura ricettiva&quot; utilizzata per collegare una struttura ricettiva a una catena di strutture ricettive</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/ACCO"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/ACCO/Accommodation"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/ACCO/AccommodationChain"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/italia/onto/POI/includesPOI -->

    <owl:ObjectProperty rdf:about="&poiapit;includesPOI">
        <rdfs:isDefinedBy rdf:resource="http://dati.gov.it/onto/poiapit"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/italia/onto/POT/hasOffer -->

    <owl:ObjectProperty rdf:about="&potapit;hasOffer">
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/POT/"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/italia/onto/SM/hasOnlineContactPoint -->

    <owl:ObjectProperty rdf:about="&smapit;hasOnlineContactPoint">
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/SM/"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://w3id.org/italia/onto/ACCO/accoStarRatingID -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/italia/onto/ACCO/accoStarRatingID">
        <rdfs:label xml:lang="en">accomodation star rating code</rdfs:label>
        <rdfs:label xml:lang="it">codice classificazione stelle strutture ricettive</rdfs:label>
        <rdfs:comment xml:lang="en">This property represents the identifier used for each element of the star classification of accommodation facilities. It is equivalent to skos:notation and dct:identifier of skos and dublin core ontologies, respectively.</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Questa proprietà rappresenta l&apos;identificativo usato per ciascun elemento della classificazione a stelle delle strutture ricettive. E&apos; equivalente a skos:notaiton e dct:identifier di skos e dublin core rispettivamente.</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <l0:controlledVocabulary rdf:resource="http://dati.gov.it/onto/controlledvocabulary/AccoStarRating"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/ACCO"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/ACCO/AccommodationStarRating"/>
        <rdfs:subPropertyOf rdf:resource="&l0;identifier"/>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/italia/onto/ACCO/accoStarRatingLabel -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/italia/onto/ACCO/accoStarRatingLabel">
        <rdfs:label xml:lang="en">accommodation star rating preferred label</rdfs:label>
        <rdfs:label xml:lang="it">etichetta classificazione stelle strutture ricettive</rdfs:label>
        <rdfs:comment xml:lang="en">This property represents the preferred label of the elements of the accommodation star rating classification. It is equivalent to skos:prefLabel.</rdfs:comment>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Questa proprietà rappresenta la label principale dell&apos;elemento della classificazione delle stelle delle strutture ricettive. E&apos; equivalente a skos:prefLabel dell&apos;ontologia skos.</rdfs:comment>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <l0:controlledVocabulary rdf:resource="http://dati.gov.it/onto/controlledvocabulary/AccoStarRating"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/ACCO"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/ACCO/AccommodationStarRating"/>
        <rdfs:subPropertyOf rdf:resource="&l0;name"/>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/italia/onto/ACCO/accommodationCode -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/italia/onto/ACCO/accommodationCode">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">accommodation code</rdfs:label>
        <rdfs:label xml:lang="it">codice struttura ricettiva</rdfs:label>
        <rdfs:isDefinedBy xml:lang="en">A code associated with the accommodation facility</rdfs:isDefinedBy>
        <owl:priorVersion xml:lang="en">stable</owl:priorVersion>
